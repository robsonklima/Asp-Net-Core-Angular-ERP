  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		Filial.NomeFilial AS Filial, Autorizada.NomeFantasia AS Autorizada, COUNT(vwc_OS.CodOS) AS QtdOS, 
		CAST(YEAR(vwc_OS.DataHoraAberturaOS)AS VARCHAR) AS Ano_Ordem_Servico, 
		CASE WHEN (MONTH(vwc_OS.DataHoraAberturaOS) &lt;10) THEN '0' + CAST(MONTH(vwc_OS.DataHoraAberturaOS)AS VARCHAR) ELSE CAST(MONTH(vwc_OS.DataHoraAberturaOS)AS VARCHAR) END AS Mes_Ordem_Servico 
FROM		dbo.vwc_OrdemServico vwc_OS
LEFT OUTER JOIN	Filial ON vwc_OS.CodFilial = Filial.CodFilial
LEFT OUTER JOIN	Autorizada ON vwc_OS.CodAutorizada = Autorizada.CodAutorizada
WHERE		(1 = 1)
AND		(vwc_OS.CodFilial IN (@CodFilial))
AND		(vwc_OS.CodAutorizada IN (@CodAutorizada))
AND		(vwc_OS.DataHoraAberturaOS &gt;= (@DataInicio + ' 00:00:00.000'))
AND		(vwc_OS.DataHoraAberturaOS &lt;= (@DataFim + ' 23:59:59.999'))
GROUP BY	Filial.NomeFilial, Autorizada.NomeFantasia, CAST(YEAR(vwc_OS.DataHoraAberturaOS)AS VARCHAR), CAST(MONTH(vwc_OS.DataHoraAberturaOS)AS INTEGER)
ORDER BY                Filial.NomeFilial, Autorizada.NomeFantasia, CAST(YEAR(vwc_OS.DataHoraAberturaOS)AS VARCHAR), CAST(MONTH(vwc_OS.DataHoraAberturaOS)AS INTEGER)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CodFilial">
            <Value>=Parameters!CodFilial.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodAutorizada">
            <Value>=Parameters!CodAutorizada.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataInicio">
            <Value>=Parameters!DataInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataFim">
            <Value>=Parameters!DataFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Filial">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Filial</DataField>
        </Field>
        <Field Name="Autorizada">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Autorizada</DataField>
        </Field>
        <Field Name="QtdOS">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>QtdOS</DataField>
        </Field>
        <Field Name="Ano_Ordem_Servico">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Ano_Ordem_Servico</DataField>
        </Field>
        <Field Name="Mes_Ordem_Servico">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Mes_Ordem_Servico</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsFilial">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT CodFilial, NomeFilial 
FROM Filial 
WHERE NomeFilial like 'F%'
ORDER BY NomeFilial</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodFilial">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CodFilial</DataField>
        </Field>
        <Field Name="NomeFilial">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeFilial</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAutorizada">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT CodAutorizada, NomeFantasia + CASE WHEN IndAtivo = 0 THEN ' - Inativo' ELSE '' END AS NomeFantasia FROM Autorizada ORDER BY NomeFantasia</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodAutorizada">
          <DataField>CodAutorizada</DataField>
        </Field>
        <Field Name="NomeFantasia">
          <DataField>NomeFantasia</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsPeriodo">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		CAST('1'
			+'/'+
			'1'
			+'/'+
			CAST(YEAR(DATEADD(DAY,-365,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR) AS DATETIME) 
			AS DataInicial</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="DataInicial">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>DataInicial</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsPeriodoFinal">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		CAST(CAST(MONTH(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR)
			+'/'+
			CAST(DAY(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR)
			+'/'+
			CAST(YEAR(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR) AS DATETIME) AS 
			DataFinal</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="DataFinal">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>DataFinal</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAnoInicioAnoFimRel">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		CAST(
			CAST(YEAR(DATEADD(DAY,-365,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR)
			+'/'+
			CAST(YEAR(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR) AS VARCHAR) AS 
			AnoInicioAnoFim</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AnoInicioAnoFim">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AnoInicioAnoFim</DataField>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
