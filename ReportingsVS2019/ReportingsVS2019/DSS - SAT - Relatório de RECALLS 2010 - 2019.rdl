  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		*
FROM		(
				SELECT			Cliente.NomeFantasia AS Cliente, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS) AS Ano,
								COUNT(DISTINCT OS.CodOS) AS QTDOS
				FROM			(
									SELECT *
									FROM
									(
										SELECT *
										FROM OS
										UNION ALL
										SELECT *
										FROM OS_ARQ_MORTO
										WHERE NOT EXISTS (SELECT 1 FROM OS WHERE OS.CODOS = OS_ARQ_MORTO.CODOS)
									) AS X
								) AS OS
				INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
				WHERE			OS.CodTipoIntervencao = 1
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				OS.DataHoraAberturaOS &gt;= '2010-01-01 00:00:00.000'
				AND				OS.CodCliente &lt;&gt; 88
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				GROUP BY		Cliente.NomeFantasia, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS)
				UNION
				SELECT			Cliente.NomeFantasia AS Cliente, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS) AS Ano,
								COUNT(DISTINCT OS.CodOS) AS QTDOS
				FROM			(
									SELECT *
									FROM
									(
										SELECT *
										FROM OS
										UNION ALL
										SELECT *
										FROM OS_ARQ_MORTO
										WHERE NOT EXISTS (SELECT 1 FROM OS WHERE OS.CODOS = OS_ARQ_MORTO.CODOS)
									) AS X
								) AS OS
				INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
				WHERE			OS.CodTipoIntervencao = 1
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				OS.DataHoraAberturaOS &gt;= '2010-01-01 00:00:00.000'
				AND				OS.CodCliente = 88
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND				OS.NumOSQuarteirizada IS NULL
				GROUP BY		Cliente.NomeFantasia, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS)
				UNION
				SELECT			Cliente.NomeFantasia AS Cliente, 'Num Pedido' NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS) AS Ano,
								COUNT(DISTINCT OS.CodOS) AS QTDOS
				FROM			(
									SELECT *
									FROM
									(
										SELECT *
										FROM OS
										UNION ALL
										SELECT *
										FROM OS_ARQ_MORTO
										WHERE NOT EXISTS (SELECT 1 FROM OS WHERE OS.CODOS = OS_ARQ_MORTO.CODOS)
									) AS X
								) AS OS
				INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
				WHERE			OS.CodTipoIntervencao = 1
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				OS.DataHoraAberturaOS &gt;= '2010-01-01 00:00:00.000'
				AND				OS.CodCliente = 88
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND				(
									OS.NumOSQuarteirizada LIKE '1%'
									OR OS.NumOSQuarteirizada LIKE '2%'
									OR OS.NumOSQuarteirizada LIKE '3%'
									OR OS.NumOSQuarteirizada LIKE '4%'
									OR OS.NumOSQuarteirizada LIKE '5%'
								)
				GROUP BY		Cliente.NomeFantasia, YEAR(OS.DataHoraAberturaOS)
				UNION
				SELECT			Cliente.NomeFantasia AS Cliente, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS) AS Ano,
								COUNT(DISTINCT OS.CodOS) AS QTDOS
				FROM			(
									SELECT *
									FROM
									(
										SELECT *
										FROM OS
										UNION ALL
										SELECT *
										FROM OS_ARQ_MORTO
										WHERE NOT EXISTS (SELECT 1 FROM OS WHERE OS.CODOS = OS_ARQ_MORTO.CODOS)
									) AS X
								) AS OS
				INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
				WHERE			OS.CodTipoIntervencao = 1
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				OS.DataHoraAberturaOS &gt;= '2010-01-01 00:00:00.000'
				AND				OS.CodCliente = 88
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND				OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND				(
									OS.NumOSQuarteirizada NOT LIKE '1%'
									AND OS.NumOSQuarteirizada NOT LIKE '2%'
									AND OS.NumOSQuarteirizada NOT LIKE '3%'
									AND OS.NumOSQuarteirizada NOT LIKE '4%'
									AND OS.NumOSQuarteirizada NOT LIKE '5%'
								)
				GROUP BY		Cliente.NomeFantasia, OS.NumOSQuarteirizada, YEAR(OS.DataHoraAberturaOS)
			) AS DADOS
WHERE		NumOSQuarteirizada IS NOT NULL
ORDER BY	Ano, Cliente, NumOSQuarteirizada
</CommandText>
