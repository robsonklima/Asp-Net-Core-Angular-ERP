  <DataSets>
    <DataSet Name="SAT">
      <Fields>
        <Field Name="NomeTipoEquip">
          <DataField>NomeTipoEquip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomeFilial">
          <DataField>NomeFilial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomeFantasia">
          <DataField>NomeFantasia</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QtdEquiptos">
          <DataField>QtdEquiptos</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		TE.NomeTipoEquip, FI.NomeFilial, AU.NomeFantasia,
		COUNT(VPR.CodEquip) AS QtdEquiptos
FROM		vwc_Parque_Resumo VPR
INNER JOIN	Cliente CL ON VPR.CodCliente = CL.CodCliente
INNER JOIN	GrupoEquipamento GE ON VPR.CodGrupoEquip = GE.CodGrupoEquip
INNER JOIN	TipoEquipamento TE ON VPR.CodTipoEquip = TE.CodTipoEquip
INNER JOIN	Equipamento EQ ON VPR.CodEquip = EQ.CodEquip
		AND TE.CodTipoEquip = EQ.CodTipoEquip
		AND GE.CodGrupoEquip = EQ.CodGrupoEquip
INNER JOIN	Filial FI ON VPR.CodFilial = FI.CodFilial
INNER JOIN	Autorizada AU ON VPR.CodAutorizada = AU.CodAutorizada
WHERE		(1=1)
AND		(VPR.IndAtivo IN (@IndAtivo))
AND		(VPR.IndReceita IN (@IndReceita))
AND		(VPR.AnoMes IN (@AnoMes))
AND		(VPR.CodAutorizada IN (@CodAutorizada))
GROUP BY	TE.NomeTipoEquip, FI.NomeFilial, AU.NomeFantasia
ORDER BY	TE.NomeTipoEquip, FI.NomeFilial, AU.NomeFantasia</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodAutorizada">
            <Value>=Parameters!CodAutorizada.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
