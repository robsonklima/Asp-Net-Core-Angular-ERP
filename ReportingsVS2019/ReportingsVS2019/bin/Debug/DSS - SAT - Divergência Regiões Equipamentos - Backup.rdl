      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			NroContrato Contrato
				, Cliente.RazaoSocial Cliente
				, NomeEquip Equipamento
				, NomeTipoEquip 'Tipo Equipamento'
				, NumSerie 'Série'
				, NomeFilial Filial
				, R1.NomeRegiao AS 'Região Equipamento'
				, r2.NomeRegiao AS 'Região Agência'
				,  NomeLocal
				, (LocalAtendimento.NumAgencia + '/' + LocalAtendimento.DCPosto) AS NumAgencia 	
FROM			EquipamentoContrato 
INNER JOIN		Contrato ON Contrato.CodContrato = EquipamentoContrato.CodContrato
INNER JOIN		Filial ON EquipamentoContrato.CodFilial = Filial.CodFilial
INNER JOIN		TipoEquipamento ON TipoEquipamento.CodTipoEquip = EquipamentoContrato.CodTipoEquip
INNER JOIN		Equipamento ON Equipamento.CodEquip = EquipamentoContrato.CodEquip
INNER JOIN		Cliente ON Cliente.CodCliente = EquipamentoContrato.CodCliente
INNER JOIN		Regiao R1 ON R1.CodRegiao = EquipamentoContrato.CodRegiao
INNER JOIN		LocalAtendimento ON EquipamentoContrato.CodPosto = LocalAtendimento.CodPosto
LEFT JOIN		Regiao R2 ON LocalAtendimento.CodRegiao = R2.CodRegiao

WHERE			(1 = 1)
AND				EquipamentoContrato.CodCliente = 1 
AND				EquipamentoContrato.IndAtivo = 1 
AND				EquipamentoContrato.CodRegiao = 458</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
