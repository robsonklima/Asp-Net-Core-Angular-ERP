  <DataSets>
    <DataSet Name="DsTecnicosIntervencao">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			*
FROM			(
					--Técnicos
					--SELECT			'1' AS Ordem, 'TÉCNICOS ATIVOS' AS Descricao,  CAST(COUNT(*) AS VARCHAR) AS Qtd				
					--FROM			Tecnico
					--INNER JOIN		Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
					--WHERE			Usuario.CodPerfil IN (35,79)
					--AND				Tecnico.CodAutorizada = 8
					--AND				Tecnico.CodTecnico &lt;&gt; 1153
					--AND				Tecnico.IndAtivo = 1
					--AND				Tecnico.indFerias &lt;&gt; 1
					--UNION
					SELECT			Ordem, 'TÉCNICOS ' + Descricao AS Descricao, Qtd
					FROM			FilialTecnicos
					WHERE			CodFilial = 4
					UNION
					SELECT			'3' AS Ordem, ' ' AS Descricao, 9999 AS Qtd
					UNION
					--TOTAIS
					select			'4' AS Ordem, 'CHAMADOS CORRETIVOS - EM TELA' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					WHERE			OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodAutorizada = 8
					AND				OS.CodTipoIntervencao = 2
					UNION
					select			'4' AS Ordem, 'CHAMADOS CORRETIVOS (ABERTO DIA ANTERIOR)' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					WHERE			OS.CodStatusServico &lt;&gt; 2
					AND				OS.CodAutorizada = 8
					AND				OS.CodTipoIntervencao = 2
					AND				OS.DataHoraAberturaOS BETWEEN
									(
										SELECT CASE WHEN DATEPART (DW , GETDATE()) = 2 
														THEN (CAST((CAST(CAST(DATEADD(DD,-3,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))
														ELSE (CAST((CAST(CAST(DATEADD(DD,-1,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))
												END
									)
									AND (SELECT (CAST((CAST(CAST(DATEADD(DD,0,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME)))
					UNION
					select			'4' AS Ordem, 'CHAMADOS OUTROS - EM TELA' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					WHERE			OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodAutorizada = 8
					AND				OS.CodTipoIntervencao &lt;&gt; 2
					AND				OS.CodOS NOT IN (
											select			OS.CodOS			
											from			OS
											WHERE			OS.CodAutorizada = 8
											AND				OS.CodCliente IN (2,88)
											AND				OS.CodTipoIntervencao = 1
									)
					UNION
					select			'4' AS Ordem, 'CHAMADOS TRANSFERIDOS - CORRETIVOS' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					WHERE			OS.CodStatusServico = 8
					AND				OS.CodAutorizada = 8
					AND				OS.CodTipoIntervencao = 2
					UNION
					select			'4' AS Ordem, 'CHAMADOS TRANSFERIDOS - OUTROS' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					WHERE			OS.CodStatusServico = 8
					AND				OS.CodAutorizada = 8
					AND				OS.CodTipoIntervencao &lt;&gt; 2
					AND				OS.CodOS NOT IN (
											select			OS.CodOS			
											from			OS
											WHERE			OS.CodAutorizada = 8
											AND				OS.CodCliente IN (2,88)
											AND				OS.CodTipoIntervencao = 1
									)
					UNION
					--Chamados por Projetos
					select			'6' AS Ordem, 'CHAMADOS PROJETO BANRISUL (ALTERAÇÃO DE ENGENHARIA)' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					inner join		Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE			OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodAutorizada = 8
					AND				OS.CodCliente = 2
					AND				OS.CodTipoIntervencao = 1
					GROUP BY		Cliente.NomeFantasia
					UNION
					select			'6' AS Ordem, 'CHAMADOS PROJETO SICREDI (ALTERAÇÃO DE ENGENHARIA)' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					inner join		Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE			OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodAutorizada = 8
					AND				OS.CodCliente = 88
					AND				OS.CodTipoIntervencao = 1
					GROUP BY		Cliente.NomeFantasia
					UNION
					--Chamados Maquina Nova Banrisul
					select			'6' AS Ordem, 'CHAMADOS MAQUINA NOVA BANRISUL' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			OS
					inner join		Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE			OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodAutorizada = 8
					AND				OS.CodCliente = 2
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodEquip IN (132,231,355,464)
					GROUP BY		Cliente.NomeFantasia
					UNION
					--Chamados Atendidos dia Anterior
					select			'7' AS Ordem, 'CHAMADOS ATENDIDOS (DIA ANTERIOR)' AS Descricao, COUNT(DISTINCT OS.CodOS) AS Qtd 	
					from			RAT
					INNER JOIN		OS ON RAT.CodOS = OS.CodOS
					WHERE			OS.CodStatusServico &lt;&gt; 2
					AND				OS.CodAutorizada = 8
					AND				RAT.DataHoraSolucao BETWEEN
									(
										SELECT CASE WHEN DATEPART (DW , GETDATE()) = 2 
														THEN (CAST((CAST(CAST(DATEADD(DD,-3,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))
														ELSE (CAST((CAST(CAST(DATEADD(DD,-1,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))
												END
									)
									AND (SELECT (CAST((CAST(CAST(DATEADD(DD,0,GETDATE()) AS DATE) AS VARCHAR) + ' 00:00:00.000') AS DATETIME)))
					UNION
					SELECT			'8' AS Ordem, ' ' AS Descricao, 9999 AS Qtd
					UNION
					--Técnicos por Intervenção (Transferidos)
					select			'9' AS Ordem, 'TÉCNICOS C/ ' + TipoIntervencao.NomTipoIntervencao + ' - ' + Cliente.NomeFantasia AS Descricao, COUNT(DISTINCT OS.CodTecnico) AS Qtd 	
					from			OS
					inner join		Tecnico ON OS.CodTecnico = Tecnico.CodTecnico
					inner join		Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
					inner join		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
					inner join		Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE			Usuario.CodPerfil IN (35, 79)
					AND				Tecnico.CodAutorizada = 8
					AND				Tecnico.CodTecnico &lt;&gt; 1153
					AND				OS.CodStatusServico = 8
					AND				OS.CodTipoIntervencao = 1
					AND				OS.CodCliente IN (2,88)
					--AND				OS.CodTipoIntervencao &lt;&gt; 14
					GROUP BY		TipoIntervencao.NomTipoIntervencao, Cliente.NomeFantasia
					UNION
					select			'9' AS Ordem, 'TÉCNICOS C/ ' + TipoIntervencao.NomTipoIntervencao AS Descricao, COUNT(DISTINCT OS.CodTecnico) AS Qtd 	
					from			OS
					inner join		Tecnico ON OS.CodTecnico = Tecnico.CodTecnico
					inner join		Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
					inner join		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
					WHERE			Usuario.CodPerfil IN (35, 79)
					AND				Tecnico.CodAutorizada = 8
					AND				Tecnico.CodTecnico &lt;&gt; 1153
					AND				OS.CodStatusServico = 8
					AND				OS.CodTipoIntervencao IN (2,4)
					--AND				OS.CodTipoIntervencao &lt;&gt; 14
					GROUP BY		TipoIntervencao.NomTipoIntervencao
				) AS DADOS
ORDER BY		1,2</CommandText>
      </Query>
      <Fields>
        <Field Name="Descricao">
          <DataField>Descricao</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ordem">
          <DataField>Ordem</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
