  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		CL.NomeFantasia, CO.NomeContrato, VEQ.AgenciaNumSerie, COUNT(VEQ.CodEquipContrato) AS QtdEquip
FROM		vwc_EquipamentoContrato VEQ
LEFT OUTER JOIN	Cliente CL ON VEQ.CodCliente = CL.CodCliente
LEFT OUTER JOIN	Contrato CO ON VEQ.CodContrato = CO.CodContrato
WHERE		(1 = 1)
AND 		(VEQ.IndAtivo IN (@IndAtivo))
AND 		(VEQ.IndGarantia IN (@IndGarantia))
AND 		(VEQ.IndReceita IN (@IndReceita))
AND 		(VEQ.CodCliente IN (@CodCliente))
AND 		(VEQ.CodContrato IN (@CodContrato))
--AND 		(VEQ.AgenciaNumSerie IN (@AgenciaNumSerie))
GROUP BY	CL.NomeFantasia, CO.NomeContrato, VEQ.AgenciaNumSerie, VEQ.CodEquipContrato
ORDER BY	VEQ.CodEquipContrato</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndGarantia">
            <Value>=Parameters!IndGarantia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodCliente">
            <Value>=Parameters!CodCliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodContrato">
            <Value>=Parameters!CodContrato.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
