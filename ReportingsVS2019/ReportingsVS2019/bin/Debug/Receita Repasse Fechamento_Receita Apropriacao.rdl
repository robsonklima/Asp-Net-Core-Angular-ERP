  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		TE.NomeTipoEquip, CL.NomeFantasia AS Cliente, CO.NomeContrato, EQ.NomeEquip, VPR.ValorReceita AS ReceitaUnit, Fi.NomeFilial,
		COUNT(VPR.CodEquip) AS QtdEquiptos, SUM(VPR.ValorReceita) AS ValorReceita
FROM		vwc_Parque_Resumo VPR
INNER JOIN	Cliente CL ON VPR.CodCliente = CL.CodCliente
INNER JOIN	GrupoEquipamento GE ON VPR.CodGrupoEquip = GE.CodGrupoEquip
INNER JOIN	TipoEquipamento TE ON VPR.CodTipoEquip = TE.CodTipoEquip
INNER JOIN	Equipamento EQ ON VPR.CodEquip = EQ.CodEquip
		AND TE.CodTipoEquip = EQ.CodTipoEquip
		AND GE.CodGrupoEquip = EQ.CodGrupoEquip
INNER JOIN	Filial FI ON VPR.CodFilial = FI.CodFilial
INNER JOIN	Contrato CO ON VPR.CodContrato = CO.CodContrato
WHERE		(1=1)
AND		(VPR.IndAtivo IN (@IndAtivo))
AND		(VPR.IndReceita IN (@IndReceita))
AND		(VPR.IndGarantia IN (@IndGarantia))
AND		(VPR.AnoMes IN (@AnoMes))
GROUP BY	TE.NomeTipoEquip, CL.NomeFantasia, CO.NomeContrato, EQ.NomeEquip, VPR.ValorReceita, Fi.NomeFilial
ORDER BY	TE.NomeTipoEquip, CL.NomeFantasia, CO.NomeContrato, EQ.NomeEquip, Fi.NomeFilial, VPR.ValorReceita</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndGarantia">
            <Value>=Parameters!IndGarantia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
