  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CodFilial">
            <Value>=Parameters!CodFilial.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NomeTecnico AS Tecnico, NomeFilial AS Filial, QtdChamadosAtendidos, QtdChamadosReincidentes, CAST((PERC * 100) AS decimal(10,2)) AS PERC
FROM  
    (  
        SELECT			UPPER(Tecnico.Nome) AS NomeTecnico, 
		                Filial.NomeFilial, 
                        SUM (vwc_Reporting_Indice_de_Reincidencia.ChamadosMes) AS QtdChamadosAtendidos,  
                        SUM (vwc_Reporting_Indice_de_Reincidencia.ChamadosMesReinc) AS QtdChamadosReincidentes, 
                        CAST((
								(
									CAST(SUM(vwc_Reporting_Indice_de_Reincidencia.ChamadosMesReinc)	AS FLOAT)
									/
									CAST(SUM(vwc_Reporting_Indice_de_Reincidencia.ChamadosMes) AS FLOAT)
								)
							) AS FLOAT) AS PERC 
        FROM			vwc_Reporting_Indice_de_ReincidenciaSlideNew AS vwc_Reporting_Indice_de_Reincidencia  
        INNER JOIN		TipoEquipamento ON vwc_Reporting_Indice_de_Reincidencia.CodTipoEquip = TipoEquipamento.CodTipoEquip  
        INNER JOIN		Cliente ON vwc_Reporting_Indice_de_Reincidencia.CodCliente = Cliente.CodCliente  
        INNER JOIN		Filial ON vwc_Reporting_Indice_de_Reincidencia.CodFilial = Filial.CodFilial  
        INNER JOIN		Autorizada ON vwc_Reporting_Indice_de_Reincidencia.CodAutorizada = Autorizada.CodAutorizada  
        INNER JOIN		Tecnico ON vwc_Reporting_Indice_de_Reincidencia.CodTecnico = Tecnico.CodTecnico  
        INNER JOIN		UF ON vwc_Reporting_Indice_de_Reincidencia.CodUF = UF.CodUF 
WHERE Filial.CodFilial = @CodFilial
        GROUP BY		UPPER(Tecnico.Nome), Filial.NomeFilial  
    ) AS Dados 
GROUP BY NomeTecnico, NomeFilial, QtdChamadosAtendidos, QtdChamadosReincidentes, PERC 
--HAVING	QtdChamadosAtendidos &gt; 5 
ORDER BY PERC DESC, NomeTecnico ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Tecnico">
          <DataField>Tecnico</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Filial">
          <DataField>Filial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QtdChamadosAtendidos">
