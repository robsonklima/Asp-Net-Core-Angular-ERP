        <Field Name="NomeTipoEquip">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeTipoEquip</DataField>
        </Field>
        <Field Name="NomeFantasia">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeFantasia</DataField>
        </Field>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
        <Field Name="QtdEquiptos">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>QtdEquiptos</DataField>
        </Field>
        <Field Name="ValorReceita">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ValorReceita</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsTipoModelo">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT CodTipoEquip, NomeTipoEquip 
FROM TipoEquipamento ORDER BY NomeTipoEquip</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodTipoEquip">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CodTipoEquip</DataField>
        </Field>
        <Field Name="NomeTipoEquip">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeTipoEquip</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCliente">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT CodCliente,
CASE WHEN IndAtivo = 0 THEN NomeFantasia + ' - Inativo'
ELSE NomeFantasia END AS NomeFantasia
FROM Cliente
ORDER BY NomeFantasia</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodCliente">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CodCliente</DataField>
        </Field>
        <Field Name="NomeFantasia">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeFantasia</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAnoMes">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT DISTINCT AnoMes FROM Parque_Resumo ORDER BY AnoMes</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAnoMesCorrente">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT TOP 1 AnoMes FROM Parque_Resumo ORDER BY AnoMes DESC</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
