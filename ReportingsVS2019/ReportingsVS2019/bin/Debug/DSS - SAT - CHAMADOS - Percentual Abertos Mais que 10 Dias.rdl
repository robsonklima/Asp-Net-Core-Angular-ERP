  <DataSources>
    <DataSource Name="SAT">
      <DataSourceReference>SAT</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cfd7bdc4-fcdd-4347-b7d8-a67ee5cd9602</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ANO">
            <Value>=Parameters!ANO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Tipo">
            <Value>=Parameters!Tipo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MES">
            <Value>=Parameters!MES.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE			@DataFiltro1 DATETIME
DECLARE			@DataFiltro2 DATETIME
SET				@DataFiltro1 = CAST((@ANO + '-' +  @MES + '-' + '01') AS DATETIME)
SET				@DataFiltro2 = CAST(((CASE @MES WHEN 12 THEN CAST((@ANO + 1) AS VARCHAR) ELSE @ANO END) + '-' + (CASE @MES WHEN 12 THEN '01' ELSE DBO.INSERTZERO((@MES + 1),2) END) + '-' + '01') AS DATETIME)

SELECT			@Tipo AS Tipo,
				Filial,
				CASE @Tipo 
					WHEN 'Corretivas' THEN CorrMes
					WHEN 'Orçamentos' THEN OrcMes
					WHEN 'Ambas' THEN CorrMes + OrcMes
					ELSE 0
				END AS ChamadosMes,
				CASE @Tipo 
					WHEN 'Corretivas' THEN CorrMaior10Dias
					WHEN 'Orçamentos' THEN OrcMaior10Dias
					WHEN 'Ambas' THEN CorrMaior10Dias + OrcMaior10Dias
					ELSE 0
				END AS ChamadosForaDoPrazoMaior10Dias,
				CAST((
						CASE @Tipo 
							WHEN 'Corretivas' THEN CAST(CorrMaior10Dias AS FLOAT) / CASE WHEN CorrMes = 0 THEN 1 ELSE CAST(CorrMes AS FLOAT) END * 100.00
							WHEN 'Orçamentos' THEN CAST(OrcMaior10Dias AS FLOAT) / CASE WHEN OrcMes = 0 THEN 1 ELSE CAST(OrcMes AS FLOAT) END * 100.00
							WHEN 'Ambas' THEN (CAST(CorrMaior10Dias AS FLOAT) + CAST(OrcMaior10Dias AS FLOAT)) 
											  / CASE WHEN OrcMes + CorrMes = 0 THEN 1 ELSE (CAST(CorrMes AS FLOAT) + CAST(OrcMes AS FLOAT)) END * 100
							ELSE 0
						END
					) AS DECIMAL(10,2)
				) AS Percentual
FROM			(
					SELECT	FI.NomeFilial AS Filial,
							(			
								SELECT			COUNT(OS.CodOS) AS CorrMaior10Dias
								FROM			OS
								INNER JOIN		StatusServico ON OS.CodStatusServico = StatusServico.CodStatusServico
								INNER JOIN		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
								INNER JOIN		Autorizada ON OS.CodAutorizada = Autorizada.CodAutorizada
								INNER JOIN		Regiao ON OS.CodRegiao = Regiao.CodRegiao
								INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
								INNER JOIN		LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto
								INNER JOIN		Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
								INNER JOIN		UF ON Cidade.CodUF = UF.CodUF
								LEFT OUTER JOIN	Equipamento ON OS.CodEquip = Equipamento.CodEquip
								LEFT OUTER JOIN	EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
								LEFT OUTER JOIN	SLA ON EquipamentoContrato.CodSLA = SLA.CodSLA
								LEFT OUTER JOIN	Filial ON OS.CodFilial = Filial.CodFilial
								WHERE			(1 = 1)
								AND		        OS.CodStatusServico &lt;&gt; 2
								AND		        OS.CodTipoIntervencao = 2
								AND				OS.CodFilial = FI.CodFilial
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
								AND				OS.DataHoraAberturaOS BETWEEN @DataFiltro1 AND @DataFiltro2
								AND				(
													(
														DATEADD(DD,10, OS.DataHoraAberturaOS) &lt; OS.DataHoraFechamento
														AND 
														OS.CodStatusServico = 3
													) 
													OR 
													(
														OS.DataHoraAberturaOS &lt; DATEADD(DD,-10, GETDATE())
														AND
														OS.CodStatusServico NOT IN (2,3)
													)
												)
							) AS CorrMaior10Dias,
							(
								SELECT			COUNT(OS.CodOS) AS OrcMaior10Dias 
								FROM			OS
								INNER JOIN		StatusServico ON OS.CodStatusServico = StatusServico.CodStatusServico
								INNER JOIN		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
								INNER JOIN		Autorizada ON OS.CodAutorizada = Autorizada.CodAutorizada
								INNER JOIN		Regiao ON OS.CodRegiao = Regiao.CodRegiao
								INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
								INNER JOIN		LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto
								INNER JOIN		Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
								INNER JOIN		UF ON Cidade.CodUF = UF.CodUF
								LEFT OUTER JOIN	Equipamento ON OS.CodEquip = Equipamento.CodEquip
								LEFT OUTER JOIN	EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
								LEFT OUTER JOIN	SLA ON EquipamentoContrato.CodSLA = SLA.CodSLA
								LEFT OUTER JOIN	Filial ON OS.CodFilial = Filial.CodFilial
								WHERE			(1 = 1)
								AND		        OS.CodStatusServico &lt;&gt; 2
								AND		        OS.CodTipoIntervencao IN (5, 17, 18, 19, 20)
								AND				OS.CodFilial = FI.CodFilial
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
								AND				OS.DataHoraAberturaOS BETWEEN @DataFiltro1 AND @DataFiltro2
								AND				(
													(
														DATEADD(DD,10, OS.DataHoraAberturaOS) &lt; OS.DataHoraFechamento
														AND 
														OS.CodStatusServico = 3
													) 
													OR 
													(
														OS.DataHoraAberturaOS &lt; DATEADD(DD,-10, GETDATE())
														AND
														OS.CodStatusServico NOT IN (2,3,16)
													)
												)
							) AS OrcMaior10Dias,
							(
								SELECT			COUNT(OS.CodOS) AS CorrMes
								FROM			OS
								INNER JOIN		StatusServico ON OS.CodStatusServico = StatusServico.CodStatusServico
								INNER JOIN		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
								INNER JOIN		Autorizada ON OS.CodAutorizada = Autorizada.CodAutorizada
								INNER JOIN		Regiao ON OS.CodRegiao = Regiao.CodRegiao
								INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
								INNER JOIN		LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto
								INNER JOIN		Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
								INNER JOIN		UF ON Cidade.CodUF = UF.CodUF
								LEFT OUTER JOIN	Equipamento ON OS.CodEquip = Equipamento.CodEquip
								LEFT OUTER JOIN	EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
								LEFT OUTER JOIN	SLA ON EquipamentoContrato.CodSLA = SLA.CodSLA
								LEFT OUTER JOIN	Filial ON OS.CodFilial = Filial.CodFilial
								WHERE			(1 = 1)
								AND		        OS.CodStatusServico &lt;&gt; 2
								AND		        OS.CodTipoIntervencao = 2
								AND				OS.CodFilial = FI.CodFilial
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
								AND				OS.DataHoraAberturaOS BETWEEN @DataFiltro1 AND @DataFiltro2
							)  AS CorrMes,
							(
								SELECT			COUNT(OS.CodOS) AS OrcMes 
								FROM			OS
								INNER JOIN		StatusServico ON OS.CodStatusServico = StatusServico.CodStatusServico
								INNER JOIN		TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
								INNER JOIN		Autorizada ON OS.CodAutorizada = Autorizada.CodAutorizada
								INNER JOIN		Regiao ON OS.CodRegiao = Regiao.CodRegiao
								INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
								INNER JOIN		LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto
								INNER JOIN		Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
								INNER JOIN		UF ON Cidade.CodUF = UF.CodUF
								LEFT OUTER JOIN	Equipamento ON OS.CodEquip = Equipamento.CodEquip
								LEFT OUTER JOIN	EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
								LEFT OUTER JOIN	SLA ON EquipamentoContrato.CodSLA = SLA.CodSLA
								LEFT OUTER JOIN	Filial ON OS.CodFilial = Filial.CodFilial
								WHERE			(1 = 1)
								AND		        OS.CodStatusServico &lt;&gt; 2
								AND		        OS.CodTipoIntervencao IN (5, 17, 18, 19, 20)
								AND				OS.CodFilial = FI.CodFilial
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
								AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
								AND				OS.DataHoraAberturaOS BETWEEN @DataFiltro1 AND @DataFiltro2
							) AS OrcMes
					FROM	FILIAL FI
					WHERE	FI.IndAtivo = 1
					AND		FI.CodFiliAL &lt;&gt; 21
				) AS DADOS</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
