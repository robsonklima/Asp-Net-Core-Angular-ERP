  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DtInicio">
            <Value>=Parameters!DtInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtFim">
            <Value>=Parameters!DtFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE             @DataInicio DATE
DECLARE             @DataFim DATE

SET					@DataInicio = @DtInicio
SET					@DataFim = @DtFim

SELECT	 		OS.CodOS AS OS,		
				Filial.NomeFilial AS Filial,
				Cliente.NomeFantasia AS Cliente,
				DATEDIFF(DD,OS.DataHoraAberturaOS, (SELECT TOP 1 DataHoraSolucao FROM RAT WHERE RAT.CodOS = OS.CodOS ORDER BY DataHoraSolucao DESC)) AS DiasAbertos,
				(SELECT COUNT(*) FROM RAT WHERE RAT.CodOS = OS.CodOS) AS QTDAtendimentos,
				ISNULL((
					SELECT		SUM(DespesaValor) AS Valor
								FROM 		(
													SELECT			DespesaItem.DespesaValor
													FROM			DespesaItem
													INNER JOIN		Despesa ON DespesaItem.CodDespesa = Despesa.CodDespesa
													INNER JOIN		RAT ON Despesa.CodRAT = RAT.CodRAT
													INNER JOIN		OS OS2 ON RAT.CodOS = OS2.CodOS
													WHERE			DespesaItem.IndAtivo = 1
													AND				Despesa.IndAtivo = 1
													AND				OS2.CodOS = OS.CodOS
											) AS DADOS
				),0) As CustoOS
FROM			OS
INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial			
WHERE			(1 = 1)
AND				EquipamentoContrato.CodFilial NOT IN (7, 21)
AND				EquipamentoContrato.CodCliente &lt;&gt; 220
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
AND				OS.CodStatusServico NOT IN (2)
AND				OS.DataHoraAberturaOS BETWEEN @DtInicio AND @DtFim
AND				OS.CodTipoIntervencao IN (2,10,17)
AND				(
					SELECT COUNT(*) FROM RAT INNER JOIN RATDetalhes ON RAT.CodRAT = RATDetalhes.CodRAT WHERE RATDetalhes.CodAcao = 19 AND RAT.CodOS = OS.CodOS
				) &gt; 0
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
