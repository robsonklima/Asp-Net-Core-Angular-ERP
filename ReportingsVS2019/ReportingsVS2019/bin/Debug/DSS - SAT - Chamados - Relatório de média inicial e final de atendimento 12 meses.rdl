  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>DECLARE             @TabelaDados AS TABLE(Seq INT, MesAno VARCHAR(7), Data VARCHAR(10), Filial VARCHAR(3), HorarioInicio INT, HorarioFinal INT)
DECLARE             @AddMes INT

SET					@AddMes = 12

WHILE				(@AddMes &gt;= -1)
BEGIN

	INSERT INTO			@TabelaDados
	SELECT				Seq, MesAno, Data, Filial, MIN(HorarioInicio) AS HorarioInicio, MAX(HorarioFinal) AS HorarioFinal
	FROM				(						
							SELECT				(MONTH(RAT.DataHoraInicio) + (YEAR(RAT.DataHoraInicio)*12)) AS Seq,
												FORMAT(RAT.DataHoraInicio,'MM/yyyy', 'pt-BR') AS MesAno,
												FORMAT(RAT.DataHoraInicio,'dd/MM/yyyy', 'pt-BR') AS Data,
												Filial.NomeFilial AS Filial,							
												((DATEPART(hh,RAT.DataHoraInicio) * 60) + DATEPART(MINUTE,RAT.DataHoraInicio)) AS HorarioInicio,
												((DATEPART(hh,RAT.DataHoraSolucao) * 60) + DATEPART(MINUTE,RAT.DataHoraSolucao)) AS HorarioFinal												 
							FROM				RAT
							INNER JOIN			OS ON RAT.CodOS = OS.CodOS
							INNER JOIN			Tecnico ON RAT.CodTecnico = Tecnico.CodTecnico
							INNER JOIN			Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
							INNER JOIN			Filial ON Usuario.CodFilial = Filial.CodFilial
							WHERE				CodPerfil IN (35,84) 
							AND					Usuario.CodTecnico IS NOT NULL 
							AND					Usuario.CodTecnico NOT IN (1763,1382,646)
							AND					Usuario.IndAtivo = 1
							AND					OS.CodTipoIntervencao = 2
							AND					RAT.DataHoraInicio &gt; (SELECT DateAdd(mm, DateDiff(mm,0,GetDate()) - @AddMes, 0))
							AND					RAT.DataHoraInicio &lt; (SELECT DateAdd(mm, DateDiff(mm,0,GetDate()) - (@AddMes - 1), 0))
							AND					DATEPART(DW,RAT.DataHoraInicio) NOT IN (1,7)
							AND					DATEPART(HH,RAT.DataHoraInicio) &gt; 0
						) AS DADOS
	GROUP BY			Seq, MesAno, Data, Filial
	ORDER BY			Data
	
	SET @AddMes = @AddMes - 1

END

SELECT				Seq, MesAno, Filial, DBO.FN_CONVMIN((SUM(HorarioInicio) /COUNT(HorarioInicio))) AS HorarioInicio, 
					DBO.FN_CONVMIN((SUM(HorarioFinal)/COUNT(HorarioFinal))) AS HorarioFinal 	
FROM				@TabelaDados
GROUP BY			Seq, MesAno, Filial
UNION
SELECT				99999 AS Seq, FORMAT(GETDATE(),'MM/yyyy', 'pt-BR') AS MesAno, 'MÉDIA ACUMULADA' AS Filial, 
					DBO.FN_CONVMIN((SUM(HorarioInicio) /COUNT(HorarioInicio))) AS HorarioInicio, 
					DBO.FN_CONVMIN((SUM(HorarioFinal)/COUNT(HorarioFinal))) AS HorarioFinal 	
FROM				@TabelaDados
</CommandText>
