  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>DECLARE			@DataFinal DATETIME
SET				@DataFinal = (SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST(MONTH(GETDATE()) AS VARCHAR) + '-' + CAST(DAY(GETDATE()) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))

EXEC			pr_PercentualSLA_CEF_Contrato
				@DataFinal,
				@CodContrato

EXEC			pr_PercentualSLA_CEF_Contrato_Total
				@DataFinal,
				@CodContrato

SELECT			NomeContrato, SiglaUF, CONVERT(VARCHAR, DataPercentual, 103) AS DataPercentual, Percentual
FROM			TempSLACEFContrato
WHERE			DataPercentual  BETWEEN 
				          (	CASE	(SELECT DAY(GETDATE())) 
									WHEN 1 THEN
												(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST((MONTH(GETDATE()) - 1) AS VARCHAR) + '-' + '01' + ' 00:00:00.000') AS DATETIME))
										   ELSE			
												(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST((MONTH(GETDATE())) AS VARCHAR) + '-' + '01' + ' 00:00:00.000') AS DATETIME))
										   END
						  ) 
				          AND  
				          (SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST(MONTH(GETDATE()) AS VARCHAR) + '-' + CAST(DAY(GETDATE()) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))
ORDER BY		3,2,1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CodContrato">
            <Value>=Parameters!CodContrato.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
