  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DataInicial">
            <Value>=Parameters!DataInicial.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataFinal">
            <Value>=Parameters!DataFinal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NomeTecnico">
            <Value>=Parameters!NomeTecnico.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT			CodTecnico, Nome, NomeFilial, NumeroCartao, Placa, FORMAT(DataInicio,'dd/MM/yyyy', 'pt-BR') DataRetirada, 
				DATEDIFF(DAY, DataInicio, getdate()) ContagemDeDataInicio,
				ISNULL(SUM(KmPercorrido), 0) KmPercorrido,
				CAST(PrecoLitro AS MONEY) PrecoLitro,
				CAST(SUM(DespesaCombustivel) AS MONEY) DespesaCombustivel,
				CAST(29.87 * DATEDIFF(DAY, DataInicio, getdate()) AS MONEY) VlrLocaliza
				--ISNULL(Valor, 0) Hodometro, 
				--isnull((ISNULL(SUM(KmPercorrido), 0) - Valor), 0) Excendente,
				--ISNULL(TipoUso, '') TipoUso
FROM
(
	SELECT				RAT.DataHoraInicio RatInicio,
						Tecnico.CodTecnico,
						Tecnico.Nome, 
						Filial.NomeFilial,
						Regiao.NomeRegiao,
						DespesaCartaoCombustivel.Numero NumeroCartao,
						DespesaConfiguracaoCombustivel.PrecoLitro,
						DespesaCartaoCombustivel.Placa,
						DespesaPeriodo.CodDespesaPeriodo,
						DespesaTipo.NomeTipo,
						DespesaItem.KmPercorrido,
						Controle.DataInicio,
						CASE WHEN (NomeTipo = 'Quilometragem') THEN DespesaItem.DespesaValor ELSE 0.00 END DespesaCombustivel
						--Hodometro.Valor,
						--Hodometro.TipoUso
	FROM				Tecnico
	INNER JOIN			DespesaCartaoCombustivelControle Controle ON Controle.Tecnico = Tecnico.Nome
	INNER JOIN			Filial ON Tecnico.CodFilial = Filial.CodFilial
	INNER JOIN			Regiao ON Tecnico.CodRegiao = Regiao.CodRegiao
	LEFT JOIN			Despesa ON Despesa.CodTecnico = Tecnico.CodTecnico
	LEFT JOIN			DespesaItem ON Despesa.CodDespesa = DespesaItem.CodDespesa AND DespesaItem.IndAtivo = 1
	LEFT JOIN			Rat ON Despesa.CodRAT = RAT.CodRAT
	LEFT JOIN			DespesaTipo ON DespesaItem.CodDespesaTipo = DespesaTipo.CodDespesaTipo
	LEFT JOIN			DespesaPeriodo ON DespesaPeriodo.CodDespesaPeriodo = Despesa.CodDespesaPeriodo
	LEFT JOIN			DespesaPeriodoTecnico ON DespesaPeriodoTecnico.CodTecnico = Tecnico.CodTecnico 
						AND DespesaPeriodo.CodDespesaPeriodo = DespesaPeriodoTecnico.CodDespesaPeriodo
	INNER JOIN			DespesaConfiguracaoCombustivel ON DespesaConfiguracaoCombustivel.CodFilial = Filial.CodFilial
						AND DespesaConfiguracaoCombustivel.CodUf = (SELECT CodUF from Cidade where CodCidade = Filial.CodCidade)
	INNER JOIN			DespesaCartaoCombustivel ON DespesaCartaoCombustivel.CodDespesaCartaoCombustivel = Tecnico.CodDespesaCartaoCombustivel
	--LEFT JOIN			Hodometro ON DespesaCartaoCombustivel.Placa = Hodometro.Placa
	WHERE				(1=1)
	AND					DespesaPeriodo.DataInicio &gt;= (select TOP 1 DataHoraInicio from DespesaCartaoCombustivelTecnico dt WHERE Despesa.CodTecnico = dt.CodTecnico ORDER BY DataHoraInicio ASC)
	AND					RAT.DataHoraInicio &gt;=  CAST(@DataInicial AS DATE)
	AND					RAT.DataHoraInicio &lt;= CAST(@DataFinal AS DATE)
	AND					Tecnico.Nome LIKE '%'+@NomeTecnico+'%'
) AS Dados
GROUP BY		CodTecnico, Nome, NomeFilial, NumeroCartao, Placa, FORMAT(DataInicio,'dd/MM/yyyy', 'pt-BR'), 
				DATEDIFF(DAY, DataInicio, getdate()), PrecoLitro, 29.87 * DATEDIFF(DAY, DataInicio, getdate())
ORDER BY		Nome</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
