  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="FILIAL">
          <DataField>FILIAL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MEDIA">
          <DataField>MEDIA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			FILIAL, DBO.FN_CONVMIN((SUM(Minutos) / COUNT(Minutos))) AS MEDIA
FROM			(
					SELECT				Filial.NomeFilial AS Filial, CAST(RAT.DataHoraInicio AS TIME) AS Hora, 
										((DATEPART(HH,RAT.DataHoraInicio) * 60) + DATEPART(MI,RAT.DataHoraInicio)) AS Minutos
					FROM				RAT
					INNER JOIN			OS ON RAT.CodOS = OS.CodOS
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					INNER JOIN			LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto
					INNER JOIN			Filial ON OS.CodFilial = Filial.CodFilial
					WHERE				(1 = 1)
					AND					(
											Cliente.CodCliente IN (286,333)
											OR
											(Cliente.CodCliente = 137 AND LocalAtendimento.DCPosto = 'SH')
										)
					AND					Filial.CodFilial NOT IN (7,21)
				) AS Dados
GROUP BY		Filial</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
