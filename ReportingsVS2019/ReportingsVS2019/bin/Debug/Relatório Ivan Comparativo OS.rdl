  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			CAST(MONTH(OS.DataHoraAberturaOS) AS VARCHAR) + '/' + CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) AS MesAno,
				DAY(OS.DataHoraAberturaOS) AS Dia,
				COUNT(Distinct OS.CodOS) AS Qtd
FROM			OS
INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
WHERE			OS.CodStatusServico &lt;&gt; 2
AND				OS.CodTipoIntervencao = 2
AND				OS.CodFilial NOT IN (7, 21)
AND				OS.CodEquip NOT IN
				(
						SELECT		CodEquip 
						FROM		Equipamento 
						WHERE		NomeEquip LIKE 'POS%'
						OR			NomeEquip LIKE 'PERTOS%'
				)
AND				Cliente.IndAtivo = 1
AND				MONTH(OS.DataHoraAberturaOS) IN (2,3)
AND				YEAR(OS.DataHoraAberturaOS) IN (2018, 2019, 2020)
GROUP BY		CAST(MONTH(OS.DataHoraAberturaOS) AS VARCHAR) + '/' + CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR), 
				DAY(OS.DataHoraAberturaOS)
ORDER BY		DAY(OS.DataHoraAberturaOS), 
				CAST(MONTH(OS.DataHoraAberturaOS) AS VARCHAR) + '/' + CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="MesAno">
          <DataField>MesAno</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Dia">
          <DataField>Dia</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Ds2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Filial.NomeFilial AS Filial, 
				COUNT(Distinct OS.CodOS) AS Qtd,
				(
					SELECT			COUNT(Distinct OS.CodOS) AS Qtd
					FROM			OS
					INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
					INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
					INNER JOIN		Filial F ON OS.CodFilial = F.CodFilial
					WHERE			(1 = 1)
					AND				OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodFilial NOT IN (7, 21)
					AND				OS.CodEquip NOT IN
									(
											SELECT		CodEquip 
											FROM		Equipamento 
											WHERE		NomeEquip LIKE 'POS%'
											OR			NomeEquip LIKE 'PERTOS%'
									)
					AND				Cliente.IndAtivo = 1
					AND				F.CodFilial = Filial.CodFilial
				) Backlog
FROM			OS
INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
WHERE			(1 = 1)
AND				OS.CodStatusServico &lt;&gt; 2
AND				OS.CodTipoIntervencao = 2
AND				OS.CodFilial NOT IN (7, 21)
AND				OS.CodEquip NOT IN
				(
						SELECT		CodEquip 
						FROM		Equipamento 
						WHERE		NomeEquip LIKE 'POS%'
						OR			NomeEquip LIKE 'PERTOS%'
				)
AND				Cliente.IndAtivo = 1
AND				OS.DataHoraAberturaOS &gt;= CAST(CAST(GETDATE()-1 AS DATE) AS DATETIME) 
AND				OS.DataHoraAberturaOS &lt;= CAST(CAST(GETDATE() AS DATE) AS DATETIME) 
GROUP BY		Filial.NomeFilial, Filial.CodFilial
ORDER BY		Filial.NomeFilial, Filial.CodFilial</CommandText>
      </Query>
      <Fields>
        <Field Name="Filial">
          <DataField>Filial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Backlog">
          <DataField>Backlog</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
