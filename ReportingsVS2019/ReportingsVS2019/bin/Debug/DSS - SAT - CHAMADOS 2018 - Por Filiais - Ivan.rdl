				AND				RAT.DataHoraSolucao &lt; '2021-10-01 00:00:00.000'
				AND				OS.CodTipoIntervencao = 2
				AND				OS.CodFilial NOT IN (7, 21,33)
				AND				(OS.CodCliente = 1)  
				AND				(OS.NumOSCliente IS NOT NULL)  
				AND				(EquipamentoContrato.IndGarantia = 0)  
				AND				(EquipamentoContrato.CodContrato = 3145) 
				AND				(OS.IndServico = 1)
