      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE			@T1 TABLE (Criticidade INT, Regiao varchar(20), DispPercEquip FLOAT, Qtd INT, DispPercTotal FLOAT)

INSERT INTO		@T1
SELECT			
				Criticidade
				, Regiao
				, SUM(DispPerc) DispPercEquip
				, count(CodEquipContrato) Qtd
				, SUM(DispPerc) / count(CodEquipContrato) DispPercTotal
FROM			DispBBCalcEquipamentoContrato de
WHERE			(1=1)
AND				AnoMes = @Ano + @Mes
AND				IndispMin &gt; 0
GROUP BY		Regiao, Criticidade
ORDER BY		Criticidade ASC

DECLARE			@T2 TABLE (Criticidade INT, Regiao varchar(20), DispPercEquip FLOAT, Qtd INT, DispPercTotal FLOAT)

INSERT INTO		@T2
SELECT			11 Criticidade, 'Sudeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal 
UNION SELECT	11 Criticidade, 'Nordeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	11 Criticidade, 'Norte' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	11 Criticidade, 'Sul' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	11 Criticidade, 'Centro-Oeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	12 Criticidade, 'Sudeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal 
UNION SELECT	12 Criticidade, 'Nordeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	12 Criticidade, 'Norte' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	12 Criticidade, 'Sul' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	12 Criticidade, 'Centro-Oeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	13 Criticidade, 'Sudeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal 
UNION SELECT	13 Criticidade, 'Nordeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	13 Criticidade, 'Norte' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	13 Criticidade, 'Sul' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	13 Criticidade, 'Centro-Oeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	14 Criticidade, 'Sudeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal 
UNION SELECT	14 Criticidade, 'Nordeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	14 Criticidade, 'Norte' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	14 Criticidade, 'Sul' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	14 Criticidade, 'Centro-Oeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	15 Criticidade, 'Sudeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal 
UNION SELECT	15 Criticidade, 'Nordeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	15 Criticidade, 'Norte' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	15 Criticidade, 'Sul' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal
UNION SELECT	15 Criticidade, 'Centro-Oeste' Regiao, 0 DispPercEquip, 0 Qtd, 0 DispPercTotal

SELECT		
			Criticidade
			, Regiao
			, sum(DispPercEquip) DispPercEquip
			, sum (Qtd) Qtd
			, CASE WHEN (sum(DispPercTotal) = 0) 
				THEN 100 
				ELSE sum(DispPercTotal) 
			  END DispPercTotal
FROM 
(
	SELECT			* 
	FROM			@T1 T1
	UNION
	SELECT			* 
	FROM			@T2 T2
) Dados
WHERE       Criticidade = 11
GROUP BY Criticidade, Regiao
ORDER BY  Criticidade</CommandText>
