  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT --CAST(MONTH(EquipamentoContrato.DataDesativacao) AS VARCHAR) + '/' + CAST(YEAR(EquipamentoContrato.DataDesativacao) AS VARCHAR) AS MesAno,
	   CLIENTE.NOMEFANTASIA AS CLIENTE, 	   
       EQUIPAMENTO.NOMEEQUIP AS MODELO,
	   COUNT(DISTINCT equipamentocontrato.CodEquipContrato) AS QTD
FROM   uf 
       RIGHT OUTER JOIN contrato 
                        RIGHT OUTER JOIN equipamentocontrato 
                          ON contrato.codcontrato = 
                             equipamentocontrato.codcontrato 
                        LEFT OUTER JOIN tipocontrato 
                          ON contrato.codtipocontrato = 
                             tipocontrato.codtipocontrato 
                        LEFT OUTER JOIN regiao 
                          ON equipamentocontrato.codregiao = regiao.codregiao 
                        LEFT OUTER JOIN regiaoautorizada 
                          ON equipamentocontrato.codregiao = 
                             regiaoautorizada.codregiao 
                             AND equipamentocontrato.codautorizada = 
                                 regiaoautorizada.codautorizada 
                             AND regiaoautorizada.codfilial = 
                                 equipamentocontrato.codfilial 
                        LEFT OUTER JOIN filial 
                                        LEFT OUTER JOIN autorizada 
                                          ON filial.codfilial = 
                                             autorizada.codfilial 
                          ON equipamentocontrato.codautorizada = 
                             autorizada.codautorizada 
                             AND equipamentocontrato.codfilial = 
                                 autorizada.codfilial 
                        LEFT OUTER JOIN equipamento 
                          ON equipamentocontrato.codtipoequip = 
                             equipamento.codtipoequip 
                             AND equipamentocontrato.codgrupoequip = 
                                 equipamento.codgrupoequip 
                             AND equipamentocontrato.codequip = 
                                 equipamento.codequip 
                        LEFT OUTER JOIN grupoequipamento 
                          ON equipamento.codgrupoequip = 
                             grupoequipamento.codgrupoequip 
                        LEFT OUTER JOIN tipoequipamento 
                          ON equipamento.codtipoequip = 
                             tipoequipamento.codtipoequip 
                        LEFT OUTER JOIN cliente 
                          ON equipamentocontrato.codcliente = cliente.codcliente 
                        LEFT OUTER JOIN sla 
                          ON equipamentocontrato.codsla = sla.codsla 
                        LEFT OUTER JOIN cidade 
                                        RIGHT OUTER JOIN localatendimento 
                                          ON cidade.codcidade = 
                                             localatendimento.codcidade 
                          ON equipamentocontrato.codcliente = 
                             localatendimento.codcliente 
                             AND equipamentocontrato.codposto = 
                                 localatendimento.codposto 
				         ON uf.coduf = cidade.coduf 
                        LEFT OUTER JOIN ContratoEquipamento ON equipamentocontrato.CodContrato = ContratoEquipamento.CodContrato 
										and	equipamentocontrato.CodEquip = ContratoEquipamento.CodEquip 
					inner join Parque_Resumo ON equipamentocontrato.CodEquipContrato = Parque_Resumo.CodEquipContrato
WHERE  				EquipamentoContrato.CodFilial NOT IN (7, 21, 33)
AND					EquipamentoContrato.CodCliente NOT IN (220,565)
AND					EquipamentoContrato.IndAtivo = 1
and					Parque_Resumo.Anomes = (select CAST(YEAR(GETDATE()) AS VARCHAR) + CAST((SELECT DBO.INSERTZERO(MONTH(dateadd(month, -1, GETDATE())),2)) AS VARCHAR))
--AND					EquipamentoContrato.DataDesativacao between '2020-01-01 00:00:00.000' and '2021-01-01 00:00:00.000'
AND					EquipamentoContrato.CodAutorizada IN (select CodAutorizada from Autorizada where NomeFantasia like 'P%' AND RazaoSocial like 'PERTO AT %' AND IndAtivo = 1)
AND					EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
AND					EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
AND					EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
group by			CLIENTE.NOMEFANTASIA, EQUIPAMENTO.NOMEEQUIP, 
					CAST(MONTH(EquipamentoContrato.DataDesativacao) AS VARCHAR) + '/' + CAST(YEAR(EquipamentoContrato.DataDesativacao) AS VARCHAR)
order by			CAST(MONTH(EquipamentoContrato.DataDesativacao) AS VARCHAR) + '/' + CAST(YEAR(EquipamentoContrato.DataDesativacao) AS VARCHAR), 
					CLIENTE.NOMEFANTASIA, EQUIPAMENTO.NOMEEQUIP</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CLIENTE">
          <DataField>CLIENTE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MODELO">
          <DataField>MODELO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTD">
          <DataField>QTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
