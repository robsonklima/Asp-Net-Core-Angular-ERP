      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE @Data datetime
             SET @Data = CAST(YEAR(DATEADD(M, -3, GETDATE())) AS VARCHAR) + '-' + CAST(MONTH(DATEADD(M, -3, GETDATE())) AS VARCHAR) + '-01 00:00:00'  

SELECT CodOR, dados.mes,dados.ano, dados.mesAtual, NomeUsuario 'Técnico Reparador', Peca.CodMagnus,Peca.NomePeca,    dados.DataHoraInicio,dados.DataHoraFim FROM (             SELECT 
             MONTH(DataHoraFim) mes, YEAR(DataHoraFim) ano,
             				CASE WHEN MONTH(DataHoraFim) = 1 THEN 'Janeiro' 
             					 WHEN MONTH(DataHoraFim) = 2 THEN 'Fevereiro' 
             					 WHEN MONTH(DataHoraFim) = 3 THEN 'Março' 
             					 WHEN MONTH(DataHoraFim) = 4 THEN 'Abril' 
             					 WHEN MONTH(DataHoraFim) = 5 THEN 'Maio' 
             					 WHEN MONTH(DataHoraFim) = 6 THEN 'Junho' 
             					 WHEN MONTH(DataHoraFim) = 7 THEN 'Julho'
             					 WHEN MONTH(DataHoraFim) = 8 THEN 'Agosto'
             					 WHEN MONTH(DataHoraFim) = 9 THEN 'Setembro'
             					 WHEN MONTH(DataHoraFim) = 10 THEN 'Outubro'
             					 WHEN MONTH(DataHoraFim) = 11 THEN 'Novembro'
             					 WHEN MONTH(DataHoraFim) = 12 THEN 'Dezembro'
             				ELSE ''
             				END as mesAtual
			 ,* 
             FROM ORTempoReparo 
             where DataHoraFim BETWEEN @Data AND GETDATE() AND CodORItem IN (SELECT CodORItem FROM ORItem WHERE CodstatusOR IN (22, 36))
             ) as dados
			 INNER JOIN Usuario ON CodUsuario = dados.CodTecnico
			 INNER JOIN ORItem ON dados.CodORItem = ORItem.CodORItem
			 LEFT JOIN Peca ON Peca.CodPeca = ORItem.CodPeca
			 where dados.mesAtual IN ('Maio','Abril')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
