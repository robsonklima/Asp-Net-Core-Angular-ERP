  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia, Contrato.NomeContrato, Equipamento.NomeEquip, Parque_Resumo.ValorReceita, Parque_Resumo.AnoMes,
		DATENAME(yy, Contrato.DataFimVigencia) AS AnoVigencia, DATENAME(mm, Contrato.DataFimVigencia) AS MesVigencia,
		Filial.NomeFilial, COUNT(Parque_Resumo.CodEquip) AS QtdEquiptos, Parque_Resumo.ValorReceita * COUNT(Parque_Resumo.CodEquip) AS ReceitaTotal
FROM		Parque_Resumo 
INNER JOIN	EquipamentoContrato ON Parque_Resumo.CodEquipContrato = EquipamentoContrato.CodEquipContrato 
INNER JOIN	Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
LEFT OUTER JOIN	LocalAtendimento ON Parque_Resumo.CodCliente = LocalAtendimento.CodCliente
AND                          Parque_Resumo.CodPosto = LocalAtendimento.CodPosto
LEFT OUTER JOIN	Autorizada ON Parque_Resumo.CodAutorizada = Autorizada.CodAutorizada
LEFT OUTER JOIN	Equipamento ON Parque_Resumo.CodEquip = Equipamento.CodEquip
LEFT OUTER JOIN	TipoEquipamento ON Parque_Resumo.CodTipoEquip = TipoEquipamento.CodTipoEquip
LEFT OUTER JOIN	Cliente ON Parque_Resumo.CodCliente = Cliente.CodCliente
LEFT OUTER JOIN	Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
WHERE		(1=1)
AND		(Parque_Resumo.CodContrato IN (@CodContrato))
AND		(Parque_Resumo.IndAtivo IN (@IndAtivo))
AND		(Parque_Resumo.IndReceita IN (@IndReceita))
AND		(Parque_Resumo.IndGarantia IN (@IndGarantia))
AND		(Parque_Resumo.AnoMes IN (@AnoMes))
AND		(Parque_Resumo.CodEquip IN (@CodEquip))
AND		(Parque_Resumo.CodTipoEquip IN (@CodTipoEquip))
AND		(Parque_Resumo.CodCliente IN (@CodCliente))
AND		(Parque_Resumo.ValorReceita IN (@ValorReceita))
GROUP BY	Cliente.NomeFantasia, TipoEquipamento.NomeTipoEquip, Contrato.DataFimVigencia, Contrato.NomeContrato, Filial.NomeFilial, Equipamento.NomeEquip,
                                Parque_Resumo.ValorReceita, Parque_Resumo.AnoMes
ORDER BY	TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia, Contrato.NomeContrato, Filial.NomeFilial, Equipamento.NomeEquip ASC</CommandText>
