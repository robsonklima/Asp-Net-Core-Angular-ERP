  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT			TipoEquipamento.NomeTipoEquip, GrupoEquipamento.NomeGrupoEquip, 
				Parque_Resumo.AnoMes, COUNT(Parque_Resumo.CodEquip) AS QtdEquiptos
FROM			Parque_Resumo 

LEFT OUTER JOIN TipoEquipamento ON Parque_Resumo.CodTipoEquip = TipoEquipamento.CodTipoEquip
LEFT OUTER JOIN GrupoEquipamento ON Parque_Resumo.CodGrupoEquip = GrupoEquipamento.CodGrupoEquip

WHERE			(1=1)
AND			(Parque_Resumo.AnoMes IN (@AnoMes))
AND			(Parque_Resumo.IndAtivo IN (@IndAtivo))
AND			(Parque_Resumo.IndReceita IN (@IndReceita))
AND			(Parque_Resumo.CodTipoEquip IN (@CodTipoEquip))
AND			(Parque_Resumo.CodGrupoEquip IN (@CodGrupoEquip))

GROUP BY		TipoEquipamento.NomeTipoEquip, GrupoEquipamento.NomeGrupoEquip, Parque_Resumo.AnoMes
ORDER BY		COUNT(Parque_Resumo.CodEquip)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodTipoEquip">
            <Value>=Parameters!CodTipoEquip.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodGrupoEquip">
            <Value>=Parameters!CodGrupoEquip.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
