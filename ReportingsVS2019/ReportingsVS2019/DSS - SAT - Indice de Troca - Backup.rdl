      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT		CAST(CAL.Pai AS varchar) AS CodPai,			
			GrupoEquipamento.NomeGrupoEquip,
			CAST(CAL.MagnusPeca AS varchar) AS CodMagnus,
			Peca.NomePeca,
			Cliente.NomeFantasia,			
			ISNULL((SELECT sum(QTDPeca) FROM Tmp_Calcula_Indice_Quebra_Por_Cliente WHERE qtdPeca &gt; 0 AND magnusPeca = '032.04.052' AND codcliente = CAL.CodCliente AND pai = CAL.Pai),0) qtdTroca,
			
			--CAST(ISNULL(CAST(
			--		(
			--			CASE WHEN	ISNULL(CAST((						
			--									SELECT			SUM(CIQ.QtdPeca)
			--									FROM			Tmp_Calcula_Indice_Quebra_Por_Cliente AS CIQ
			--									WHERE			CIQ.MagnusPeca = CAL.MagnusPeca
			--									AND				CIQ.CodCliente = CAL.CodCliente
			--								) AS FLOAT),0) &gt; 0 
			--						AND
			--						ISNULL(CAST((
			--									SELECT			SUM(CPT.QTD)
			--									FROM			[SAT].[dbo].[Tmp_Calcula_Parque_Total] AS CPT
			--									WHERE			CPT.Pai IN
			--									(
			--										SELECT			DISTINCT Pai
			--										FROM			Tmp_Calcula_Indice_Quebra_Por_Cliente AS CIQ
			--										WHERE			CIQ.MagnusPeca = CAL.MagnusPeca
			--										AND				CIQ.CodCliente = CAL.CodCliente
			--									)
			--								)AS FLOAT),0) &gt; 0
			--					THEN
			--						CAST((						
			--									SELECT			SUM(CIQ.QtdPeca)
			--									FROM			Tmp_Calcula_Indice_Quebra_Por_Cliente AS CIQ
			--									WHERE			CIQ.MagnusPeca = CAL.MagnusPeca
			--									AND				CIQ.CodCliente = CAL.CodCliente
			--						) AS FLOAT)
			--						/
			--						CAST((	
			--									SELECT			SUM(CPT.QTD)
			--									FROM			[SAT].[dbo].[Tmp_Calcula_Parque_Total] AS CPT
			--									WHERE			CPT.Pai IN
			--									(
			--										SELECT			DISTINCT Pai
			--										FROM			Tmp_Calcula_Indice_Quebra_Por_Cliente AS CIQ
			--										WHERE			CIQ.MagnusPeca = CAL.MagnusPeca
			--										AND				CIQ.CodCliente = CAL.CodCliente
			--									)
			--						) AS FLOAT)						
			--						/
			--						ISNULL(CAST((
			--									SELECT			COUNT(DISTINCT CPM.AnoMes)
			--									FROM			[SAT].[dbo].[Tmp_Calcula_Parque_Meses] AS CPM
			--									WHERE			CPM.Pai IN
			--									(
			--										SELECT			DISTINCT Pai
			--										FROM			Tmp_Calcula_Indice_Quebra_Por_Cliente AS CIQ
			--										WHERE			CIQ.MagnusPeca = CAL.MagnusPeca
			--										AND				CIQ.CodCliente = CAL.CodCliente
			--									)
			--						) AS FLOAT),1)	
			--			END
			--		) 
			--		AS DECIMAL(10,8)),0.0003)AS VARCHAR) AS Indice,
			ISNULL(
					(
						SELECT			COUNT(*) AS QTD
						FROM			EquipamentoContrato EC
						INNER JOIN		ContratoEquipamento CE ON EC.CodContrato = CE.CodContrato
										AND EC.CodEquip = CE.CodEquip
						INNER JOIN		Filial ON EC.CodFilial = Filial.CodFilial 
						WHERE			PATINDEX ('%.%',CE.CodMagnus) &gt; 0
						AND				EC.IndAtivo = 1					
						AND				CE.CodMagnus = CAL.Pai			
						AND				EC.CodCliente = CAL.CodCliente							
						GROUP BY		CE.CodMagnus
					)
			,0) AS ParqueEquipamento,
			ISNULL(CAST((CAST((CAST((SELECT sum(QTDPeca) FROM Tmp_Calcula_Indice_Quebra_Por_Cliente WHERE qtdPeca &gt; 0 AND magnusPeca = '032.04.052' AND codcliente = CAL.CodCliente AND pai = CAL.Pai) AS FLOAT) / CAST((ISNULL(
					(
						SELECT			COUNT(*) AS QTD
						FROM			EquipamentoContrato EC
						INNER JOIN		ContratoEquipamento CE ON EC.CodContrato = CE.CodContrato
										AND EC.CodEquip = CE.CodEquip
						INNER JOIN		Filial ON EC.CodFilial = Filial.CodFilial 
						WHERE			PATINDEX ('%.%',CE.CodMagnus) &gt; 0
						AND				EC.IndAtivo = 1					
						AND				CE.CodMagnus = CAL.Pai			
						AND				EC.CodCliente = CAL.CodCliente							
						GROUP BY		CE.CodMagnus
					)
			,1)) AS FLOAT)) AS DECIMAL(10,2)) *100) AS VARCHAR), '0') + '%' IndiceTroca
FROM		Tmp_Calcula_Indice_Quebra_Por_Cliente CAL
INNER JOIN  Filial ON CAL.Filial = Filial.CodFilial
INNER JOIN  Peca ON CAL.MagnusPeca = Peca.CodMagnus
INNER JOIN	Cliente ON Cliente.CodCliente = CAL.CodCliente
LEFT JOIN	ContratoEquipamento ON CAL.Pai = ContratoEquipamento.CodMagnus
LEFT JOIN	Equipamento ON Equipamento.CodEquip = ContratoEquipamento.CodEquip
LEFT JOIN	GrupoEquipamento ON GrupoEquipamento.CodGrupoEquip = ContratoEquipamento.CodGrupoEquip
GROUP BY	CAL.PAI, GrupoEquipamento.NomeGrupoEquip, CAL.MagnusPeca, Peca.NomePeca, CAL.CodCliente, Cliente.NomeFantasia
HAVING		
			--CAL.PAI LIKE '%032.04.052%' AND	
			CAL.MagnusPeca LIKE '%032.04.052%'
			AND CAL.CodCliente &gt; 0
ORDER BY	CAL.PAI, CAL.MagnusPeca</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
