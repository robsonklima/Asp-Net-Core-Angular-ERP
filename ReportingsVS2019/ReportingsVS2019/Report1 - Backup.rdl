  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Filial">
          <DataField>Filial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Dia">
          <DataField>Dia</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTD">
          <DataField>QTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			Filial.Nomefilial AS Filial,
				CAST(dbo.insertZero(DAY(OS.DataHoraAberturaOS),2) AS VARCHAR) AS Dia,
				count(Distinct CodOS) AS QTD
FROM			OS
INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
WHERE			OS.DataHoraAberturaOS between '2019-01-01 00:00:00.000' and '2019-04-24 00:00:00.000'
AND				OS.CodTipoIntervencao = 2
AND				OS.CodFilial NOT IN (7, 21)
AND				EquipamentoContrato.CodFilial NOT IN (7, 21)
AND				EquipamentoContrato.CodCliente &lt;&gt; 220
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
AND				EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
AND				Filial.CodFilial IN (@Filial)
GROUP BY		Filial.Nomefilial, CAST(dbo.insertZero(DAY(OS.DataHoraAberturaOS),2) AS VARCHAR)
ORDER BY		Filial.Nomefilial, CAST(dbo.insertZero(DAY(OS.DataHoraAberturaOS),2) AS VARCHAR)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Filial">
            <Value>=Parameters!Filial.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
