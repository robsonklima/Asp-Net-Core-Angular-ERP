  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CodTecnico">
            <Value>=Parameters!CodTecnico.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select		FORMAT(DespesaAdiantamento.DataAdiantamento,'dd/MM/yyyy', 'pt-BR') AS DataAdiantamento, DespesaAdiantamento.ValorAdiantamento 
from		DespesaAdiantamento
where		codtecnico = @CodTecnico</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DataAdiantamento">
          <DataField>DataAdiantamento</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ValorAdiantamento">
          <DataField>ValorAdiantamento</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CodTecnico">
            <Value>=Parameters!CodTecnico.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT		FORMAT(DespesaPeriodo.DataInicio,'dd/MM/yyyy', 'pt-BR') AS DataInicio, FORMAT(DespesaPeriodo.DataFim,'dd/MM/yyyy', 'pt-BR') AS DataFim, 
			ValorAdiantamentoUtilizado
FROM		DespesaAdiantamentoPeriodo
INNER JOIN	DespesaPeriodo ON DespesaAdiantamentoPeriodo.CodDespesaPeriodo = DespesaPeriodo.CodDespesaPeriodo
WHERE		CodDespesaAdiantamento	in (
											select		CodDespesaAdiantamento 
											from		DespesaAdiantamento
											where		codtecnico = @CodTecnico 
											)</CommandText>
      </Query>
      <Fields>
        <Field Name="DataInicio">
          <DataField>DataInicio</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DataFim">
          <DataField>DataFim</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ValorAdiantamentoUtilizado">
          <DataField>ValorAdiantamentoUtilizado</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DsFilial">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		NomeFilial, CodFilial 
FROM		Filial
WHERE		IndAtivo = 1
AND		CodFilial NOT IN (7,21,33)</CommandText>
      </Query>
      <Fields>
        <Field Name="NomeFilial">
          <DataField>NomeFilial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CodFilial">
          <DataField>CodFilial</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DsTecnico">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Tecnico.Nome, Tecnico.CodTecnico 
FROM			Tecnico 
INNER JOIN      Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
WHERE			Tecnico.IndAtivo = 1
AND				Tecnico.CodTecnico NOT IN (1616, 1326, 1153)
AND				Usuario.CodPerfil = 35
AND				Usuario.IndAtivo = 1
ORDER BY		Tecnico.Nome</CommandText>
      </Query>
      <Fields>
        <Field Name="Nome">
          <DataField>Nome</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CodTecnico">
          <DataField>CodTecnico</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
