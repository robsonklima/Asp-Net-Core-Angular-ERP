  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>DECLARE			@MES VARCHAR(2)
DECLARE			@ANO VARCHAR(4) 
SET				@MES = DBO.INSERTZERO((@MESAUX),2)
SET				@ANO = @ANOAUX

DECLARE			@DataFinal DATETIME
SET				@DataFinal = (CASE WHEN(CAST(@MES AS INT) = (SELECT MONTH(GETDATE())))
									THEN
										(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST(MONTH(GETDATE()) AS VARCHAR) + '-' + CAST(DAY(GETDATE()) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))

									ELSE
										(CONVERT(DateTime, @ANO + '-' + @MES + '-' + CAST(DAY(DATEADD(d,-1,DATEADD(M,1,CONVERT(DATETIME,@ANO + @MES + '01')))) AS VARCHAR) + ' 00:00:00.000'))
									END
							)

EXEC			pr_PercentualSLA_CEF
				@DataFinal

EXEC			pr_PercentualSLA_CEF_Total
				@DataFinal

SELECT			SiglaUF, CONVERT(VARCHAR, DataPercentual, 103) AS DataPercentual, Percentual
FROM			TempSLACEF
WHERE			DataPercentual  BETWEEN 
				(CASE WHEN (CAST(@MES AS INT) = (SELECT MONTH(GETDATE())))
					THEN
							(CASE	(SELECT DAY(GETDATE())) 
								WHEN 1 THEN
									(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST((MONTH(GETDATE()) - 1) AS VARCHAR) + '-' + '01' + ' 00:00:00.000') AS DATETIME))
								   ELSE			
									(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST((MONTH(GETDATE())) AS VARCHAR) + '-' + '01' + ' 00:00:00.000') AS DATETIME))
								   END
							)
					ELSE
							(CONVERT(DateTime, @ANO + '-' + @MES + '-' + '01' + ' 00:00:00.000'))
					END
				)
				AND 
				(CASE WHEN(CAST(@MES AS INT) = (SELECT MONTH(GETDATE())))
					THEN
						(SELECT CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + '-' + CAST(MONTH(GETDATE()) AS VARCHAR) + '-' + CAST(DAY(GETDATE()) AS VARCHAR) + ' 00:00:00.000') AS DATETIME))

					ELSE
						(CONVERT(DateTime, @ANO + '-' + @MES + '-' + CAST(DAY(DATEADD(d,-1,DATEADD(M,1,CONVERT(DATETIME,@ANO + @MES + '01')))) AS VARCHAR) + ' 00:00:00.000'))
					END
				)
ORDER BY		2,1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@MESAUX">
            <Value>=Parameters!MESAUX.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ANOAUX">
            <Value>=Parameters!ANOAUX.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
