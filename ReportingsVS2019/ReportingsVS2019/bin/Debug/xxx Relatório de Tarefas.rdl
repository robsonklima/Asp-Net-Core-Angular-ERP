  <rd:ReportID>34a2f8d6-3b67-47cc-99c8-a5b95c59ffdb</rd:ReportID>
  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT			Tarefas.CodTarefa, ISNULL(CAST(Tarefas.NumPriorizacao AS Varchar), '-') AS Prioridade, 
				CASE UrgenciaUsuario 
					WHEN 1 THEN 'BAIXA'
					WHEN 2 THEN 'MÉDIA'
					WHEN 3 THEN 'ALTA'
					ELSE 'INDEFINIDA'
				END AS UrgenciaUsuario,
				TarefasStatus.NomeTarefaStatus AS _Status, Tarefas.Modulo, TarefasTipos.NomeTarefaTipo AS Tipo, 
				Tarefas.TituloTarefa AS Titulo, Usuario.NomeUsuario AS Solicitante,
				CONVERT(CHAR,Tarefas.DataHoraCadastro,103)AS DataCadastro 
FROM			Tarefas
LEFT OUTER JOIN	TarefasStatus ON Tarefas.codTarefaStatus = TarefasStatus.codTarefaStatus
LEFT OUTER JOIN	TarefasTipos ON Tarefas.codTarefaTipo = TarefasTipos.codTarefaTipo
LEFT OUTER JOIN	Usuario ON Tarefas.codUsuario = Usuario.codUsuario
WHERE			(1 = 1)
AND				Tarefas.TituloTarefa LIKE '%' + @Titulo + '%'
AND				Usuario.CodUsuario IN (@Solicitante)
AND				Tarefas.Modulo IN (@Modulo)
AND				Tarefas.codTarefaStatus IN (@Status)
AND				Tarefas.codTarefaTipo IN (@Tipo)
AND				(CASE WHEN Tarefas.NumPriorizacao IS NULL 
					THEN 'INDEFINIDA'
					ELSE 'DEFINIDA'
				END) IN (@StatusPrioridade)
AND				ISNULL(Tarefas.UrgenciaUsuario,0) IN (@Urgencia)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Titulo">
