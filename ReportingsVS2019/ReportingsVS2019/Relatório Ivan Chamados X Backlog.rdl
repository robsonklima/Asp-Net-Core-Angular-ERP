    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			* 
FROM			(
					SELECT			Filial.NomeFilial AS Filial, 
									COUNT(Distinct OS.CodOS) AS Qtd,
									(
										SELECT			COUNT(Distinct OS.CodOS) AS Qtd
										FROM			OS
										INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
										INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
										INNER JOIN		Filial F ON OS.CodFilial = F.CodFilial
										WHERE			(1 = 1)
										AND				OS.CodStatusServico NOT IN (2,3)
										AND				OS.CodTipoIntervencao = 2
										AND				OS.CodFilial NOT IN (7, 21)
										AND				OS.CodEquip NOT IN
														(
																SELECT		CodEquip 
																FROM		Equipamento 
																WHERE		NomeEquip LIKE 'POS%'
																OR			NomeEquip LIKE 'PERTOS%'
														)
										AND				Cliente.IndAtivo = 1
										AND				F.CodFilial = Filial.CodFilial
									) Backlog
					FROM			OS
					INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
					INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
					INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
					WHERE			(1 = 1)
					AND				OS.CodStatusServico &lt;&gt; 2
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodFilial NOT IN (7, 21)
					AND				OS.CodEquip NOT IN
									(
											SELECT		CodEquip 
											FROM		Equipamento 
											WHERE		NomeEquip LIKE 'POS%'
											OR			NomeEquip LIKE 'PERTOS%'
									)
					AND				Cliente.IndAtivo = 1
					AND				OS.DataHoraAberturaOS &gt;= 
									CASE WHEN DATEPART(DW, GETDATE()) = 2
											THEN 
												CAST(CAST(GETDATE()-3 AS DATE) AS DATETIME) 
											ELSE
												CAST(CAST(GETDATE()-1 AS DATE) AS DATETIME)
									END
					AND				OS.DataHoraAberturaOS &lt;= 
									CASE WHEN DATEPART(DW, GETDATE()) = 2
											THEN 
												CAST(CAST(GETDATE()-2 AS DATE) AS DATETIME) 
											ELSE
												CAST(CAST(GETDATE() AS DATE) AS DATETIME)
									END
					GROUP BY		Filial.NomeFilial, Filial.CodFilial
					--ORDER BY		Filial.NomeFilial, Filial.CodFilial

					UNION

					SELECT			Filial.NomeFilial AS Filial, 
									0 AS Qtd,
									(
										SELECT			COUNT(Distinct OS.CodOS) AS Qtd
										FROM			OS
										INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
										INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
										INNER JOIN		Filial F ON OS.CodFilial = F.CodFilial
										WHERE			(1 = 1)
										AND				OS.CodStatusServico NOT IN (2,3)
										AND				OS.CodTipoIntervencao = 2
										AND				OS.CodFilial NOT IN (7, 21)
										AND				OS.CodEquip NOT IN
														(
																SELECT		CodEquip 
																FROM		Equipamento 
																WHERE		NomeEquip LIKE 'POS%'
																OR			NomeEquip LIKE 'PERTOS%'
														)
										AND				Cliente.IndAtivo = 1
										AND				F.CodFilial = Filial.CodFilial
									) Backlog
					FROM			OS
					INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
					INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
					INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
					WHERE			(1 = 1)
					AND				OS.CodStatusServico &lt;&gt; 2
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodFilial NOT IN (7, 21)
					AND				OS.CodEquip NOT IN
									(
											SELECT		CodEquip 
											FROM		Equipamento 
											WHERE		NomeEquip LIKE 'POS%'
											OR			NomeEquip LIKE 'PERTOS%'
									)
					AND				Cliente.IndAtivo = 1
					AND				Filial.CodFilial  NOT IN 
									(
										SELECT			DISTINCT Filial.CodFilial 													
										FROM			OS
										INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
										INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
										INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
										WHERE			(1 = 1)
										AND				OS.CodStatusServico &lt;&gt; 2
										AND				OS.CodTipoIntervencao = 2
										AND				OS.CodFilial NOT IN (7, 21)
										AND				OS.CodEquip NOT IN
														(
																SELECT		CodEquip 
																FROM		Equipamento 
																WHERE		NomeEquip LIKE 'POS%'
																OR			NomeEquip LIKE 'PERTOS%'
														)
										AND				Cliente.IndAtivo = 1
										AND				OS.DataHoraAberturaOS &gt;= 
														CASE WHEN DATEPART(DW, GETDATE()) = 2
																THEN 
																	CAST(CAST(GETDATE()-3 AS DATE) AS DATETIME) 
																ELSE
																	CAST(CAST(GETDATE()-1 AS DATE) AS DATETIME)
														END
										AND				OS.DataHoraAberturaOS &lt;= 
														CASE WHEN DATEPART(DW, GETDATE()) = 2
																THEN 
																	CAST(CAST(GETDATE()-2 AS DATE) AS DATETIME) 
																ELSE
																	CAST(CAST(GETDATE() AS DATE) AS DATETIME)
														END
									)
					GROUP BY		Filial.NomeFilial, Filial.CodFilial
				) AS Dados
order by		Qtd desc</CommandText>
      </Query>
