  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		*
FROM		(
				SELECT			Filial.NomeFilial AS Filial, 'CHAMADOS ATMS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2021-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2021-10-01 00:00:00.000'
				AND				OS.CodTipoIntervencao = 2
				AND				OS.CodFilial NOT IN (7, 21,33)
				AND				(OS.CodCliente = 1)  
				AND				(OS.NumOSCliente IS NOT NULL)  
				AND				(EquipamentoContrato.IndGarantia = 0)  
				AND				(EquipamentoContrato.CodContrato = 3145)  
				AND				(OS.IndServico = 1)
				--AND				(EquipamentoContrato.CodEquip IN (select CodEquip from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896')))
				AND				(EquipamentoContrato.CodGrupoEquip = 84)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao), Filial.NomeFilial 
				UNION
				SELECT			Filial.NomeFilial AS Filial, 'CHAMADOS OUTROS EQUIPAMENTOS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2021-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2021-10-01 00:00:00.000'
				AND				OS.CodTipoIntervencao = 2
				AND				OS.CodFilial NOT IN (7, 21,33)
				AND				(OS.CodCliente = 1)  
				AND				(OS.NumOSCliente IS NOT NULL)  
				AND				(EquipamentoContrato.IndGarantia = 0) 
				AND				(EquipamentoContrato.CodContrato = 3145)  
				AND				(OS.IndServico = 1)
				--AND				(EquipamentoContrato.CodEquip IN (select CodEquip from equipamento where nomeequip in ('HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955')))
				AND				(EquipamentoContrato.CodGrupoEquip &lt;&gt; 84)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao), Filial.NomeFilial 
				UNION
				SELECT			Filial.NomeFilial AS Filial, 'TOTAL DE CHAMADOS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2021-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2021-10-01 00:00:00.000'
				AND				OS.CodTipoIntervencao = 2
				AND				OS.CodFilial NOT IN (7, 21,33)
				AND				(OS.CodCliente = 1)  
				AND				(OS.NumOSCliente IS NOT NULL)  
				AND				(EquipamentoContrato.IndGarantia = 0)  
				AND				(EquipamentoContrato.CodContrato = 3145) 
				AND				(OS.IndServico = 1)
				--AND				(EquipamentoContrato.CodEquip IN (select CodEquip from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896','HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955')))
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao), Filial.NomeFilial 
				UNION				
				SELECT			Filial.NomeFilial AS Filial, ' PARQUE ATM' AS Tipo, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select * from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896'))
				AND				Parque_Resumo.CodGrupoEquip = 84
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
				UNION
				SELECT			Filial.NomeFilial AS Filial, ' PARQUE OUTROS EQUIPAMENTOS' AS Tipo, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select CodEquip from equipamento where nomeequip in ('HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955'))
				AND				Parque_Resumo.CodGrupoEquip &lt;&gt; 84
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
				UNION
				SELECT			Filial.NomeFilial AS Filial, 'TOTAL DO PARQUE' AS Tipo, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select CodEquip from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896','HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955'))
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
			) AS DADOS</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
