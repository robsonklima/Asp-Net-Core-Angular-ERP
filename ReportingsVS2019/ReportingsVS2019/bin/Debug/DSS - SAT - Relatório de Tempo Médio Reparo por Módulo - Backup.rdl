    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="NomePeca">
          <DataField>NomePeca</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CodMagnus">
          <DataField>CodMagnus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TempoMedio">
          <DataField>TempoMedio</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			NomePeca,peca.CodMagnus, COUNT(ori.CodPeca) Qtd,
				((select dbo.FN_CONVMIN(AVG(DATEDIFF(mi,ORTempoReparo.DataHoraInicio, ORTempoReparo.DataHoraFim)))
						FROM ORTempoReparo WHERE CodORItem IN (SELECT CodORItem FROM ORItem WHERE ORItem.IndAtivo IN (0,2)
																								AND	CodPeca = ori.CodPeca)
																								AND DataHoraCad &gt; @DataFiltro2)) as TempoMedio

																								
FROM			ORItem ori
INNER JOIN		Peca ON Peca.CodPeca = ori.CodPeca
--INNER JOIN		Usuario ON CodUsuario = ori.CodTecnico
LEFT OUTER JOIN		ORTempoReparo ON ORTempoReparo.CodORItem = ori.CodORItem 
where			ori.IndAtivo IN (0,2)
AND				ORTempoReparo.DataHoraInicio BETWEEN @DataFiltro1 AND @DataFiltro2
GROUP BY		NomePeca,peca.CodMagnus ,ori.CodPeca
ORDER BY		NomePeca</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@DataFiltro2">
            <Value>=Parameters!DataFiltro2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataFiltro1">
            <Value>=Parameters!DataFiltro1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
