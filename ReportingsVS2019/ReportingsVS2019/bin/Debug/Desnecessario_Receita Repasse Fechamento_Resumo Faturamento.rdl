  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT			TipoEquipamento.NomeTipoEquip AS TipoModelo, Filial.NomeFilial AS Filial, 
			SUM(vwc_Parque_Resumo.ValorReceita) AS ValorReceita
FROM			vwc_Parque_Resumo 
INNER JOIN		TipoEquipamento ON vwc_Parque_Resumo.CodTipoEquip = TipoEquipamento.CodTipoEquip
INNER JOIN		Filial ON vwc_Parque_Resumo.CodFilial = Filial.CodFilial
WHERE			(1=1)
AND			(vwc_Parque_Resumo.IndAtivo IN (@IndAtivo))
AND			(vwc_Parque_Resumo.IndReceita IN (@IndReceita))
AND			(vwc_Parque_Resumo.IndGarantia IN (@IndGarantia))
AND			(vwc_Parque_Resumo.AnoMes IN (@AnoMes))
AND			(vwc_Parque_Resumo.CodTipoEquip IN (@CodTipoEquip))
AND			(vwc_Parque_Resumo.CodFilial IN (@CodFilial))
GROUP BY                               TipoEquipamento.NomeTipoEquip, Filial.NomeFilial, vwc_Parque_Resumo.ValorReceita
Order by	  	                TipoEquipamento.NomeTipoEquip, Filial.NomeFilial</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndGarantia">
            <Value>=Parameters!IndGarantia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodTipoEquip">
            <Value>=Parameters!CodTipoEquip.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodFilial">
            <Value>=Parameters!CodFilial.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="TipoModelo">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>TipoModelo</DataField>
        </Field>
        <Field Name="Filial">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Filial</DataField>
        </Field>
        <Field Name="ValorReceita">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ValorReceita</DataField>
        </Field>
      </Fields>
    </DataSet>
