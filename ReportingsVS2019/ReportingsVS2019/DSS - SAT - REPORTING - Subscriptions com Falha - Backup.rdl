  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="NomeRelatorio">
          <DataField>NomeRelatorio</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Emails">
          <DataField>Emails</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UltimoStatus">
          <DataField>UltimoStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DataHoraEnvio">
          <DataField>DataHoraEnvio</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT_REPORT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT			ReportServer.dbo.catalog.name AS NomeRelatorio,
						Substring(ReportServer.dbo.subscriptions.extensionsettings, Charindex('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', Cast(ReportServer.dbo.subscriptions.extensionsettings AS VARCHAR(max)))
						+ 22, Charindex('&lt;/Value&gt;', Cast(ReportServer.dbo.subscriptions.extensionsettings AS VARCHAR(max))) - ( Charindex('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', Cast(ReportServer.dbo.subscriptions.extensionsettings AS VARCHAR(max)))
						+ 22 )) AS Emails,
						ReportServer.dbo.subscriptions.laststatus AS UltimoStatus,
						ReportServer.dbo.subscriptions.lastruntime AS DataHoraEnvio
FROM					ReportServer.dbo.subscriptions
INNER JOIN				ReportServer.dbo.catalog ON ReportServer.dbo.subscriptions.report_oid = ReportServer.dbo.catalog.itemid
WHERE					(1 = 1)
AND						ReportServer.dbo.subscriptions.lastruntime	&gt;= GETDATE() - 3
AND						ReportServer.dbo.Subscriptions.LastStatus like '%Failure%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
