  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT				DISTINCT DispBBBloqueioOS.CodDispBBBloqueioOS, DispBBBloqueioOS.CodOS, DispBBBloqueioOS.IndAtivo, DispBBBloqueioOS.DataHoraCad, DispBBBloqueioOS.TipoBloqueio, TipoIntervencao.NomTipoIntervencao, Equipamento.NomeEquip, StatusServico.NomeStatusServico, 
					  ISNULL(                                                                                                                                                                                                                                                                                                                                                                
							   (                                                                                                                                                                                                                                                                                                                                                                     
								SELECT      CodMotivo                                                                                                                                                                                                                                                                                                                                  
								FROM        DeParaCausaMotivo                                                                                                                                                                                                                                                                                                                  
								WHERE       CodECausa =                                                                                                                                                                                                                                                                                                                        
												   (                                                                                                                                                                                                                                                                                                                                                 
																		 ISNULL(                                                                                                                                                                                                                                                                                                                     
																							 CASE (SELECT CodTipoIntervencao FROM OS WHERE OS.CodOS = RAT.CodOS) WHEN 32                                                                                                                                                                                    
																							 THEN                                                                                                                                                                                                                                                                                                        
																										 (                                                                                                                                                                                                                                                                                                        
																												SELECT           TOP 1 C.CodECausa                                                                                                                                                                                                                                            
																												FROM             RATDetalhes RD                                                                                                                                                                                                                                        
																												INNER JOIN       Causa C    ON RD.CodCausa = C.CodCausa                                                                                                                                                                                                   
																												WHERE            C.CodECausa IN ('05540','05550','18000','18300','18400','18402','18600')                                                                                                                                            
																												AND              RD.CodOS = DispBBBloqueioOS.CodOS                                                                                                                                                                                                                           
																										 )                                                                                                                                                                                                                                                                                                        
																							 ELSE                                                                                                                                                                                                                                                                                                        
																										 (                                                                                                                                                                                                                                                                                                        
																												SELECT           TOP 1 C.CodECausa                                                                                                                                                                                                                                            
																												FROM             RATDetalhes RD                                                                                                                                                                                                                                        
																												INNER JOIN       Causa C    ON RD.CodCausa = C.CodCausa                                                                                                                                                                                                   
																												WHERE            (C.CodECausa LIKE '02%'                                                                                                                                                                                                                               
																																		   OR C.CodECausa LIKE '08%'                                                                                                                                                                                                                          
																																		   AND    C.CodECausa NOT IN ('08300','08310','08320'))                                                                                                                                                                                 
																												AND              RD.CodOS = DispBBBloqueioOS.CodOS                                                                                                                                                                                                                           
																										 )                                                                                                                                                                                                                                                                                                        
																							  END                                                                                                                                                                                                                                                                                                        
																							 ,                                                                                                                                                                                                                                                                                                            
																							 Causa.CodECausa                                                                                                                                                                                                                                                                                           
																		 )                                                                                                                                                                                                                                                                                                                                       
												   )                                                                                                                                                                                                                                                                                                                                                 
							   ),'000'                                                                                                                                                                                                                                                                                                                                                        
					  ) AS CodMotivo,
					  SLA_NEW.NomeSLA AS SLA, 
					  DispBBRegiao.Nome AS Regiao
FROM				DispBBBloqueioOS 
INNER JOIN			OS ON DispBBBloqueioOS.CodOS = OS.CodOS
INNER JOIN			TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
INNER JOIN			Equipamento ON Equipamento.CodEquip = OS.CodEquip
INNER JOIN			StatusServico ON OS.CodStatusServico = StatusServico.CodStatusServico
LEFT OUTER JOIN		RAT ON RAT.CodOS = OS.CodOS 
LEFT OUTER JOIN		RATDetalhes ON RATDetalhes.CodRAT = RAT.CodRAT 
LEFT OUTER JOIN		Causa ON RATDetalhes.CodTipoCausa = Causa.CodTipoCausa 
					AND RATDetalhes.CodGrupoCausa = Causa.CodGrupoCausa 
					AND RATDetalhes.CodCausa = Causa.CodCausa 
INNER JOIN			EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
INNER JOIN			SLA_NEW ON EquipamentoContrato.CodSLA = SLA_NEW.CodSLA
INNER JOIN			LocalAtendimento ON LocalAtendimento.CodPosto = EquipamentoContrato.CodPosto
INNER JOIN			Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
INNER JOIN			UF ON UF.CodUF = Cidade.CodUF
INNER JOIN			DispBBRegiaoUF  ON UF.CodUF = DispBBRegiaoUF.CodUF
INNER JOIN			DispBBRegiao ON DispBBRegiaoUF.CodDispBBRegiao = DispBBRegiao.CodDispBBRegiao
WHERE				DispBBBloqueioOS.IndAtivo = 1
ORDER BY			DispBBBloqueioOS.DataHoraCad DESC

</CommandText>
