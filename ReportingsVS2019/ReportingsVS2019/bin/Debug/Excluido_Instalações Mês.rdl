          <DataField>ValorInstalacao</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Filial.NomeFilial AS Filial, Cliente.NomeFantasia AS Cliente,
			Contrato.NomeContrato AS Contrato, Equipamento.NomeEquip AS Modelo,
			SUM(ISNULL(ContratoServico.Valor, 0.00)) AS ValorInstalacao
FROM			Cliente 
RIGHT OUTER JOIN		OS ON Cliente.CodCliente = OS.CodCliente 
LEFT OUTER JOIN		Contrato 
INNER JOIN		Equipamento 
INNER JOIN		EquipamentoContrato ON Equipamento.CodEquip = EquipamentoContrato.CodEquip 
			AND Equipamento.CodGrupoEquip = EquipamentoContrato.CodGrupoEquip 
			AND Equipamento.CodTipoEquip = EquipamentoContrato.CodTipoEquip 
			ON Contrato.CodContrato = EquipamentoContrato.CodContrato 
			ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato 
LEFT OUTER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
LEFT OUTER JOIN		ContratoServico ON Contrato.CodContrato = ContratoServico.CodContrato
			AND ContratoServico.CodServico = 36
			AND Equipamento.CodEquip = ContratoServico.CodEquip
			AND Equipamento.CodGrupoEquip = ContratoServico.CodGrupoEquip
			AND Equipamento.CodTipoEquip = ContratoServico.CodTipoEquip
WHERE			(OS.CodFilial NOT IN (7))
AND			(OS.CodStatusServico IN (3))
AND			(OS.CodTipoIntervencao IN (4))
AND			((SELECT TOP 1	 DataHoraSolucao
				FROM	 RAT AS RAT_2
				WHERE	 (CodOS = OS.CodOS)
				ORDER BY CodRAT DESC) &gt;= CONVERT(DateTime, @DataHoraInicio + ' 00:00:00.000'))
AND			((SELECT TOP 1	 DataHoraSolucao
				FROM       RAT AS RAT_1
				WHERE	(CodOS = OS.CodOS)
				ORDER BY	CodRAT DESC) &lt;= CONVERT(DateTime, @DataHoraFinal + ' 23:59:59.999'))
GROUP BY		Filial.NomeFilial, Cliente.NomeFantasia, Contrato.NomeContrato, Equipamento.NomeEquip, OS.CodOS
ORDER BY		Filial.NomeFilial, Cliente.NomeFantasia, Contrato.NomeContrato, Equipamento.NomeEquip</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DataHoraInicio">
