				SELECT			'TOTAL DE CHAMADOS' AS Tipo, Filial.NomeFilial AS Filial, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
				AND				OS.CodStatusServico &lt;&gt; 2
