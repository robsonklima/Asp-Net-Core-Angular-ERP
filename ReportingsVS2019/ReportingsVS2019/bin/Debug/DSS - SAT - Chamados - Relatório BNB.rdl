  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			AnoMes AS Mes, Indice, Tipo, SUM(Qtde) AS Qtde
FROM 			(
					SELECT				Cliente.NomeFantasia, 
										CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR) AS AnoMes, 
										1 AS Indice,
										'Qtde Chamados Corretivos' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde										
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente = 253
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) &gt;= '2017'
					AND					OS.CodEquip IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'TMF%')
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodTipoIntervencao = 2
					GROUP BY			Cliente.NomeFantasia, CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)

					UNION

					SELECT				Cliente.NomeFantasia, 
										CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR) AS AnoMes, 
										2 AS Indice,
										'Qtde Chamados Preventivos' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde								
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente = 253
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) &gt;= '2017'
					AND					OS.CodEquip IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'TMF%')
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodTipoIntervencao = 6
					GROUP BY			Cliente.NomeFantasia, CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)

					UNION
										
					SELECT				Cliente.NomeFantasia, 
										Parque_Resumo.AnoMes, 
										3 AS Indice,
										'Parque Ativos' AS Tipo,
										COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS Qtde
					FROM				Parque_Resumo 
					INNER JOIN			Cliente ON Parque_Resumo.CodCliente = Cliente.CodCliente
					WHERE				Parque_Resumo.CodCliente = 253
					AND					SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),1,4) &gt;= '2017'
					AND					Parque_Resumo.IndAtivo = 1
					AND					Parque_Resumo.CodEquip IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'TMF%')
					GROUP BY			Cliente.NomeFantasia, Parque_Resumo.AnoMes

				) AS Dados
GROUP BY		AnoMes, Indice, Tipo
ORDER BY		AnoMes, Indice, Tipo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
