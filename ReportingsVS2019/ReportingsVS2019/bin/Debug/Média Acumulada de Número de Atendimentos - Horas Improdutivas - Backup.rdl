      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>/****** Object: [???]    Script Date: 19/05/2016 10:23:42 Author: Robson Lima ******/
SELECT nome_filial

	 -- Quantidade de atendimentos dividido pela quantidade de dias úteis anual
	 , cast(media_acum_atend_tecnico_anual AS FLOAT) / (CASE cast(dias_uteis_por_tecnico_anual AS FLOAT)
		   WHEN 0 THEN
			   1
		   ELSE
			   dias_uteis_por_tecnico_anual
	   END) AS media_acum_atend_tecnico_anual_calc

	 -- Quantidade de atendimentos dividido pela quantidade de dias úteis mensal
	 , cast(media_acum_atend_tecnico_mensal AS FLOAT) / (CASE cast(dias_uteis_por_tecnico_mensal AS FLOAT)
		   WHEN 0 THEN
			   1
		   ELSE
			   dias_uteis_por_tecnico_mensal
	   END) AS media_acum_atend_tecnico_mensal_calc

	 -- soma de (dataSolucaoRat - dataInicioRat) dividido pela quantidade de RATs nos ultimos 30 dias do tecnico
	 , dbo.FN_CONVMIN(media_tempo_solucao) AS media_tempo_solucao_horas


	 , dbo.FN_CONVMIN(cast(cast(media_acum_atend_tecnico_mensal AS FLOAT) / (CASE cast(dias_uteis_por_tecnico_mensal AS FLOAT)
		   WHEN 0 THEN
			   1
		   ELSE
			   dias_uteis_por_tecnico_mensal
	   END) * media_tempo_solucao AS INT)) AS horas_utilizadas

	 -- 8:48 menos horas_utilizadas
	 , dbo.FN_CONVMIN(528 - (cast(cast(media_acum_atend_tecnico_mensal AS FLOAT) / (CASE cast(dias_uteis_por_tecnico_mensal AS FLOAT)
		   WHEN 0 THEN
			   1
		   ELSE
			   dias_uteis_por_tecnico_mensal
	   END) * media_tempo_solucao AS INT))) AS horas_nao_utilizadas

FROM
	(SELECT Filial.NomeFilial AS nome_filial
		  , Tecnico.Nome AS nome_tecnico

		  -- Descrever o bloco
		  , CASE
				WHEN Tecnico.DataAdmissao &gt;= (getdate() - 365) THEN
					dbo.fc_VerificaDiasUteis(Tecnico.DataAdmissao, getdate())
				ELSE
					dbo.fc_VerificaDiasUteis(getdate() - 365, getdate())
			END AS dias_uteis_por_tecnico_anual

		  -- Descrever o bloco
		  , (SELECT count(*)
			 FROM
				 RAT
				 INNER JOIN OS
					 ON OS.CodOS = RAT.CodOS
			 WHERE
				 (1 = 1)
				 AND RAT.CodTecnico = Tecnico.CodTecnico
				 -- somente os com tipo de intervencao: corretiva
				 AND OS.CodTipoIntervencao IN (2)
				 AND OS.DataHoraCad &gt; (getdate() - 365)) AS media_acum_atend_tecnico_anual
		  , CASE
				WHEN Tecnico.DataAdmissao &gt;= (getdate() - 30) THEN
					dbo.fc_VerificaDiasUteis(Tecnico.DataAdmissao, getdate())
				ELSE
					dbo.fc_VerificaDiasUteis(getdate() - 30, getdate())
			END AS dias_uteis_por_tecnico_mensal

		  -- Descrever o bloco
		  , (SELECT count(*)
			 FROM
				 RAT
				 INNER JOIN OS
					 ON OS.CodOS = RAT.CodOS
			 WHERE
				 (1 = 1)
				 AND RAT.CodTecnico = Tecnico.CodTecnico
				 -- somente os com tipo de intervencao: corretiva
				 AND OS.CodTipoIntervencao IN (2)
				 AND OS.DataHoraCad &gt; (getdate() - 30)) AS media_acum_atend_tecnico_mensal

		  -- Descrever o bloco
		  , isnull((SELECT sum(datediff(MINUTE, DataHoraInicio, DataHoraSolucao)) / isnull((SELECT count(codrat)
																							FROM
																								rat
																								INNER JOIN OS
																									ON OS.codos = RAT.codos
																							WHERE
																								RAT.DataHoraCad &gt; (getdate() - 30)
																								AND RAT.CodTecnico = Tecnico.CodTecnico
																								-- somente os com tipo de intervencao: corretiva
																								AND OS.CodTipoIntervencao IN (2)), 1)
					FROM
						rat
						INNER JOIN OS
							ON OS.codos = RAT.codos
					WHERE
						RAT.DataHoraCad &gt; (getdate() - 30)
						AND RAT.CodTecnico = Tecnico.CodTecnico
						-- somente os com tipo de intervencao: corretiva
						AND OS.CodTipoIntervencao IN (2)), 0) AS media_tempo_solucao
	 FROM
		 Tecnico
		 INNER JOIN Filial
			 ON Tecnico.CodFilial = Filial.CodFilial
		 INNER JOIN Usuario
			 ON Usuario.CodTecnico = Tecnico.CodTecnico
	 WHERE
		 (1 = 1)
		 AND Tecnico.IndAtivo = 1
		 AND Usuario.IndAtivo = 1
		 -- somente usuarios com perfil tecnico
		 AND Usuario.CodPerfil = 35
		 -- desconsidera tecnicos abstratos, ex.: ivan medina
		 AND Tecnico.CodTecnico NOT IN (1763, 1382)) AS dados
WHERE
	nome_filial IN (@nomeFilial)
ORDER BY
	nome_filial ASC
  , nome_tecnico ASC</CommandText>
