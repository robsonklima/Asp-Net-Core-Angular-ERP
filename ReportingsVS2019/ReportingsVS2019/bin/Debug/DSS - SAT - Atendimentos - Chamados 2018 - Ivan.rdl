  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		*
FROM		(
				SELECT			'GARANTIA' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
				AND				(
									OS.CodCliente NOT IN (197, 256, 76, 166, 444)
									AND 
									EquipamentoContrato.CodContrato NOT IN (3050,3651)
								)
				AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)
				AND				Contrato.CodTipoContrato = 2
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
				UNION
				SELECT			'DSR' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
				AND				OS.CodCliente IN (197, 256, 76, 166, 444)		
				AND				EquipamentoContrato.CodContrato NOT IN (3050,3651)
				AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)		
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
				UNION
				SELECT			'CONTRATOS E SERVIÇOS EVENTUAIS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
				AND				(
									OS.CodTipoIntervencao IN (5, 17, 18, 19, 20)				
									OR
									(
										Contrato.CodTipoContrato = 1
										AND		
										(
											OS.CodCliente NOT IN (197, 256, 76, 166, 444)
											OR
											EquipamentoContrato.CodContrato IN (3050,3651)
										)
									)
								)
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
				UNION
				SELECT			'OUTROS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
				AND				Contrato.CodTipoContrato NOT IN (1,2)			
				AND				EquipamentoContrato.CodContrato NOT IN (3050,3651)
				AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)				
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				AND				RAT.CodRAT NOT IN 
								(
										SELECT			RAT.CodRAT
										FROM			RAT
										INNER JOIN		OS ON RAT.CodOS = OS.CodOS
										INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
										INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
										AND				OS.CodStatusServico &lt;&gt; 2
										AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
										AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
										AND				OS.CodCliente IN (197, 256, 76, 166, 444)		
										AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)		
										AND				OS.CodFilial NOT IN (7, 21)
										AND				OS.CodCliente &lt;&gt; 220
										AND				OS.CodEquipContrato NOT IN
														(
															SELECT			CodEquipContrato 
															FROM			EquipamentoContrato 
															WHERE			CodCliente = 2	
															AND				CodEquip IN
																			(
																				SELECT		CodEquip 
																				FROM		Equipamento 
																				WHERE		NomeEquip LIKE 'POS%'
																				OR			NomeEquip LIKE 'PERTOS%'
																			)
														)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
				UNION
				SELECT			'TOTAL DE CHAMADOS' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2018-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2019-01-01 00:00:00.000'
				--AND				OS.CodCliente NOT IN (197, 256, 76, 166, 444)
				--AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)	
				--AND				Contrato.CodTipoContrato NOT IN (1,2)
				AND				OS.CodFilial NOT IN (7, 21)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
			) AS DADOS
</CommandText>
      </Query>
