    <DataSet Name="DsMes">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>;WITH months(MonthNumber) AS
(
    SELECT 1
    UNION ALL
    SELECT MonthNumber+1 
    FROM months
    WHERE MonthNumber &lt; 12
)
select MonthNumber, (CASE WHEN MonthNumber = 1 THEN 'Janeiro' 
             					 WHEN MonthNumber = 2 THEN 'Fevereiro' 
             					 WHEN MonthNumber = 3 THEN 'Março' 
             					 WHEN MonthNumber = 4 THEN 'Abril' 
             					 WHEN MonthNumber = 5 THEN 'Maio' 
             					 WHEN MonthNumber = 6 THEN 'Junho' 
             					 WHEN MonthNumber = 7 THEN 'Julho'
             					 WHEN MonthNumber = 8 THEN 'Agosto'
             					 WHEN MonthNumber = 9 THEN 'Setembro'
             					 WHEN MonthNumber = 10 THEN 'Outubro'
             					 WHEN MonthNumber = 11 THEN 'Novembro'
             					 WHEN MonthNumber = 12 THEN 'Dezembro'
             				ELSE ''
             				END) mesAtual
from months
--WHERE MonthNumber BETWEEN MONTH(GETDATE()) - 3 AND MONTH(getdate())</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthNumber">
          <DataField>MonthNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="mesAtual">
          <DataField>mesAtual</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DsAno">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>;WITH years(YearNumber) AS
(
    SELECT 2017
    UNION ALL
    SELECT YearNumber+1 
    FROM years
    WHERE YearNumber &lt; 2022
)
select YearNumber
from years


</CommandText>
      </Query>
