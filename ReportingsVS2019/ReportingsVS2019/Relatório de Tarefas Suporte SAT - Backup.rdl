        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE		@DATA	DATETIME
DECLARE		@TOTAL INT
DECLARE		@TOTALFIM INT

SET			@DATA = getdate() --'2016-05-13 23:00:00.000'

SET			@TOTAL =	(
								SELECT		count(*)
								FROM		SATSUP.dbo.Tarefa
								WHERE		SATSUP.dbo.Tarefa.CodClassificacao = 1
								AND			SATSUP.dbo.Tarefa.DataHoraCad &lt;= @DATA
						)
						
SET			@TOTALFIM =	(
								SELECT		count(*)
								FROM		SATSUP.dbo.Tarefa
								LEFT JOIN	SATSUP.dbo.Atendimento
											ON SATSUP.dbo.Atendimento.CodTarefa = SATSUP.dbo.Tarefa.CodTarefa
								WHERE		SATSUP.dbo.Tarefa.CodClassificacao = 1
								AND			SATSUP.dbo.Tarefa.CodStatus IN  (4,5)
								AND			SATSUP.dbo.Tarefa.DataHoraCad &lt;= @DATA
								AND			SATSUP.dbo.Atendimento.DataHoraCad &lt;= @DATA
								AND			SATSUP.dbo.Atendimento.CodStatus IN (4,5)

						)



--TOTAL DE CHAMADOS
SELECT 
(SELECT top 1 id + 1 from SATSUP.dbo.BacklogTarefas order BY 1 desc) AS id,
(SELECT		count(*)
FROM		SATSUP.dbo.Tarefa
WHERE		CodClassificacao = 1
AND			DataHoraCad &lt;= @DATA) AS 'TarefasAbertas',


--TOTAL DE CHAMADOS FINALIZADOS

(SELECT					count(*) 
FROM					SATSUP.dbo.Tarefa
LEFT OUTER JOIN			SATSUP.dbo.Atendimento
						ON Atendimento.CodTarefa = Tarefa.CodTarefa
WHERE					CodClassificacao = 1
AND						Tarefa.CodStatus IN  (4,5)
AND						Tarefa.DataHoraCad &lt;= @DATA
AND						Atendimento.DataHoraCad &lt;= @DATA
AND						Atendimento.CodStatus IN (4,5)) AS 'TarefasFechadasCanceladas',

--BACKLOG

(SELECT (@TOTAL-@TOTALFIM)) AS Backlog,
(SELECT @DATA) AS DataHoraBacklog

UNION

SELECT * FROM SATSUP.dbo.BacklogTarefas</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
