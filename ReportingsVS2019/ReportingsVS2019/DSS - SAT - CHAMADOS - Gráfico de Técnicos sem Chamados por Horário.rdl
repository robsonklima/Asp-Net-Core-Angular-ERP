    <DataSet Name="DsQTDTecnico">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT				COUNT(DISTINCT Tecnico.CodTecnico)
						FROM				Tecnico
						INNER JOIN			Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
						WHERE				CodPerfil IN (35,84) 
						AND					Usuario.CodTecnico IS NOT NULL 
						AND					Usuario.CodTecnico NOT IN (1763,1382,646)
						AND					Usuario.IndAtivo = 1
						AND					Tecnico.indFerias &lt;&gt; 1</CommandText>
      </Query>
