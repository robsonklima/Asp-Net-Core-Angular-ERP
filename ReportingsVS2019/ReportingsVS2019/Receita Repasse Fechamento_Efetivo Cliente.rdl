  <DataSets>
    <DataSet Name="SAT">
      <Fields>
        <Field Name="NomeTipoEquip">
          <DataField>NomeTipoEquip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomeFantasia">
          <DataField>NomeFantasia</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QtdEquiptos">
          <DataField>QtdEquiptos</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Receita">
          <DataField>Receita</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		TE.NomeTipoEquip, CL.NomeFantasia, COUNT(VPR.CodEquip) AS QtdEquiptos, SUM(VPR.ValorReceita) AS Receita
FROM		vwc_Parque_Resumo VPR
INNER JOIN	Cliente CL ON VPR.CodCliente = CL.CodCliente
INNER JOIN	Contrato CO ON VPR.CodContrato = CO.CodContrato
INNER JOIN	TipoEquipamento TE ON VPR.CodTipoEquip = TE.CodTipoEquip
INNER JOIN	Equipamento EQ ON VPR.CodEquip = EQ.CodEquip
		AND TE.CodTipoEquip = EQ.CodTipoEquip
WHERE		(1=1)
AND		(VPR.IndAtivo IN (@IndAtivo))
AND		(VPR.IndGarantia IN (@IndGarantia))
AND		(VPR.IndReceita IN (@IndReceita))
AND		(VPR.AnoMes IN (@AnoMes))
GROUP BY	TE.NomeTipoEquip, CL.NomeFantasia
ORDER BY	TE.NomeTipoEquip, CL.NomeFantasia</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndGarantia">
            <Value>=Parameters!IndGarantia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsAnoMes">
      <Fields>
        <Field Name="AnoMes">
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT DISTINCT AnoMes FROM Parque_Resumo ORDER BY AnoMes</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsAnoMesCorrente">
      <Fields>
        <Field Name="AnoMes">
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT TOP 1 AnoMes FROM Parque_Resumo ORDER BY AnoMes DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
