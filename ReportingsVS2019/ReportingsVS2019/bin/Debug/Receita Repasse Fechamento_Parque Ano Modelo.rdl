        <Field Name="NomeTipoEquip">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeTipoEquip</DataField>
        </Field>
        <Field Name="NomeGrupoEquip">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeGrupoEquip</DataField>
        </Field>
        <Field Name="AnoMes">
