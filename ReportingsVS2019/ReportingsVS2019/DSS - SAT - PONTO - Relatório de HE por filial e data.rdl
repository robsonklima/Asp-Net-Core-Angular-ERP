    <DataSet Name="DsFilial">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT NomeFilial, CodFilial FROM Filial WHERE CodFilial NOT IN (7,21) AND IndAtivo = 1 ORDER BY 1 DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NomeFilial">
