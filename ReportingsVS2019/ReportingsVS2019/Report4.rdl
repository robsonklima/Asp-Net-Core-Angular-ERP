  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			TipoEquipamento.NomETipoEquip AS TipoEquipamento, SUBSTRING(CAST(AnoMes AS VARCHAR),1,4) AS Ano,
				CASE WHEN SUBSTRING(CAST(AnoMes AS VARCHAR),1,4) = '2012' 
						THEN COUNT(*) / 6
						ELSE COUNT(*) / 12
				END AS QTD			
FROM			Parque_Resumo
INNER JOIN		TipoEquipamento ON Parque_Resumo.CodTipoEquip = TipoEquipamento.CodTipoEquip
WHERE			Parque_Resumo.IndAtivo = 1
AND 			Parque_Resumo.AnoMes IS NOT NULL
GROUP BY		TipoEquipamento.NomETipoEquip, SUBSTRING(CAST(AnoMes AS VARCHAR),1,4)
ORDER BY		SUBSTRING(CAST(AnoMes AS VARCHAR),1,4), TipoEquipamento.NomETipoEquip</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TipoEquipamento">
          <DataField>TipoEquipamento</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ano">
          <DataField>Ano</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTD">
          <DataField>QTD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
