    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>--SELECT			month(OS.DataHoraAberturaOS) AS Mes,
--				CAST((DATEPART(week, OS.DataHoraAberturaOS) - DATEPART(week, CONVERT(CHAR(6), OS.DataHoraAberturaOS, 112)+'01') + 1) AS VARCHAR) + ' ° Semana' AS Semana,
--				COUNT(Distinct OS.CodOS) AS Qtd
--FROM			OS
--INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato				
--AND				OS.CodStatusServico &lt;&gt; 2
--AND				OS.DataHoraAberturaOS &gt;= '2018-11-01 00:00:00.000'
--AND				OS.CodTipoIntervencao = 2
--AND				OS.CodFilial NOT IN (7, 21)
--AND				OS.CodCliente = 2
--AND				OS.CodEquip NOT IN
--				(
--						SELECT		CodEquip 
--						FROM		Equipamento 
--						WHERE		NomeEquip LIKE 'POS%'
--						OR			NomeEquip LIKE 'PERTOS%'
--				)
--GROUP BY		month(OS.DataHoraAberturaOS), CAST((DATEPART(week, OS.DataHoraAberturaOS) - DATEPART(week, CONVERT(CHAR(6), OS.DataHoraAberturaOS, 112)+'01') + 1) AS VARCHAR)
--ORDER BY		1,2

SELECT * FROM 
(
	SELECT			CAST(OS.DataHoraAberturaOS AS DATE) AS Dia,
					'CORRETIVOS' AS Intervencao,
					COUNT(Distinct OS.CodOS) AS Qtd
	FROM			OS
	INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato				
	AND				OS.CodStatusServico &lt;&gt; 2
	AND				OS.DataHoraAberturaOS &gt;= '2020-01-01 00:00:00.000'
	AND				OS.DataHoraAberturaOS &lt; '2020-12-01 00:00:00.000'
	AND				DAY(OS.DataHoraAberturaOS) IN (4,5,6,7,8)
	AND				OS.CodTipoIntervencao = 2
	AND				OS.CodFilial NOT IN (7, 21,33)
	AND				OS.CodEquip NOT IN
					(
							SELECT		CodEquip 
							FROM		Equipamento 
							WHERE		NomeEquip LIKE 'POS%'
							OR			NomeEquip LIKE 'PERTOS%'
					)
	GROUP BY		CAST(OS.DataHoraAberturaOS AS DATE) 
	union
	SELECT			CAST(OS.DataHoraAberturaOS AS DATE) AS Dia,
					'ORÇAMENTOS' AS Intervencao,
					COUNT(Distinct OS.CodOS) AS Qtd
	FROM			OS
	INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato				
	AND				OS.CodStatusServico &lt;&gt; 2
	AND				OS.DataHoraAberturaOS &gt;= '2020-01-01 00:00:00.000'
	AND				OS.DataHoraAberturaOS &lt; '2020-12-01 00:00:00.000'
	AND				DAY(OS.DataHoraAberturaOS) IN (4,5,6,7,8)
	AND				OS.CodTipoIntervencao in (5,17,18,19,20)
	AND				OS.CodFilial NOT IN (7, 21,33)
	AND				OS.CodEquip NOT IN
					(
							SELECT		CodEquip 
							FROM		Equipamento 
							WHERE		NomeEquip LIKE 'POS%'
							OR			NomeEquip LIKE 'PERTOS%'
					)
	GROUP BY		CAST(OS.DataHoraAberturaOS AS DATE) 
) AS dados
ORDER BY		Dia</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
