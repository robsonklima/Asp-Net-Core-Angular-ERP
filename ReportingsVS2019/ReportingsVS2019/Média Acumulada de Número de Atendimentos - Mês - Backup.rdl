      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE	@tabela_dados AS TABLE(nome_filial VARCHAR(6), qtd_atend INT, qtd_tecnico INT, media FLOAT, medi_temp_solu VARCHAR(10), hora_tecn_util VARCHAR(10), horas_nao_usadas VARCHAR(10), periodo VARCHAR(30))

--media anual todas as intervencoes
INSERT INTO @tabela_dados

	SELECT f.NomeFilial AS nome_filial
		 , count(DISTINCT r.CodRAT) AS qtd_atend
		 , tf.QtdTecnicos AS qtd_tecnico
		 , (((cast(count(DISTINCT r.CodRAT) AS FLOAT) / (tf.QtdTecnicos * 12))) / 23) as media
		 , dbo.FN_CONVMIN((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT))) as medi_temp_solu
		 , dbo.FN_CONVMIN(((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / (tf.QtdTecnicos * 12)) / 23))) as hora_tecn_util
		 , dbo.FN_CONVMIN(528 - ((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / (tf.QtdTecnicos * 12)) / 23))) as horas_nao_usadas
		 , 'ANUAL - TODOS' as periodo_tipo
	FROM
		filial f
		INNER JOIN OS o
			ON o.CodFilial = f.CodFilial
		INNER JOIN RAT r
			ON r.CodOS = o.codos
		INNER JOIN Tecnico t
			ON r.CodTecnico = t.CodTecnico
		LEFT JOIN Usuario u
			ON u.CodTecnico = t.CodTecnico
		INNER JOIN TecnicosFilial tf
			ON tf.CodFilial = f.CodFilial
	WHERE
		(1 = 1)
		AND f.IndAtivo IN (1)
		AND u.CodPerfil IN (35)
		AND t.codtecnico NOT IN (1763, 1382)
		AND o.CodEquip NOT IN (85, 107, 134, 147, 157, 158, 172, 204, 268)
		AND r.DataHoraSolucao &gt;= (getdate()-365)
	GROUP BY
		f.NomeFilial, tf.QtdTecnicos

--media mensal todas as intervencoes
INSERT INTO @tabela_dados

	SELECT f.NomeFilial AS nome_filial
		 , count(DISTINCT r.CodRAT) AS qtd_atend
		 , tf.QtdTecnicos AS qtd_tecnico
		 , ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23) as media
		 , dbo.FN_CONVMIN((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT))) as medi_temp_solu
		 , dbo.FN_CONVMIN(((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23))) as hora_tecn_util
		 , dbo.FN_CONVMIN(528 - ((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23))) as horas_nao_usadas
		 , 'MENSAL - TODOS' as periodo_tipo
	FROM
		filial f
		INNER JOIN OS o
			ON o.CodFilial = f.CodFilial
		INNER JOIN RAT r
			ON r.CodOS = o.codos
		INNER JOIN Tecnico t
			ON r.CodTecnico = t.CodTecnico
		LEFT JOIN Usuario u
			ON u.CodTecnico = t.CodTecnico
		INNER JOIN TecnicosFilial tf
			ON tf.CodFilial = f.CodFilial
	WHERE
		(1 = 1)
		AND f.IndAtivo IN (1)
		AND u.CodPerfil IN (35)
		AND t.codtecnico NOT IN (1763, 1382)
		AND o.CodEquip NOT IN (85, 107, 134, 147, 157, 158, 172, 204, 268)
		AND r.DataHoraSolucao BETWEEN FORMAT(DATEADD(MM,-1,DATEADD(DD,-DAY(GETDATE())+1,GETDATE())), 'yyyy-MM-dd 00:00:01') 
												AND FORMAT(DATEADD(DD,-DAY(GETDATE()),GETDATE()), 'yyyy-MM-dd 23:59:59')
	GROUP BY
		f.NomeFilial, tf.QtdTecnicos

--media mensal corretivo
INSERT INTO @tabela_dados

	SELECT f.NomeFilial AS nome_filial
		 , count(DISTINCT r.CodRAT) AS qtd_atend
		 , tf.QtdTecnicos AS qtd_tecnico
		 , ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23) as media
		 , dbo.FN_CONVMIN((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT))) as medi_temp_solu
		 , dbo.FN_CONVMIN(((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23))) as hora_tecn_util
		 , dbo.FN_CONVMIN(528 - ((SUM(datediff(minute, r.DataHoraInicio, r.DataHoraSolucao)) / count(DISTINCT r.CodRAT)) * ((cast(count(DISTINCT r.CodRAT) AS FLOAT) / tf.QtdTecnicos) / 23))) as horas_nao_usadas
		 , 'MENSAL - CORRETIVO' as periodo_tipo
	FROM
		filial f
		INNER JOIN OS o
			ON o.CodFilial = f.CodFilial
		INNER JOIN RAT r
			ON r.CodOS = o.codos
		INNER JOIN Tecnico t
			ON r.CodTecnico = t.CodTecnico
		LEFT JOIN Usuario u
			ON u.CodTecnico = t.CodTecnico
		INNER JOIN TecnicosFilial tf
			ON tf.CodFilial = f.CodFilial
	WHERE
		(1 = 1)
		AND f.IndAtivo IN (1)
		AND	o.CodTipoIntervencao IN (2)
		AND u.CodPerfil IN (35)
		AND t.codtecnico NOT IN (1763, 1382)
		AND o.CodEquip NOT IN (85, 107, 134, 147, 157, 158, 172, 204, 268)
		AND r.DataHoraSolucao BETWEEN FORMAT(DATEADD(MM,-1,DATEADD(DD,-DAY(GETDATE())+1,GETDATE())), 'yyyy-MM-dd 00:00:01') 
												AND FORMAT(DATEADD(DD,-DAY(GETDATE()),GETDATE()), 'yyyy-MM-dd 23:59:59')
	GROUP BY
		f.NomeFilial, tf.QtdTecnicos

---------------------------------------------------------------------------------------------------------------------------

SELECT *
FROM
	@tabela_dados
WHERE
	(1=1)
	--AND nome_filial = 'FSC'
ORDER BY
	nome_filial ASC, periodo ASC</CommandText>
