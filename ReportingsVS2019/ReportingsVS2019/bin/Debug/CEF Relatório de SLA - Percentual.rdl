    <DataSet Name="DsMes">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>WITH Meses AS
(
  SELECT 1 AS IdMes
  UNION ALL
  SELECT IdMes + 1
  FROM Meses
  WHERE IdMes &lt; 12
)
SELECT * FROM Meses</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="IdMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>IdMes</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DsAno">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>WITH Anos AS
(
  SELECT 2008 AS IdAno
  UNION ALL
  SELECT IdAno + 1
  FROM Anos
  WHERE IdAno &lt; 2012
)
SELECT * FROM Anos</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="IdAno">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>IdAno</DataField>
        </Field>
      </Fields>
    </DataSet>
