  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Data">
            <Value>=Parameters!Data.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodCliente">
            <Value>=Parameters!CodCliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodContrato">
            <Value>=Parameters!CodContrato.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodFilial">
            <Value>=Parameters!CodFilial.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT			Cliente.NomeFantasia AS Cliente, Contrato.NomeContrato AS Contrato, Filial.NomeFilial AS Filial,
				Equipamento.NomeEquip AS Modelo, (SUM(MTBF) / COUNT(MTBF)) AS MTBF
FROM			
				(
					SELECT			CodCliente, CodContrato, CodTipoEquip, CodGrupoEquip, CodEquip, CodFilial, CodEquipContrato,
									(CASE WHEN DATEDIFF(DD, EquipamentoContrato.DataAtivacao, GETDATE()) &gt;= DATEDIFF(DD, @Data, GETDATE()) 
											THEN 
												DATEDIFF(DD, @Data, GETDATE())
											ELSE
												DATEDIFF(DD, EquipamentoContrato.DataAtivacao, GETDATE())
									END) AS QtdDiaInstalado,
									(SELECT     COUNT(CodOS) AS countOSGeral
									FROM        dbo.OS AS OSG
									WHERE		(CodEquipContrato = dbo.EquipamentoContrato.CodEquipContrato)
									AND			(CodTipoIntervencao NOT IN (4))
									AND			(CodStatusServico NOT IN (2))
									AND			(DataHoraAberturaOS &gt;= @Data)
									) AS QtdOSGeral,
									(
										(
											CASE WHEN DATEDIFF(DD, EquipamentoContrato.DataAtivacao, GETDATE()) &gt;= DATEDIFF(DD, @Data, GETDATE()) 
													THEN 
														DATEDIFF(DD, @Data, GETDATE())
													ELSE
														DATEDIFF(DD, EquipamentoContrato.DataAtivacao, GETDATE())
											END
										)
										/
										(
											CASE WHEN 
														(
															SELECT     COUNT(CodOS) AS countOSGeral
															FROM        dbo.OS AS OSG
															WHERE		(CodEquipContrato = dbo.EquipamentoContrato.CodEquipContrato)
															AND			(CodTipoIntervencao NOT IN (4))
															AND			(CodStatusServico NOT IN (2))
															AND			(DataHoraAberturaOS &gt;= @Data)
														) = 0 
													THEN 1 
													ELSE 
														(
															SELECT     COUNT(CodOS) AS countOSGeral
															FROM        dbo.OS AS OSG
															WHERE		(CodEquipContrato = dbo.EquipamentoContrato.CodEquipContrato)
															AND			(CodTipoIntervencao NOT IN (4))
															AND			(CodStatusServico NOT IN (2))
															AND			(DataHoraAberturaOS &gt;= @Data)
														)
											END				
										)
									) AS MTBF
					FROM		dbo.EquipamentoContrato
					WHERE		(DataAtivacao IS NOT NULL)
					AND			(IndAtivo = 1)
					AND			(CodCliente IN (@CodCliente))
					AND			(CodContrato IN (@CodContrato))
					AND			(CodFilial IN (@CodFilial))
				) AS Dados
INNER JOIN		EquipamentoContrato ON Dados.CodEquipContrato = EquipamentoContrato.CodEquipContrato
INNER JOIN		Equipamento ON Dados.CodEquip = Equipamento.CodEquip
INNER JOIN		Cliente ON Dados.CodCliente = Cliente.CodCliente
INNER JOIN		Contrato ON Dados.CodContrato = Contrato.CodContrato
INNER JOIN		Filial ON Dados.CodFilial = Filial.CodFilial
GROUP BY		Cliente.NomeFantasia, Contrato.NomeContrato, Filial.NomeFilial, Equipamento.NomeEquip
ORDER BY		Cliente.NomeFantasia, Contrato.NomeContrato, Filial.NomeFilial, Equipamento.NomeEquip</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
