  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		Autorizada.NomeFantasia, EQ.NomeEquip, Parque_Resumo.ValorDespesa, COUNT(Parque_Resumo.CodEquip) AS QtdEquiptos, 
		(Parque_Resumo.ValorDespesa * COUNT(Parque_Resumo.CodEquip)) AS Repasse
FROM		Parque_Resumo
INNER JOIN	Autorizada ON Parque_Resumo.CodAutorizada = Autorizada.CodAutorizada
INNER JOIN	Equipamento EQ ON Parque_Resumo.CodEquip = EQ.CodEquip AND Parque_Resumo.CodTipoEquip = EQ.CodTipoEquip
WHERE		(1 = 1)
AND		(Parque_Resumo.AnoMes IN (@AnoMes))
AND		(Parque_Resumo.IndAtivo IN (@IndAtivo))
AND		(Parque_Resumo.IndRepasse IN (@IndRepasse))
AND		(Parque_Resumo.CodAutorizada IN (@CodAutorizada))
AND		(Parque_Resumo.CodEquip IN (@CodEquip))
AND		(Parque_Resumo.ValorDespesa IN (@ValorDespesa))
GROUP BY	Autorizada.NomeFantasia, EQ.NomeEquip, Parque_Resumo.ValorDespesa
ORDER BY	Autorizada.NomeFantasia, EQ.NomeEquip</CommandText>
        <QueryParameters>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndRepasse">
            <Value>=Parameters!IndRepasse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodAutorizada">
            <Value>=Parameters!CodAutorizada.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodEquip">
            <Value>=Parameters!CodEquip.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ValorDespesa">
            <Value>=Parameters!ValorDespesa.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
