  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		year(Data) AS Ano, Data, FORMAT(Data,'dd/MM/yyyy', 'pt-BR') AS Data2, SUM(Abertos) AS Abertos, SUM(Fechados) AS Fechados
FROM		(
					(
						SELECT			CAST(OS.DataHoraAberturaOS AS date) AS Data,			
										COUNT(Distinct OS.CodOS) AS Abertos,
										0 AS Fechados
						FROM			OS
						INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
						INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente
						WHERE			OS.CodStatusServico &lt;&gt; 2
						AND				OS.CodTipoIntervencao = 2
						AND				OS.CodFilial NOT IN (7, 21)
						AND				OS.CodEquip NOT IN
										(
												SELECT		CodEquip 
												FROM		Equipamento 
												WHERE		NomeEquip LIKE 'POS%'
												OR			NomeEquip LIKE 'PERTOS%'
										)
						--AND				Cliente.IndAtivo = 1
						AND				EquipamentoContrato.IndAtivo = 1
						AND				OS.DataHoraAberturaOS &gt;= CAST((CAST((CAST((GETDATE() - 11) AS DATE)) AS VARCHAR) + ' 00:00:00.000') AS DATETIME)--'2019-10-01 00:00:00.000'
						AND				OS.DataHoraAberturaOS &lt;= CAST(CAST(GETDATE() AS DATE) AS DATETIME) 
						AND				DATEPART(DW, OS.DataHoraAberturaOS) NOT IN (1,7)
						AND				(SELECT dbo.fc_CalculaDiasNaoUteis(CAST(OS.DataHoraAberturaOS AS date), CAST(OS.DataHoraAberturaOS AS date), 0, 0, 0, 1, null, null)) = 0
						GROUP BY		CAST(OS.DataHoraAberturaOS AS date) 
					)
					UNION 
					(
						SELECT			CAST(OS.DataHoraFechamento AS date) AS Data,			
										0 AS Abertos,
										COUNT(Distinct OS.CodOS) AS Fechados
						FROM			OS
						INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
						INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
						WHERE			OS.CodStatusServico &lt;&gt; 2
						AND				OS.CodTipoIntervencao = 2
						AND				OS.CodFilial NOT IN (7, 21)
						AND				OS.CodEquip NOT IN
										(
												SELECT		CodEquip 
												FROM		Equipamento 
												WHERE		NomeEquip LIKE 'POS%'
												OR			NomeEquip LIKE 'PERTOS%'
										)
						--AND				Cliente.IndAtivo = 1
						AND				EquipamentoContrato.IndAtivo = 1
						AND				OS.DataHoraFechamento &gt;= CAST((CAST((CAST((GETDATE() - 11) AS DATE)) AS VARCHAR) + ' 00:00:00.000') AS DATETIME)--'2019-10-01 00:00:00.000'
						AND				OS.DataHoraFechamento &lt;= CAST(CAST(GETDATE() AS DATE) AS DATETIME) 
						AND				DATEPART(DW, OS.DataHoraFechamento) NOT IN (1,7)
						AND				(SELECT dbo.fc_CalculaDiasNaoUteis(CAST(OS.DataHoraFechamento AS date), CAST(OS.DataHoraFechamento AS date), 0, 0, 0, 1, null, null)) = 0
						GROUP BY		CAST(OS.DataHoraFechamento AS date) 
					)
			) AS DADOS
GROUP BY	year(Data), Data
ORDER BY	year(Data), Data</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
