				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao), Filial.NomeFilial 
				UNION				
				SELECT			' PARQUE ATM' AS Tipo, Filial.NomeFilial AS Filial, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select * from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896'))
				AND				Parque_Resumo.CodGrupoEquip = 84
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
				UNION
				SELECT			' PARQUE OUTROS EQUIPAMENTOS' AS Tipo, Filial.NomeFilial AS Filial, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select CodEquip from equipamento where nomeequip in ('HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955'))
				AND				Parque_Resumo.CodGrupoEquip &lt;&gt; 84
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
				UNION
				SELECT			'TOTAL DO PARQUE' AS Tipo, Filial.NomeFilial AS Filial, Parque_Resumo.AnoMes AS Mes, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS QTD
				FROM			Parque_Resumo
				INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
				WHERE			Parque_Resumo.AnoMes BETWEEN '202101' and '202109'				
				AND				Parque_Resumo.CodCliente = 1
				AND				Parque_Resumo.IndGarantia = 0
				AND				Parque_Resumo.CodContrato = 3145
				AND				Parque_Resumo.IndAtivo = 1
				--AND				Parque_Resumo.CodEquip IN (select CodEquip from equipamento where nomeequip in ('ATMFFT - 290.01.444','TAADC - 290.01.712','TAADC - 290.01.789','TAALV - 290.01.727','TAAMC - 290.01.711','TAAMC - 290.01.782','TAAMC - 290.02.049','TAAMT - 290.01.739','TAART - 290.02.309','TAASF - 290.01.536','TAASF - 290.02.047','TAASF - 290.10.896','HSL 5000 TESOURARIA - 290.01.859','HSL 5100 TESOURARIA - 290.10.155','TCC 2100 - 290.01.858','TCC 2100 - 290.02.251','TDS 2100 - 290.01.857','TDS 2100 - 290.02.250','TOP - 290.01.955'))
				GROUP BY		Parque_Resumo.AnoMes, Filial.NomeFilial 
