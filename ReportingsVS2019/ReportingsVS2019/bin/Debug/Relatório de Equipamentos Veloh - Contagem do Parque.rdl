  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>(

SELECT 'Equipamentos Ativos'  AS 'Descrição'
	  , count(*) as 'Quantidade'
 FROM
	 SAT.dbo.EquipamentoContrato
	 INNER JOIN SAT.dbo.OS
		 ON SAT.dbo.EquipamentoContrato.CodEquipContrato = SAT.dbo.OS.CodEquipContrato
	
 WHERE
	 (1 = 1)
	 AND SAT.dbo.EquipamentoContrato.CodEquip = 172
	 AND SAT.dbo.EquipamentoContrato.IndAtivo = 1
													   
)

UNION

(

SELECT 'Equipamentos Em Circulação / Inativos (Inativos e na Smart)' AS 'Descrição'
	  , count(*) as 'Quantidade'
 FROM
	 SAT.dbo.EquipamentoContrato
	 INNER JOIN SAT.dbo.OS
		 ON SAT.dbo.EquipamentoContrato.CodEquipContrato = SAT.dbo.OS.CodEquipContrato
 WHERE
	 (1 = 1)
	 AND SAT.dbo.EquipamentoContrato.CodEquip = 172
	 AND SAT.dbo.EquipamentoContrato.IndAtivo = 0
	 AND EquipamentoContrato.CodPosto IN (333621)
) 

UNION

(

SELECT 'Equipamentos Excluídos (via Integração Banrisul)' AS 'Descrição'
	  , count(*) as 'Quantidade'
 FROM
	 SAT.dbo.EquipamentoContrato
	 INNER JOIN SAT.dbo.OS
		 ON SAT.dbo.EquipamentoContrato.CodEquipContrato = SAT.dbo.OS.CodEquipContrato
 WHERE
	 (1 = 1)
	 AND SAT.dbo.EquipamentoContrato.CodEquip = 172
	 AND SAT.dbo.OS.IndExclusaoBanrisul IS NOT NULL
	 AND SAT.dbo.OS.DataExclusaoBanrisul IS NOT NULL

)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
