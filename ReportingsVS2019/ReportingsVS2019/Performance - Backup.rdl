        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			AnoMes, Equipamento.NomeEquip AS Modelo, COUNT(DISTINCT EquipamentoContrato.CodEquipContrato) AS Qtd
FROM			Parque_Resumo
INNER JOIN		EquipamentoContrato ON Parque_Resumo.CodEquipContrato = EquipamentoContrato.CodEquipContrato
INNER JOIN		Cliente ON Parque_Resumo.CodCliente = Cliente.CodCliente
INNER JOIN		Filial ON Parque_Resumo.CodFilial = Filial.CodFilial
INNER JOIN		Equipamento ON Parque_Resumo.CodEquip = Equipamento.CodEquip
WHERE			Parque_Resumo.AnoMes IN (@AnoMes)
AND				Parque_Resumo.IndAtivo = 1
AND				Parque_Resumo.CodCliente = @CodCliente
AND				Parque_Resumo.codequipcontrato IN ( SELECT codequipcontrato
													FROM
														dbo.vwc_equipamentocontratocomsla
													WHERE
														codcliente = @CodCliente)
GROUP BY		AnoMes, Equipamento.NomeEquip</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodCliente">
            <Value>=Parameters!CodCliente.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
