  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Cliente.NomeFantasia AS Cliente, 
				COUNT(Distinct OS.CodOS) AS Qtd,
				(
					SELECT			COUNT(Distinct OS.CodOS) AS Qtd
					FROM			OS
					INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
					INNER JOIN		Cliente C ON OS.CodCliente = C.CodCliente	
					INNER JOIN		Filial F ON OS.CodFilial = F.CodFilial
					WHERE			(1 = 1)
					AND				OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodFilial NOT IN (7, 21)
					AND				OS.CodEquip NOT IN
									(
											SELECT		CodEquip 
											FROM		Equipamento 
											WHERE		NomeEquip LIKE 'POS%'
											OR			NomeEquip LIKE 'PERTOS%'
									)
					AND				C.IndAtivo = 1
					AND				C.CodCliente = Cliente.CodCliente
				) Backlog
FROM			OS
INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
WHERE			(1 = 1)
AND				OS.CodStatusServico &lt;&gt; 2
AND				OS.CodTipoIntervencao = 2
AND				OS.CodFilial NOT IN (7, 21)
AND				OS.CodEquip NOT IN
				(
						SELECT		CodEquip 
						FROM		Equipamento 
						WHERE		NomeEquip LIKE 'POS%'
						OR			NomeEquip LIKE 'PERTOS%'
				)
AND				Cliente.IndAtivo = 1
AND				OS.DataHoraAberturaOS &gt;= CAST(CAST(GETDATE()-1 AS DATE) AS DATETIME) 
AND				OS.DataHoraAberturaOS &lt;= CAST(CAST(GETDATE() AS DATE) AS DATETIME) 
GROUP BY		Cliente.NomeFantasia, Cliente.CodCliente
ORDER BY		COUNT(Distinct OS.CodOS) desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
