      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			Indice, ANOMES , CLIENTE, NROCONTRATO, NOMECONTRATO, VIGENTES
FROM			(
						SELECT			1 AS Indice, * FROM SlideContrato_Detalhamento 
						UNION 
						SELECT			2 AS Indice, AnoMes, 'TOTAL' AS Cliente, '' AS NroContrato, '' AS NomeContrato , SUM(Vigentes) AS Vigentes, SUM(Renovados) AS Renovados,  
										SUM(Reajustados) AS Reajustados, SUM(NaoReajustados) AS NaoReajustados, SUM(EmNegociacao) AS EmNegociacao, SUM(Declinados) AS Declinados 
						FROM			SlideContrato_Detalhamento 
						GROUP BY		AnoMes 
				) AS DADOS 
ORDER BY		1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
