        <CommandText>SELECT CodAutorizada, NomeFantasia FROM Autorizada ORDER BY NomeFantasia</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsAnoMesCorrente">
