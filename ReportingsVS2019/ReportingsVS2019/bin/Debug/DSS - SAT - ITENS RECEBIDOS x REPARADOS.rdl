  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Tipo">
            <Value>=Parameters!Tipo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Periodo">
            <Value>=Parameters!Periodo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ano">
            <Value>=Parameters!Ano.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Tipo int
--DECLARE @Periodo varchar(20)
--DECLARE @Ano INT
DECLARE @Report varchar(10)
--SET @Periodo = 'maio'
--SET @Ano = 2018
--SET @Tipo = 1

IF @Tipo = 1
	SET @Report = '22,36'
IF @Tipo = 2
	SET @Report = '1,2'




DECLARE @Data datetime
             SET @Data = CAST(YEAR(DATEADD(M, -3, GETDATE())) AS VARCHAR) + '-' + CAST(MONTH(DATEADD(M, -3, GETDATE())) AS VARCHAR) + '-01 00:00:00'  

SELECT		CodOR
			,dados.mes
			,dados.ano
			, dados.mesAtual
			, UPPER( NomeUsuario ) 'Técnico Reparador'
			, Peca.CodMagnus,Peca.NomePeca
			
			,UPPER( ORItem.DefeitoRelatado) DefeitoRelatado
			,UPPER( ORItem.RelatoSolucao) RelatoSolucao
			, FORMAT(dados.DataHoraInicio, 'dd/MM/yyyy HH:mm:ss','pt-BR') DataHoraInicio
			, FORMAT(dados.DataHoraFim, 'dd/MM/yyyy HH:mm:ss','pt-BR') DataHoraFim 
				,ISNULL(((select dbo.FN_CONVMIN(SUM(DATEDIFF(mi,ORTempoReparo.DataHoraInicio, ORTempoReparo.DataHoraFim)))
										FROM ORTempoReparo WHERE CodORItem IN (dados.CodORItem))),'') as 'Tempo de Reparo do Item',
							ISNULL(((select dbo.FN_CONVMIN(SUM(DATEDIFF(mi,ORTempoReparo.DataHoraInicio, ORTempoReparo.DataHoraFim)))
										FROM ORTempoReparo WHERE CodORItem IN (dados.CodORItem))),'') as 'Tempo de Reparo do Técnico'
							,ISNULL(((select dbo.FN_CONVMIN(SUM(dbo.fc_CalculaHorasUteis(ORTempoReparo.DataHoraInicio, ORTempoReparo.DataHoraFim)))
										FROM ORTempoReparo WHERE CodORItem IN (dados.CodORItem))),'') as 'Tempo de Reparo Uteis do Técnico'	
			,FORMAT( ORItem.DataConfLog , 'dd/MM/yyyy HH:mm:ss','pt-BR') DataConfLog
			,FORMAT( ORItem.DataConfLab, 'dd/MM/yyyy HH:mm:ss','pt-BR') DataConfLab
			,oritem.NumSerie			
		
FROM		(SELECT 
             MONTH(DataHoraFim) mes, YEAR(DataHoraFim) ano,
             				CASE WHEN MONTH(DataHoraFim) = 1 THEN 'Janeiro' 
             					 WHEN MONTH(DataHoraFim) = 2 THEN 'Fevereiro' 
             					 WHEN MONTH(DataHoraFim) = 3 THEN 'Março' 
             					 WHEN MONTH(DataHoraFim) = 4 THEN 'Abril' 
             					 WHEN MONTH(DataHoraFim) = 5 THEN 'Maio' 
             					 WHEN MONTH(DataHoraFim) = 6 THEN 'Junho' 
             					 WHEN MONTH(DataHoraFim) = 7 THEN 'Julho'
             					 WHEN MONTH(DataHoraFim) = 8 THEN 'Agosto'
             					 WHEN MONTH(DataHoraFim) = 9 THEN 'Setembro'
             					 WHEN MONTH(DataHoraFim) = 10 THEN 'Outubro'
             					 WHEN MONTH(DataHoraFim) = 11 THEN 'Novembro'
             					 WHEN MONTH(DataHoraFim) = 12 THEN 'Dezembro'
             				ELSE ''
             				END as mesAtual
			 ,* 
             FROM ORTempoReparo 
             where DataHoraFim BETWEEN @Data AND GETDATE() AND CodORItem IN (SELECT CodORItem FROM ORItem WHERE CodstatusOR IN (select * from dbo.fc_Split(@Report,',')))
             ) as dados
			 INNER JOIN Usuario ON CodUsuario = dados.CodTecnico
			 INNER JOIN ORItem ON dados.CodORItem = ORItem.CodORItem
			 LEFT JOIN Peca ON Peca.CodPeca = ORItem.CodPeca
			 where dados.mesAtual IN (@Periodo)
AND dados.ano IN (@Ano)</CommandText>
        <Timeout>18</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
