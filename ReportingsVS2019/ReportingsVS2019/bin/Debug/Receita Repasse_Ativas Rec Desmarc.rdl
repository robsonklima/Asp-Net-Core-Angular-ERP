    <DataSet Name="dsDataRelatorio">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT		CASE WHEN (CAST(MONTH(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS INTEGER) &lt; 10) 
			THEN
			'0' +
			CAST(CAST(MONTH(DATEADD(DAY,0,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR)
			+'/'+
			CAST(YEAR(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR) AS VARCHAR)
			ELSE
			CAST(CAST(MONTH(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR)
			+'/'+
			CAST(YEAR(DATEADD(DAY,-1,(DATEADD(MONTH,0,CONVERT(CHAR(08),GETDATE(),126)+'01')))) AS VARCHAR) AS VARCHAR)
			END AS DataFinal</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="DataFinal">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>DataFinal</DataField>
        </Field>
      </Fields>
    </DataSet>
