  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>--qtd técnicos ativos
--qtd clientes atendidos, (cliente nao contrato) por uf somente com contratos e eqpt ativos -pos
--qtd pontos atendidos, locais ativos e com equipamentos -pos
--qtd total equipamentos parque ativo
--qtd chamados, somente corretivos, 01-31 janeiro 2018

SELECT		CASE UF.SiglaUF 
					WHEN 'DF' THEN 'CENTRO - OESTE'
					WHEN 'GO' THEN 'CENTRO - OESTE'
					WHEN 'MT' THEN 'CENTRO - OESTE'
					WHEN 'MS' THEN 'CENTRO - OESTE'
					WHEN 'AC' THEN 'NORTE'
					WHEN 'AP' THEN 'NORTE'
					WHEN 'AM' THEN 'NORTE'
					WHEN 'MA' THEN 'NORTE'
					WHEN 'PA' THEN 'NORTE'
					WHEN 'PI' THEN 'NORTE'
					WHEN 'RO' THEN 'NORTE'
					WHEN 'RR' THEN 'NORTE'
					WHEN 'TO' THEN 'NORTE'
					WHEN 'AL' THEN 'NORDESTE'
					WHEN 'BA' THEN 'NORDESTE'
					WHEN 'CE' THEN 'NORDESTE'
					WHEN 'PB' THEN 'NORDESTE'
					WHEN 'PE' THEN 'NORDESTE'
					WHEN 'RN' THEN 'NORDESTE'
					WHEN 'SE' THEN 'NORDESTE'
					WHEN 'PR' THEN 'SUL'
					WHEN 'RS' THEN 'SUL'
					WHEN 'SC' THEN 'SUL'
					WHEN 'ES' THEN 'SUDESTE'
					WHEN 'MG' THEN 'SUDESTE'
					WHEN 'RJ' THEN 'SUDESTE'
					WHEN 'SP' THEN 'SUDESTE'				
					ELSE ''
			END 'Região',
			UF.SiglaUF AS UF,
			(		
				SELECT		COUNT(DISTINCT Tecnico.CodTecnico)
				FROM		Tecnico 
				INNER JOIN	Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
				INNER JOIN	Filial ON Tecnico.CodFilial = Filial.CodFilial
				INNER JOIN	Cidade ON Filial.CodCidade = Cidade.CodCidade
				WHERE		Tecnico.IndAtivo = 1 
				AND			Usuario.CodPerfil = 35
				AND			Cidade.CodUF = UF.CodUF
			) 'Qtde de Técnicos',
			(
				SELECT		COUNT(DISTINCT EquipamentoContrato.CodCliente)
				FROM		EquipamentoContrato				
				INNER JOIN	LocalAtendimento ON EquipamentoContrato.CodPosto = LocalAtendimento.CodPosto
				INNER JOIN	Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
				WHERE		EquipamentoContrato.CodFilial NOT IN (7, 21)
				AND			EquipamentoContrato.IndAtivo = 1
				AND			EquipamentoContrato.CodCliente &lt;&gt; 220
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND			Cidade.CodUF = UF.CodUF
			) 'Qtde de clientes atendidos',
			(
				SELECT		COUNT(DISTINCT EquipamentoContrato.CodPosto)
				FROM		EquipamentoContrato				
				INNER JOIN	LocalAtendimento ON EquipamentoContrato.CodPosto = LocalAtendimento.CodPosto
				INNER JOIN	Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
				WHERE		EquipamentoContrato.CodFilial NOT IN (7, 21)
				AND			EquipamentoContrato.IndAtivo = 1
				AND			EquipamentoContrato.CodCliente &lt;&gt; 220
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND			Cidade.CodUF = UF.CodUF			
			) 'Qtde de pontos atendidos (endereço físico)',
			(
				SELECT		COUNT(DISTINCT EquipamentoContrato.CodEquipContrato)
				FROM		EquipamentoContrato				
				INNER JOIN	LocalAtendimento ON EquipamentoContrato.CodPosto = LocalAtendimento.CodPosto
				INNER JOIN	Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
				WHERE		EquipamentoContrato.CodFilial NOT IN (7, 21)
				AND			EquipamentoContrato.IndAtivo = 1
				AND			EquipamentoContrato.CodCliente &lt;&gt; 220
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND			Cidade.CodUF = UF.CodUF			
			) 'Qtde total de equipamentos',
			(
				SELECT		COUNT(DISTINCT OS.CodOS)
				FROM		OS
				INNER JOIN	EquipamentoContrato	ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN	LocalAtendimento ON EquipamentoContrato.CodPosto = LocalAtendimento.CodPosto
				INNER JOIN	Cidade ON LocalAtendimento.CodCidade = Cidade.CodCidade
				WHERE		EquipamentoContrato.CodFilial NOT IN (7, 21)
				--AND			EquipamentoContrato.IndAtivo = 1
				AND			EquipamentoContrato.CodCliente &lt;&gt; 220
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
				AND			EquipamentoContrato.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
				AND			OS.CodTipoIntervencao = 2
				AND			OS.DataHoraAberturaOS BETWEEN '2018-01-01 00:00:00.000' AND '2018-02-01 00:00:00.000'
				AND			Cidade.CodUF = UF.CodUF						
			) 'Qtde total de ocorrências'
FROM		UF
WHERE		UF.IndAtivo = 1
AND			UF.CodPais = 1
ORDER BY	1,2</CommandText>
