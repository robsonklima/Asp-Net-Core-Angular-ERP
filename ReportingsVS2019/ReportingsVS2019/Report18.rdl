  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>DECLARE			@DataFiltro DATETIME
DECLARE			@DataFiltro3 DATETIME
SET				@DataFiltro = (
									SELECT		CAST((CAST(MONTH(DATEADD(MONTH, -12, GETDATE())) AS VARCHAR)
												+'/'+
												'01'
												+'/'+
												CAST(YEAR(DATEADD(MONTH, -12, GETDATE())) AS VARCHAR)) AS DATETIME)
							   )
SET				@DataFiltro3 = (
									SELECT		CAST((CAST(MONTH(DATEADD(MONTH, 0, GETDATE())) AS VARCHAR)
												+'/'+
												'01'
												+'/'+
												CAST(YEAR(DATEADD(MONTH, 0, GETDATE())) AS VARCHAR)) AS DATETIME)
								)

(
SELECT			NomeFilial AS Filial,
				AnoMes, 
				SUM(ChamadosMesPecasPendentes) AS ChamadosPendentes,
				SUM(ChamadosMes) AS Chamados,
				CAST((
					  (
								CAST((
										CAST(SUM(ChamadosMesPecasPendentes)AS FLOAT)
										/
										CAST(SUM(ChamadosMes)AS FLOAT)
									)AS FLOAT)
									* 
									100.00
							)
				) AS DECIMAL(10,1)
				) AS SLA
FROM 			(
					SELECT	 		Filial.NomeFilial,
									(	
										CAST(YEAR(Atendimentos.DataHoraAberturaOS) AS VARCHAR) +
										CAST((DBO.INSERTZERO(MONTH(Atendimentos.DataHoraAberturaOS),2))AS VARCHAR)
									) AS AnoMes,
									ISNULL((
										SELECT		COUNT(*)
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
									),0) AS ChamadosMes,
									ISNULL((
										SELECT		COUNT(*)
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			RAT.CodStatusServico = 7
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
									),0) AS ChamadosMesPecasPendentes
					FROM			OS AS Atendimentos
					INNER JOIN		Filial ON Atendimentos.CodFilial = Filial.CodFilial
					WHERE			(1=1)
					AND				Atendimentos.CodFilial NOT IN (3, 7, 21)
					AND				Atendimentos.CodStatusServico NOT IN (2)
					AND				(
										SELECT		TOP 1 RAT.CodTecnico
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
										ORDER BY	RAT.CodRAT DESC
									) IN (SELECT CodTecnico FROM Tecnico WHERE IndAtivo = 1)
				) AS DADOS
GROUP BY		AnoMes,NomeFilial
)
UNION
(
SELECT			'Média Brasil' AS Filial,
				AnoMes, 
				SUM(ChamadosMesPecasPendentes) AS ChamadosPendentes,
				SUM(ChamadosMes) AS Chamados,
				CAST((
					  (
								CAST((
										CAST(SUM(ChamadosMesPecasPendentes)AS FLOAT)
										/
										CAST(SUM(ChamadosMes)AS FLOAT)
									)AS FLOAT)
									* 
									100.00
							)
				) AS DECIMAL(10,1)
				) AS SLA
FROM 			(
					SELECT	 		Filial.NomeFilial,
									(	
										CAST(YEAR(Atendimentos.DataHoraAberturaOS) AS VARCHAR) +
										CAST((DBO.INSERTZERO(MONTH(Atendimentos.DataHoraAberturaOS),2))AS VARCHAR)
									) AS AnoMes,
									ISNULL((
										SELECT		COUNT(*)
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
									),0) AS ChamadosMes,
									ISNULL((
										SELECT		COUNT(*)
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			RAT.CodStatusServico = 7
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
									),0) AS ChamadosMesPecasPendentes
					FROM			OS AS Atendimentos
					INNER JOIN		Filial ON Atendimentos.CodFilial = Filial.CodFilial
					WHERE			(1=1)
					AND				Atendimentos.CodFilial NOT IN (3, 7, 21)
					AND				Atendimentos.CodStatusServico NOT IN (2)
					AND				(
										SELECT		TOP 1 RAT.CodTecnico
										FROM		RAT
										INNER JOIN	OS ON OS.CodOS = RAT.CodOS
										WHERE		RAT.CodStatusServico NOT IN (2)
										AND			RAT.CodOS = Atendimentos.CodOS
										AND			(OS.CodAutorizada IN (select CodAutorizada from Autorizada where RazaoSocial like 'PERTO AT%' AND IndAtivo = 1))
										AND			(OS.DataHoraAberturaOS between (@DataFiltro) and (@DataFiltro3))  
										ORDER BY	RAT.CodRAT DESC
									) IN (SELECT CodTecnico FROM Tecnico WHERE IndAtivo = 1)
				) AS DADOS
GROUP BY		AnoMes
)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
