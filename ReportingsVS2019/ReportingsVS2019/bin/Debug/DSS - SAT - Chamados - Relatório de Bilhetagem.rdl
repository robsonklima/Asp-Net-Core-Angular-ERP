  <DataSources>
    <DataSource Name="SAT">
      <DataSourceReference>SAT</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8a37a9d0-5c02-4840-b6fd-44e983663000</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			NomeFantasia, Mes, SUM(Parque) AS Parque, SUM(ParqueGarantia) AS ParqueGarantia, SUM(QtdTotal) AS QtdTotal, SUM(QtdGarantia) AS QtdGarantia, SUM(QtdTotalCorretiva) AS QtdTotalCorretiva, 
				SUM(QtdGarantiaCorretiva) AS QtdGarantiaCorretiva, SUM(QtdTotalOutrasIntervencoes) AS QtdTotalOutrasIntervencoes, 
				SUM(QtdGarantiaOutrasIntervencoes) AS QtdGarantiaOutrasIntervencoes
FROM 			(
					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, COUNT(DISTINCT OS.CodOS) AS QtdTotal, 0 AS QtdGarantia, 
										0 AS QtdTotalCorretiva,	0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 0 AS Parque, 
										0 AS ParqueGarantia									
					FROM				OS
					INNER JOIN			EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 0 AS QtdTotal, COUNT(DISTINCT OS.CodOS) AS QtdGarantia, 
										0 AS QtdTotalCorretiva,	0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 0 AS Parque, 
										0 AS ParqueGarantia
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Parque_Resumo.IndGarantia = 1
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 
										COUNT(DISTINCT OS.CodOS) AS QtdTotalCorretiva, 0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 
										0 AS QtdGarantiaOutrasIntervencoes, 0 AS Parque, 0 AS ParqueGarantia
					FROM				OS
					INNER JOIN			EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					OS.CodTipoIntervencao = 2
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 0 AS QtdTotalCorretiva,
										COUNT(DISTINCT OS.CodOS) AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 0 AS Parque, 
										0 AS ParqueGarantia
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Parque_Resumo.IndGarantia = 1
					AND					OS.CodTipoIntervencao = 2
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 0 AS QtdTotalCorretiva,
										0 AS QtdGarantiaCorretiva, COUNT(DISTINCT OS.CodOS) AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 0 AS Parque,
										0 AS ParqueGarantia
					FROM				OS
					INNER JOIN			EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					OS.CodTipoIntervencao &lt;&gt; 2
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				Cliente.NomeFantasia, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 0 AS QtdTotalCorretiva,
										0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, COUNT(DISTINCT OS.CodOS) AS QtdGarantiaOutrasIntervencoes, 0 AS Parque,
										0 AS ParqueGarantia
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (166, 444, 256)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Parque_Resumo.IndGarantia = 1
					AND					OS.CodTipoIntervencao &lt;&gt; 2
					GROUP BY			Cliente.NomeFantasia, MONTH(OS.DataHoraAberturaOS)

					UNION 

					SELECT				Cliente.NomeFantasia, SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),5,2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 0 AS QtdTotalCorretiva,
										0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 
										COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS ParqueTotal, 0 AS ParqueGarantia
					FROM				Parque_Resumo 
					INNER JOIN			Cliente ON Parque_Resumo.CodCliente = Cliente.CodCliente
					WHERE				Parque_Resumo.CodCliente IN (166, 444, 256)
					AND					SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),1,4) = '2017'
					AND					Parque_Resumo.IndAtivo = 1
					GROUP BY			Cliente.NomeFantasia, Parque_Resumo.AnoMes

					UNION 

					SELECT				Cliente.NomeFantasia, SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),5,2) AS Mes, 0 AS QtdTotal, 0 AS QtdGarantia, 0 AS QtdTotalCorretiva,
										0 AS QtdGarantiaCorretiva, 0 AS QtdTotalOutrasIntervencoes, 0 AS QtdGarantiaOutrasIntervencoes, 
										0 AS ParqueTotal, COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS ParqueGarantia
					FROM				Parque_Resumo 
					INNER JOIN			Cliente ON Parque_Resumo.CodCliente = Cliente.CodCliente
					WHERE				Parque_Resumo.CodCliente IN (166, 444, 256)
					AND					SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),1,4) = '2017'
					AND					Parque_Resumo.IndAtivo = 1
					AND					Parque_Resumo.IndGarantia = 1
					GROUP BY			Cliente.NomeFantasia, Parque_Resumo.AnoMes
				) AS Dados
GROUP BY		NomeFantasia, Mes</CommandText>
