    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="CodMagnus">
          <DataField>CodMagnus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomePeca">
          <DataField>NomePeca</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomeFilial">
          <DataField>NomeFilial</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qtd">
          <DataField>Qtd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT			Peca.CodMagnus, Peca.NomePeca, NomeFilial, COUNT(ORItem.CodPeca) 'Qtd'
from			ORItem
INNER JOIN		Peca on Peca.CodPeca = ORItem.CodPeca
INNER JOIN		[OR] ON [OR].CodOR = ORItem.CodOR
LEFT JOIN		Filial ON CodFilial = [OR].CodOrigem
WHERE			ORItem.IndAtivo &gt; 0
AND				[OR].CodOrigem = 32
GROUP BY		Peca.CodMagnus, Peca.NomePeca,NomeFilial
ORDER BY		4 DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
