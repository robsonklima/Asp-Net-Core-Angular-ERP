  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Mes, Indice, Tipo,
				SUM(Qtde) AS Qtde
FROM 			(
					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 1 AS Indice, 'DSR Qtde de Chamados' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (197, 202, 76, 475, 166, 256, 444)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 2 AS Indice, 'GAR Qtde de Chamados' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Parque_Resumo.IndGarantia = 1
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 3 AS Indice, 'LOC Qtde de Chamados' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					INNER JOIN			Contrato ON Parque_Resumo.CodContrato = Contrato.CodContrato
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Contrato.CodTipoContrato = 4
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)	
					
					UNION
					
					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 4 AS Indice, 'Qtd de Total de Chamados (Todos clientes MAN, GAR E LOC)' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)																		


					UNION

					SELECT				SUBSTRING(CAST(Parque_Resumo.AnoMes AS VARCHAR),5,2) AS Mes, 5 AS Indice, 'GAR Parque de Máquinas Ativas' AS Tipo,
										COUNT(DISTINCT Parque_Resumo.CodEquipContrato) AS Qtde
					FROM				Parque_Resumo
					WHERE				Parque_Resumo.AnoMes BETWEEN '201701' AND '201712'
					AND					Parque_Resumo.IndAtivo = 1
					AND					Parque_Resumo.IndGarantia = 1
					AND					Parque_Resumo.CodFilial NOT IN (7, 21)
					AND					Parque_Resumo.CodCliente &lt;&gt; 220
					AND					Parque_Resumo.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					Parque_Resumo.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					Parque_Resumo.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			Parque_Resumo.AnoMes
				) AS Dados
GROUP BY		Mes, Indice, Tipo
ORDER BY		Mes, Indice</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Mes">
          <DataField>Mes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Indice">
          <DataField>Indice</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tipo">
          <DataField>Tipo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qtde">
          <DataField>Qtde</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Mes, Indice, Tipo,
				SUM(Qtde) AS Qtde
FROM 			(					
					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 4 AS Indice, 'DSR Qtde de Atendimentos' AS Tipo,
										COUNT(DISTINCT RAT.CodRAT) AS Qtde 
					FROM				OS
					INNER JOIN			RAT ON OS.CodOS = RAT.CodOS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodCliente IN (197, 202, 76, 475, 166, 256, 444)
					AND					OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 5 AS Indice, 'GAR Qtde de Atendimentos' AS Tipo,
										COUNT(DISTINCT RAT.CodRAT) AS Qtde 
					FROM				OS
					INNER JOIN			RAT ON OS.CodOS = RAT.CodOS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Parque_Resumo.IndGarantia = 1
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 6 AS Indice, 'LOC Qtde de Atendimentos' AS Tipo,
										COUNT(DISTINCT RAT.CodRAT) AS Qtde 
					FROM				OS
					INNER JOIN			RAT ON OS.CodOS = RAT.CodOS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					INNER JOIN			Contrato ON Parque_Resumo.CodContrato = Contrato.CodContrato
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					Contrato.CodTipoContrato = 4
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)				
					
					UNION 

					SELECT				DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 4 AS Indice, 'Qtd de Total de Atendimentos (Todos clientes MAN, GAR E LOC)' AS Tipo,
										COUNT(DISTINCT RAT.CodRAT) AS Qtde 
					FROM				OS
					INNER JOIN			RAT ON OS.CodOS = RAT.CodOS
					INNER JOIN			Parque_Resumo ON OS.CodEquipContrato = Parque_Resumo.CodEquipContrato
					INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
					WHERE				OS.CodStatusServico &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) = '2017'
					AND					Parque_Resumo.AnoMes = CAST(YEAR(OS.DataHoraAberturaOS) AS VARCHAR) + CAST(DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS VARCHAR)
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodCliente &lt;&gt; 220
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					--AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PIN%')
					GROUP BY			MONTH(OS.DataHoraAberturaOS)															
				) AS Dados
GROUP BY		Mes, Indice, Tipo
ORDER BY		Mes, Indice</CommandText>
      </Query>
      <Fields>
        <Field Name="Mes">
          <DataField>Mes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Indice">
          <DataField>Indice</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tipo">
          <DataField>Tipo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qtde">
          <DataField>Qtde</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
