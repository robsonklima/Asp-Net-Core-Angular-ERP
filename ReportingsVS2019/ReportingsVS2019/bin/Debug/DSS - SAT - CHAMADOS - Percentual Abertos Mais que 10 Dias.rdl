      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>WITH Anos AS
(
  SELECT 2018 AS IdAno
  UNION ALL
  SELECT IdAno + 1
  FROM Anos
  WHERE IdAno &lt; YEAR(GETDATE())
)
SELECT * FROM Anos
</CommandText>
