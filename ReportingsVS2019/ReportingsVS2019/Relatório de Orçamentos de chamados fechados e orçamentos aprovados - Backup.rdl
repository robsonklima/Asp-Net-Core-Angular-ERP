  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT OS.CodOS,
	   OS.NumOSCliente,
	   Orcamentos.NumOrcamento,
 	   Orcamentos.ValorTotalOrcamento
 FROM Orcamentos
INNER JOIN OS ON Orcamentos.CodOS = OS.CodOS
WHERE (1 = 1)
  AND Orcamentos.IndAtivo = 1
  AND OS.CodStatusServico = 3
  AND Orcamentos.CodOrcamentoStatus = 5
  AND Orcamentos.DataHoraCad BETWEEN CAST(DATEPART(YYYY,GETDATE()) AS VARCHAR) + '-' + CAST(DATEPART(MM,DATEADD(MM,-1,GETDATE())) AS VARCHAR) + '-01 00:00:00.000'
									 --'2010-12-01 00:00:00.000'
                                     AND 
									 --'2011-01-01 23:59:59.999'
									 CAST(DATEPART(YYYY,GETDATE()) AS VARCHAR) + '-' + CAST(DATEPART(MM,DATEADD(MM,0,GETDATE())) AS VARCHAR) + '-01 00:00:00.000'
ORDER BY Orcamentos.CodOrcamento ASC</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodOS">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CodOS</DataField>
        </Field>
        <Field Name="NumOSCliente">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NumOSCliente</DataField>
        </Field>
        <Field Name="NumOrcamento">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NumOrcamento</DataField>
        </Field>
        <Field Name="ValorTotalOrcamento">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ValorTotalOrcamento</DataField>
        </Field>
      </Fields>
    </DataSet>
