        </Field>
        <Field Name="PERC">
          <DataField>PERC</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DsFilial">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT 		CodFilial, NomeFilial
FROM		Filial
WHERE		IndAtivo = 1
AND		RazaoSocial LIKE 'PERTO%AT%'</CommandText>
      </Query>
      <Fields>
