												CAST(CAST(GETDATE() AS DATE) AS DATETIME)
									END
					GROUP BY		Cliente.NomeFantasia, Cliente.CodCliente
					--ORDER BY		COUNT(Distinct OS.CodOS) desc

					UNION

					SELECT			Cliente.NomeFantasia AS Cliente, 
									0 AS Qtd,
									COUNT(Distinct OS.CodOS) AS Backlog
					FROM			OS
					INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
					INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
					INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
					WHERE			(1 = 1)
					AND				OS.CodStatusServico NOT IN (2,3)
					AND				OS.CodTipoIntervencao = 2
					AND				OS.CodFilial NOT IN (7, 21)
					AND				OS.CodEquip NOT IN
									(
											SELECT		CodEquip 
											FROM		Equipamento 
											WHERE		NomeEquip LIKE 'POS%'
											OR			NomeEquip LIKE 'PERTOS%'
									)
					AND				Cliente.IndAtivo = 1
					AND				Cliente.CodCliente NOT IN 
									(
										SELECT			Distinct Cliente.CodCliente
										FROM			OS
										INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato		
										INNER JOIN		Cliente ON OS.CodCliente = Cliente.CodCliente	
										INNER JOIN		Filial ON OS.CodFilial = Filial.CodFilial
										WHERE			(1 = 1)
										AND				OS.CodStatusServico &lt;&gt; 2
										AND				OS.CodTipoIntervencao = 2
										AND				OS.CodFilial NOT IN (7, 21)
										AND				OS.CodEquip NOT IN
														(
																SELECT		CodEquip 
																FROM		Equipamento 
																WHERE		NomeEquip LIKE 'POS%'
																OR			NomeEquip LIKE 'PERTOS%'
														)
										AND				Cliente.IndAtivo = 1
										AND				OS.DataHoraAberturaOS &gt;= 
														CASE WHEN DATEPART(DW, GETDATE()) = 2
																THEN 
																	CAST(CAST(GETDATE()-3 AS DATE) AS DATETIME) 
																ELSE
																	CAST(CAST(GETDATE()-1 AS DATE) AS DATETIME)
														END
										AND				OS.DataHoraAberturaOS &lt;= 
														CASE WHEN DATEPART(DW, GETDATE()) = 2
																THEN 
																	CAST(CAST(GETDATE()-2 AS DATE) AS DATETIME) 
																ELSE
																	CAST(CAST(GETDATE() AS DATE) AS DATETIME)
														END
										GROUP BY		Cliente.NomeFantasia, Cliente.CodCliente
									)
					GROUP BY		Cliente.NomeFantasia, Cliente.CodCliente
				) AS Dados
order by		Qtd desc</CommandText>
      </Query>
