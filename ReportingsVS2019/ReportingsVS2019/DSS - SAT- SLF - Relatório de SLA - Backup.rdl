      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE		@MES VARCHAR(2)
DECLARE		@ANO VARCHAR(4) 
DECLARE		@AnoMes VARCHAR(6)

SET			@MES = DBO.INSERTZERO((12),2)
SET			@ANO = 2017

SET			@AnoMes = CASE WHEN DAY(GETDATE()) = 1 AND DBO.insertZero(MONTH(GETDATE()),2) = @MES AND YEAR(GETDATE()) = @ANO 
								THEN
											CAST(YEAR(DateAdd(MM, -1, GETDATE())) AS VARCHAR) + CAST(DBO.insertZero(MONTH(DateAdd(MM, -1, GETDATE())),2) AS VARCHAR)
								ELSE			
											@ANO + @MES
					  END

--DECLARE @Cliente INT
--SET @Cliente = 137

exec pr_Reporting_Indice_de_Solucao_Novo @AnoMes, @Cliente

SELECT		* 
FROM		(
				SELECT		'1' AS ColunaA, Filial.NomeFilial AS FILIAL
							 , sum(IndiceSolucaoNew.ChamadosMesStatusSLA) AS DENTRO
							 , (sum(IndiceSolucaoNew.ChamadosMes) - sum(IndiceSolucaoNew.ChamadosMesStatusSLA)) AS FORA
							 , sum(IndiceSolucaoNew.ChamadosMes) AS TotalGeral
							 , cast((cast(sum(IndiceSolucaoNew.ChamadosMesStatusSLA) AS FLOAT) / (CASE
								   WHEN (cast((sum(IndiceSolucaoNew.ChamadosMes)) AS FLOAT)) &lt; 1 THEN
									   1
								   ELSE
									   (cast((sum(IndiceSolucaoNew.ChamadosMes)) AS FLOAT))
							   END) * 100) AS DECIMAL(10, 2)) AS Percentual
				FROM		IndiceSolucaoNew 
				INNER JOIN	Filial ON IndiceSolucaoNew.CodFilial = Filial.CodFilial
				WHERE		IndiceSolucaoNew.AnoMes = @AnoMes
				--AND			IndiceSolucaoNew.CodCliente IN (SELECT DISTINCT(CodCliente) FROM EquipamentoContrato where CodEquip IN (233,301,302) AND IndAtivo = 1)
				AND			IndiceSolucaoNew.CodContrato IN (SELECT DISTINCT(CodContrato) FROM EquipamentoContrato where CodEquip IN (233,301,302) AND IndAtivo = 1)
				GROUP BY	Filial.NomeFilial
				UNION
				SELECT		'2' AS ColunaA, 'TOTAL' AS FILIAL
							 , sum(IndiceSolucaoNew.ChamadosMesStatusSLA) AS DENTRO
							 , (sum(IndiceSolucaoNew.ChamadosMes) - sum(IndiceSolucaoNew.ChamadosMesStatusSLA)) AS FORA
							 , sum(IndiceSolucaoNew.ChamadosMes) AS TotalGeral
							 , cast((cast(sum(IndiceSolucaoNew.ChamadosMesStatusSLA) AS FLOAT) / (CASE
								   WHEN (cast((sum(IndiceSolucaoNew.ChamadosMes)) AS FLOAT)) &lt; 1 THEN
									   1
								   ELSE
									   (cast((sum(IndiceSolucaoNew.ChamadosMes)) AS FLOAT))
							   END) * 100) AS DECIMAL(10, 2)) AS Percentual
				FROM		IndiceSolucaoNew 
				WHERE		IndiceSolucaoNew.AnoMes = @AnoMes
				--AND			IndiceSolucaoNew.CodCliente IN (SELECT DISTINCT(CodCliente) FROM EquipamentoContrato where CodEquip IN (233,301,302) AND IndAtivo = 1)
				AND			IndiceSolucaoNew.CodContrato IN (SELECT DISTINCT(CodContrato) FROM EquipamentoContrato where CodEquip IN (233,301,302) AND IndAtivo = 1)
			) AS Dados
WHERE		TotalGeral &gt; 0
ORDER BY	ColunaA, Percentual DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
