  <DataSources>
    <DataSource Name="SAT">
      <DataSourceReference>SAT</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>5dcd2644-7504-48be-9cc2-c6cadfc7fc9d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT		*
FROM		(
				SELECT			'GARANTIA' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2021-01-01 00:00:00.000'
				AND				RAT.DataHoraSolucao &lt; '2021-10-01 00:00:00.000'
				AND				(
									OS.CodCliente NOT IN (197, 256, 76, 166, 444, 425)
									AND 
									EquipamentoContrato.CodContrato NOT IN (3050,3651)
								)
				AND				OS.CodTipoIntervencao NOT IN (5, 17, 18, 19, 20)
				AND				Contrato.CodTipoContrato = 2
				AND				OS.CodFilial NOT IN (7, 21,33)
				AND				OS.CodCliente &lt;&gt; 220
				AND				OS.CodEquipContrato NOT IN
								(
									SELECT			CodEquipContrato 
									FROM			EquipamentoContrato 
									WHERE			CodCliente = 2	
									AND				CodEquip IN
													(
														SELECT		CodEquip 
														FROM		Equipamento 
														WHERE		NomeEquip LIKE 'POS%'
														OR			NomeEquip LIKE 'PERTOS%'
													)
								)
				GROUP BY		YEAR(RAT.DataHoraSolucao), MONTH(RAT.DataHoraSolucao)
				UNION
				SELECT			'DSR' AS Tipo, CAST(YEAR(RAT.DataHoraSolucao) AS VARCHAR) + '/' + CAST(DBO.INSERTZERO(MONTH(RAT.DataHoraSolucao),2) AS VARCHAR) AS Mes, COUNT(DISTINCT OS.CodOS) AS QTD
				FROM			RAT
				INNER JOIN		OS ON RAT.CodOS = OS.CodOS
				INNER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
				INNER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
				AND				OS.CodStatusServico &lt;&gt; 2
				AND				RAT.DataHoraSolucao &gt;= '2021-01-01 00:00:00.000'
