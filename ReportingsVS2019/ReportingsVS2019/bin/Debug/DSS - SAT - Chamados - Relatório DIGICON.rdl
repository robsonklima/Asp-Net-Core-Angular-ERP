  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>SELECT			Ano, Mes, Indice, Tipo,
				SUM(Qtde) AS Qtde
FROM 			(
					SELECT				YEAR(OS.DataHoraAberturaOS) AS Ano, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 1 AS Indice, 'CORRETIVA' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					WHERE				OS.CodCliente = 80
					AND					OS.CodStatusServico = 3
					AND					OS.CodTipoIntervencao = 2
					AND					YEAR(OS.DataHoraAberturaOS) IN ('2017','2018')
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					GROUP BY			YEAR(OS.DataHoraAberturaOS), MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				YEAR(OS.DataHoraAberturaOS) AS Ano, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 2 AS Indice, 'OUTRAS' AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					WHERE				OS.CodCliente = 80
					AND					OS.CodStatusServico = 3
					AND					OS.CodTipoIntervencao &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) IN ('2017','2018')
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					GROUP BY			YEAR(OS.DataHoraAberturaOS), MONTH(OS.DataHoraAberturaOS)

					UNION

					SELECT				YEAR(OS.DataHoraAberturaOS) AS Ano, DBO.insertZero(MONTH(OS.DataHoraAberturaOS),2) AS Mes, 3 AS Indice, TipoIntervencao.NomTipoIntervencao AS Tipo,
										COUNT(DISTINCT OS.CodOS) AS Qtde
					FROM				OS
					INNER JOIN			TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao
					WHERE				OS.CodCliente = 80
					AND					OS.CodStatusServico = 3
					AND					OS.CodTipoIntervencao &lt;&gt; 2
					AND					YEAR(OS.DataHoraAberturaOS) IN ('2017','2018')
					AND					OS.CodFilial NOT IN (7, 21)
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
					AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
					GROUP BY			YEAR(OS.DataHoraAberturaOS), MONTH(OS.DataHoraAberturaOS), TipoIntervencao.NomTipoIntervencao

					UNION

					SELECT			YEAR(Data) AS Ano, DBO.insertZero(MONTH(Data),2) AS Mes, 4 AS Indice, 'LOCAIS PREVENTIVAS' AS Tipo,
									COUNT(DISTINCT CodPosto) AS Qtde
					FROM			(
										SELECT				CAST(OS.DataHoraAberturaOS AS DATE) AS Data, OS.CodPosto							
										FROM				OS
										INNER JOIN			TipoIntervencao ON OS.CodTipoIntervencao = TipoIntervencao.CodTipoIntervencao				
										WHERE				OS.CodCliente = 80
										AND					OS.CodStatusServico = 3
										AND					OS.CodTipoIntervencao = 6
										AND					YEAR(OS.DataHoraAberturaOS) IN ('2017','2018')
										AND					OS.CodFilial NOT IN (7, 21)
										AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'POS%')
										AND					OS.CodEquip NOT IN (SELECT CodEquip FROM Equipamento WHERE NomeEquip LIKE 'PERTOS%')
										GROUP BY			CAST(OS.DataHoraAberturaOS AS DATE), OS.CodPosto
									) AS DADOS
					GROUP BY		YEAR(Data), DBO.insertZero(MONTH(Data),2)					
				) AS Dados
GROUP BY		Ano, Mes, Indice, Tipo
ORDER BY		Ano DESC, Mes DESC, Indice </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
