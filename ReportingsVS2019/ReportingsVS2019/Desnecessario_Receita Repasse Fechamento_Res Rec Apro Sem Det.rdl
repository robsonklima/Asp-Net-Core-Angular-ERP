  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT			TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia, vwc_Parque_Resumo.AnoMes,
			COUNT(vwc_Parque_Resumo.CodEquip) AS QtdEquiptos, SUM(vwc_Parque_Resumo.ValorReceita) AS ValorReceita
FROM			vwc_Parque_Resumo 
LEFT OUTER JOIN	                TipoEquipamento ON vwc_Parque_Resumo.CodTipoEquip = TipoEquipamento.CodTipoEquip
LEFT OUTER JOIN            	Cliente ON vwc_Parque_Resumo.CodCliente = Cliente.CodCliente
WHERE			(1=1)
AND			(vwc_Parque_Resumo.IndAtivo IN (@IndAtivo))
AND			(vwc_Parque_Resumo.IndReceita IN (@IndReceita))
AND			(vwc_Parque_Resumo.IndGarantia IN (@IndGarantia))
AND			(vwc_Parque_Resumo.CodTipoEquip IN (@CodTipoEquip))
AND			(vwc_Parque_Resumo.CodCliente IN (@CodCliente))
AND			(vwc_Parque_Resumo.AnoMes IN (@AnoMes))
GROUP BY	               TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia, vwc_Parque_Resumo.AnoMes, vwc_Parque_Resumo.ValorReceita
ORDER BY	               TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndReceita">
            <Value>=Parameters!IndReceita.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndGarantia">
            <Value>=Parameters!IndGarantia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodTipoEquip">
            <Value>=Parameters!CodTipoEquip.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodCliente">
            <Value>=Parameters!CodCliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
