  <rd:ReportID>50f4ba06-2bcf-4dbe-aa48-388c73ffe60c</rd:ReportID>
  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT				AU.NomeFantasia, EQ.NomeEquip, VPR.ValorDespesa AS RepasseUnit, VPR.AnoMes, 
				COUNT(VPR.CodEquip) AS QtdEquiptos, SUM(VPR.ValorDespesa) AS ValorRepasse
FROM				vwc_Parque_Resumo VPR
INNER JOIN			Cliente CL ON VPR.CodCliente = CL.CodCliente
INNER JOIN			Contrato CO ON VPR.CodContrato = CO.CodContrato
INNER JOIN			GrupoEquipamento GE ON VPR.CodGrupoEquip = GE.CodGrupoEquip
INNER JOIN			TipoEquipamento TE ON VPR.CodTipoEquip = TE.CodTipoEquip
INNER JOIN			Equipamento EQ ON VPR.CodEquip = EQ.CodEquip
				AND TE.CodTipoEquip = EQ.CodTipoEquip
INNER JOIN			Autorizada AU ON VPR.CodAutorizada = AU.CodAutorizada
WHERE				(1=1)
AND				(VPR.IndAtivo IN (@IndAtivo))
AND				(VPR.IndRepasse IN (@IndRepasse))
AND				(VPR.AnoMes IN (@AnoMes))
AND				(AU.CodAutorizada IN (@CodAutorizada))
GROUP BY			AU.NomeFantasia, EQ.NomeEquip, VPR.ValorDespesa, VPR.AnoMes
ORDER BY			AU.NomeFantasia, EQ.NomeEquip, VPR.ValorDespesa, VPR.AnoMes</CommandText>
        <QueryParameters>
          <QueryParameter Name="@IndAtivo">
            <Value>=Parameters!IndAtivo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndRepasse">
            <Value>=Parameters!IndRepasse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnoMes">
            <Value>=Parameters!AnoMes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodAutorizada">
            <Value>=Parameters!CodAutorizada.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="NomeFantasia">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeFantasia</DataField>
        </Field>
        <Field Name="NomeEquip">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeEquip</DataField>
        </Field>
        <Field Name="RepasseUnit">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>RepasseUnit</DataField>
        </Field>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
        <Field Name="QtdEquiptos">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>QtdEquiptos</DataField>
        </Field>
        <Field Name="ValorRepasse">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ValorRepasse</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAnoMes">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT DISTINCT AnoMes
FROM	             vwc_Parque_Resumo
ORDER BY            AnoMes</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAutorizada">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT       CodAutorizada, NomeFantasia
FROM         Autorizada
ORDER BY  NomeFantasia</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CodAutorizada">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CodAutorizada</DataField>
        </Field>
        <Field Name="NomeFantasia">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>NomeFantasia</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAnoMesCorrente">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT DISTINCT TOP 13 AnoMes FROM Parque_Resumo ORDER BY AnoMes DESC</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AnoMes">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AnoMes</DataField>
        </Field>
      </Fields>
    </DataSet>
