  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>DECLARE             @TabelaDados AS TABLE(Data varchar(10), HorarioInicio INT,	HorarioFim	INT, CodTecnico INT)
DECLARE             @HoraInicial INT
DECLARE             @HoraFinal INT

SET					@HoraInicial = 7
SET					@HoraFinal = 19

WHILE				(@HoraInicial &lt;= @HoraFinal)
BEGIN

	INSERT INTO			@TabelaDados
	SELECT				Data, @HoraInicial AS HorarioInicio, HorarioFim,CodTecnico
	FROM				(						
							SELECT				FORMAT(RAT.DataHoraInicio,'dd/MM/yyyy', 'pt-BR') AS Data,										
												DBO.INSERTZERO(DATEPART(hh,RAT.DataHoraInicio),2) AS HorarioInicio,
												DBO.INSERTZERO(DATEPART(hh,RAT.DataHoraSolucao),2) AS HorarioFim,
												RAT.CodTecnico
							FROM				RAT
							INNER JOIN			Tecnico ON RAT.CodTecnico = Tecnico.CodTecnico
							INNER JOIN			Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
							WHERE				CodPerfil IN (35,84) 
							AND					Usuario.CodTecnico IS NOT NULL 
							AND					Usuario.CodTecnico NOT IN (1763,1382,646)
							AND					Usuario.IndAtivo = 1
							AND					RAT.DataHoraInicio &gt;= DateAdd(mm, DateDiff(mm,0,GetDate()), 0)
							AND					RAT.DataHoraInicio &lt;= GETDATE()
							AND					DATEPART(DW,RAT.DataHoraInicio) NOT IN (1,7)
							AND					DATEPART(HH,RAT.DataHoraInicio) &gt;= 7
							AND					DATEPART(HH,RAT.DataHoraSolucao) &lt;= 19
							AND					@HoraInicial BETWEEN DATEPART(HH,RAT.DataHoraInicio) and DATEPART(HH,RAT.DataHoraSolucao)
							--AND					CAST(DATEPART(HH,RAT.DataHoraSolucao) AS INT) &gt;= @HoraInicial
						) AS DADOS
	ORDER BY			Data, HorarioInicio, HorarioFim

	SET @HoraInicial = @HoraInicial + 1

END

SELECT				Data, HorarioInicio AS Horario, HorarioInicio AS HorarioGrafico, 
					(
						SELECT				COUNT(DISTINCT Tecnico.CodTecnico)
						FROM				Tecnico
						INNER JOIN			Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico
						WHERE				CodPerfil IN (35,84) 
						AND					Usuario.CodTecnico IS NOT NULL 
						AND					Usuario.CodTecnico NOT IN (1763,1382,646)
						AND					Usuario.IndAtivo = 1
						AND					Tecnico.indFerias &lt;&gt; 1
					) - COUNT(DISTINCT CodTecnico) AS QTD
FROM				@TabelaDados
GROUP BY			Data, HorarioInicio
ORDER BY			Data, HorarioInicio

</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
