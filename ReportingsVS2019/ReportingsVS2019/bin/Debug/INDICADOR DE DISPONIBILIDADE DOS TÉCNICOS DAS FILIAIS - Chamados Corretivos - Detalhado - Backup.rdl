      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE @Tecnicos AS TABLE ( 
                     NomeUsuario VARCHAR(MAX), 
                     CodUsuario VARCHAR(25),  
	                 CodTecnico INT,  
	                 CodFilial INT,  
	                 IndFerias TINYINT,  
	                 IndUsuarioAtivo TINYINT,  
	                 IndTecnicoAtivo TINYINT 
                 )  
                 INSERT      INTO @Tecnicos   
                 SELECT      Usuario.NomeUsuario, 
                             Usuario.CodUsuario,  
                             Tecnico.CodTecnico, 
                             Usuario.CodFilialPonto AS CodFilial, 
                             Tecnico.IndFerias, 
                             Usuario.IndAtivo, 
                             Tecnico.IndAtivo 
                 FROM	    Tecnico 
                 INNER JOIN  vwc_usuario_ativo AS Usuario ON Tecnico.CodTecnico = Usuario.CodTecnico 
                
				

				 SELECT	OS.Codos, RAT.CodRAT, NumRAT, Tecnico.Nome, Cliente.NomeFantasia, RAT.DataHoraInicio, RAT.DataHoraSolucao
                                 FROM	RAT 
								 INNER JOIN OS on OS.Codos = RAT.CodOS
								 INNER JOIN Cliente on Cliente.CodCliente = OS.CodCliente
								 INNER JOIN Tecnico on Rat.CodTecnico = Tecnico.CodTecnico
								 INNER JOIN LocalAtendimento ON OS.CodPosto = LocalAtendimento.CodPosto 
								 INNER JOIN Filial ON Filial.CodFilial = OS.CodFilial 
								 

                                 WHERE	RAT.CodTecnico IN (SELECT T.CodTecnico from tecnico tec INNER JOIN @Tecnicos AS T ON T.CodTecnico = tec.CodTecnico) 
								 
                                 AND		(RAT.CodStatusServico &lt; 2 OR RAT.CodStatusServico &gt; 2) 
                                 AND		RAT.DataHoraSolucao BETWEEN FORMAT(GETDATE(), 'yyyy-MM-01 00:00:00') AND GETDATE() 
								 AND		OS.CodFilial = 4
								 AND		OS.CodTipoIntervencao = 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
