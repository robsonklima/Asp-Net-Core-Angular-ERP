  <rd:ReportID>766f0406-e83b-4298-8a1f-392aad043667</rd:ReportID>
  <LeftMargin>2.5cm</LeftMargin>
  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT				Cliente.NomeFantasia,
					Contrato.NomeContrato,
					Filial.NomeFilial, 
					Tecnico.Nome,
					Peca.CodMagnus,
					Peca.NomePeca,
					Peca.ValCusto,
					RATDetalhesPecas.QtdePecas AS QtdPeca,
					(SELECT		COUNT (*)
					FROM		EquipamentoContrato AS EC
					WHERE		EC.CodContrato = EquipamentoContrato.CodContrato		
					AND			EC.CodFilial = EquipamentoContrato.CodFilial				
					AND			EC.CodEquip = OS.CodEquip
					) AS TotalEquipFilial,
					(SELECT		COUNT (*)
					FROM		EquipamentoContrato AS EC
					WHERE		EC.CodContrato = EquipamentoContrato.CodContrato				
					AND			EC.CodEquip = OS.CodEquip
					) AS TotalEquip,
					Acao.NomeAcao
FROM				RAT
INNER JOIN			RATDetalhes ON RAT.CodRAT = RATDetalhes.CodRAT 
LEFT OUTER JOIN		RATDetalhesPecas ON RATDetalhes.CodRATDetalhe = RATDetalhesPecas.CodRATDetalhe
LEFT OUTER JOIN		Peca ON RATDetalhesPecas.CodPeca = Peca.CodPeca
INNER JOIN			OS ON RAT.CodOS = OS.CodOS
INNER JOIN			Filial ON OS.CodFilial = Filial.CodFilial
INNER JOIN			Tecnico ON RAT.CodTecnico = Tecnico.CodTecnico
LEFT OUTER JOIN		EquipamentoContrato ON OS.CodEquipContrato = EquipamentoContrato.CodEquipContrato
LEFT OUTER JOIN		Contrato ON EquipamentoContrato.CodContrato = Contrato.CodContrato
					AND OS.CodCliente = Contrato.CodCliente
INNER JOIN			Cliente ON OS.CodCliente = Cliente.CodCliente
INNER JOIN			Acao ON RATDetalhes.CodAcao = Acao.CodAcao
WHERE				(RATDetalhes.CodAcao IN (19,26))
AND					(Cliente.IndAtivo = 1)
AND					(OS.DataHoraAberturaOS BETWEEN (getdate()-365) AND getdate())</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
