      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE		@TabelaDados AS TABLE(Percentual FLOAT, Mes INT, Cliente INT)
DECLARE		@QtdMes INT
DECLARE		@AnoMes VARCHAR(6)

SET @QtdMes = 12


WHILE(@QtdMes &gt; 0)
BEGIN

SET @AnoMes = (SELECT FORMAT(dateadd(MONTH, -@QtdMes, getdate()), 'yyyyMM'))
EXEC pr_Reporting_Historico_SLA_Mensal_Cliente @AnoMes, @Cliente
INSERT INTO @TabelaDados
SELECT *
FROM
	(SELECT cast((cast(sum(Historico_SLA_Mensal_Cliente.ChamadosMesStatusSLA) AS FLOAT) / (CASE
				WHEN (cast((sum(Historico_SLA_Mensal_Cliente.ChamadosMes)) AS FLOAT)) &lt; 1 THEN
					1
				ELSE
					(cast((sum(Historico_SLA_Mensal_Cliente.ChamadosMes)) AS FLOAT))
			END) * 100) AS DECIMAL(10, 2)) AS Percentual
		  , FORMAT(dateadd(MONTH, -@QtdMes, getdate()), 'yyyyMM') AS Mes
		  , @Cliente AS Cliente
	 FROM
		 Historico_SLA_Mensal_Cliente
		 INNER JOIN Filial
			 ON Historico_SLA_Mensal_Cliente.CodFilial = Filial.CodFilial
	 WHERE
		 Historico_SLA_Mensal_Cliente.AnoMes = FORMAT(dateadd(MONTH, -@QtdMes, getdate()), 'yyyyMM')
		 AND Historico_SLA_Mensal_Cliente.CodCliente IN (@Cliente)

	 UNION
	 SELECT cast((cast(sum(Historico_SLA_Mensal_Cliente.ChamadosMesStatusSLA) AS FLOAT) / (CASE
				WHEN (cast((sum(Historico_SLA_Mensal_Cliente.ChamadosMes)) AS FLOAT)) &lt; 1 THEN
					1
				ELSE
					(cast((sum(Historico_SLA_Mensal_Cliente.ChamadosMes)) AS FLOAT))
			END) * 100) AS DECIMAL(10, 2)) AS Percentual
		  , FORMAT(dateadd(MONTH, -@QtdMes, getdate()), 'yyyyMM') AS Mes
		  , @Cliente AS Cliente
	 FROM
		 Historico_SLA_Mensal_Cliente
	 WHERE
		 Historico_SLA_Mensal_Cliente.AnoMes = FORMAT(dateadd(MONTH, -@QtdMes, getdate()), 'yyyyMM')
		 AND Historico_SLA_Mensal_Cliente.CodCliente IN (@Cliente)) AS Dados
WHERE
	Percentual &gt; 0
ORDER BY
	Percentual DESC

SET @QtdMes = @QtdMes - 1
END

SELECT *
FROM
	@TabelaDados
Where Cliente = @Cliente</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Cliente">
            <Value>=Parameters!Cliente.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
