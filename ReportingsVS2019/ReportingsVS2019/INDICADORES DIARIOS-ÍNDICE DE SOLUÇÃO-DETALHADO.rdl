  <DataSets>
    <DataSet Name="SAT">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT			TipoEquipamento.NomeTipoEquip AS TipoEquipamento,
				Cliente.NomeFantasia AS Cliente,
				Filial.NomeFilial AS Filial,
				Autorizada.NomeFantasia AS PAT,
				Regiao.NomeRegiao AS Regiao,
				UF.SiglaUF AS UF,
                SUM (vwc_Reporting_Indice_de_Solucao.ChamadosMes) AS ChamadosMes,
                SUM (vwc_Reporting_Indice_de_Solucao.ChamadosMesStatusSLA) AS ChamadosMesStatusSLA,
                SUM (vwc_Reporting_Indice_de_Solucao.ChamadosAnoStatusSLA) AS ChamadosAnoStatusSLA
FROM			vwc_Reporting_Indice_de_Solucao
INNER JOIN		TipoEquipamento ON vwc_Reporting_Indice_de_Solucao.CodTipoEquip = TipoEquipamento.CodTipoEquip
INNER JOIN		Cliente ON vwc_Reporting_Indice_de_Solucao.CodCliente = Cliente.CodCliente
INNER JOIN		Filial ON vwc_Reporting_Indice_de_Solucao.CodFilial = Filial.CodFilial
INNER JOIN		Autorizada ON vwc_Reporting_Indice_de_Solucao.CodAutorizada = Autorizada.CodAutorizada
INNER JOIN		Regiao ON vwc_Reporting_Indice_de_Solucao.CodRegiao = Regiao.CodRegiao
INNER JOIN		UF ON vwc_Reporting_Indice_de_Solucao.CodUF = UF.CodUF
GROUP BY		TipoEquipamento.NomeTipoEquip, Cliente.NomeFantasia, Filial.NomeFilial, Autorizada.NomeFantasia,
				Regiao.NomeRegiao,	UF.SiglaUF</CommandText>
        <DataSourceName>SAT</DataSourceName>
      </Query>
