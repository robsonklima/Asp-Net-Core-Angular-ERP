  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>DECLARE			@AnoMes VARCHAR(6)
SET				@AnoMes = CAST((CAST(YEAR(GETDATE()) AS VARCHAR) + CAST(DBO.insertZero(MONTH(GETDATE()),2) AS VARCHAR)) AS VARCHAR)

SELECT			Regiao, Criticidade, 
				Parque,
				DiasMes AS 'Qtd Dias/Mês Considerados',
				HorasDisponiveis AS 'Total de Horas Disponíveis (Parque X Janela)', 
				PercentualContrato AS '% permitido de Indisponibilidade',
				CAST((
					HorasDisponiveis * (PercentualContrato/100)
				) AS DECIMAL(10,2)) 'Total de Horas Disponíveis no Mês (Considerando o Percentual de Disponibilidade)',
				HorasIndisponiveis AS 'Horas Já Usadas até XXXXXX' , 
				DiasConsumo AS 'Qtd de Dias contados de acordo com contrato até  XXXXXX',
				CAST((				
					HorasDisponiveis * (PercentualContrato/100)					
					/
					DiasMes
					*
					(
						DiasMes - DiasConsumo
					)
				) AS DECIMAL(10,2)) AS 'Qtd de Horas Utilizados Até XXXXXX',
				0 AS 'Qtd de Dias Disponível de acordo com contrato Até XXXXXX',
				0 AS 'Qtd de Horas disponíveis Até XXXXXX',
				CAST((
					HorasIndisponiveis 
					/
					(HorasDisponiveis * (PercentualContrato/100))
					* 
					100
				) AS DECIMAL(10,2)) AS '% Utilizado',
				FORMAT(getdate()-1,'dd/MM/yy', 'pt-BR') AS DtRef
				--CAST((
				--	CAST(
				--	(
				--		HorasIndisponiveis
				--		/
				--		HorasDisponiveis
				--	) AS FLOAT)
				--	* 100.00
				--) AS DECIMAL(10,2)) AS PercentualUtilizado,
				--(
				--	PercentualContrato - 
				--	CAST((
				--		CAST(
				--		(
				--			HorasIndisponiveis
				--			/
				--			HorasDisponiveis
				--		) AS FLOAT)
				--		* 100.00
				--	) AS DECIMAL(10,2))
				--) AS PercentualDisponível,
				--CASE WHEN (DAY(GETDATE()) - DiasMes) &lt; 0 
				--		THEN 0
				--		ELSE DAY(GETDATE()) - DiasMes
				--END AS 'DiasAConsumir'
FROM			(
					SELECT			DispBBCalcEquipamentoContrato.Regiao, 
									DispBBCalcEquipamentoContrato.Criticidade, 
									CAST((SUM(DispBBCalcEquipamentoContrato.MinTotais)/60) AS FLOAT) AS HorasDisponiveis,
									CAST((CAST(ISNULL((
										SELECT			SUM(DispBBCalcOS.IndispMin) 
										FROM			DispBBCalcOS 
										WHERE			DispBBCalcOS.AnoMes = DispBBCalcEquipamentoContrato.AnoMes
										AND				DispBBCalcOS.Regiao = DispBBCalcEquipamentoContrato.Regiao
										AND				DispBBCalcOS.CodDispBBCriticidade = DispBBCalcEquipamentoContrato.Criticidade
									),0.00) AS FLOAT)
									/60.00) AS DECIMAL(10,2)) AS HorasIndisponiveis,
									MAX(CAST(DispBBPercRegiao.Percentual AS DECIMAL(10,2))) AS PercentualContrato,
									MAX(CAST((
										CAST(DispBBCalcEquipamentoContrato.MinTotais/60 AS FLOAT)
										/
										CAST(CAST(DATEDIFF(MINUTE, DispBBCriticidade.HorarioInicio, DispBBCriticidade.HorarioFim) AS FLOAT)/60.00 AS FLOAT)
									)AS DECIMAL(10,0))) AS DiasMes,
									CASE WHEN DispBBCalcEquipamentoContrato.Criticidade IN (11,12)
												THEN 
													DateDiff(dd, (CAST(substring(@AnoMes, 1,4) + '-' + substring(@AnoMes, 5,2) + '-01 00:00:00.000' AS datetime)), 
																(CAST(substring(@AnoMes, 1,4) + '-' + substring(@AnoMes, 5,2) + '-' + DBO.insertZero(DAY(GETDATE()),2) + ' 00:00:00.000' AS datetime))
															)
												ELSE	
													DateDiff(dd, (CAST(substring(@AnoMes, 1,4) + '-' + substring(@AnoMes, 5,2) + '-01 00:00:00.000' AS datetime)), (CAST(substring(@AnoMes, 1,4) + '-' + substring(@AnoMes, 5,2) + '-' + DBO.insertZero(DAY(GETDATE()),2) + ' 00:00:00.000' AS datetime)))
													- 
													dbo.fc_CalculaDiasNaoUteis((CAST(substring(@AnoMes, 1,4) + '-' + substring(@AnoMes, 5,2) + '-01 00:00:00.000' AS datetime)), getdate(), 0, 0, 0, 1, 0, 0) / 24													
									END	AS DiasConsumo,
									COUNT(DISTINCT CodEquipContrato) AS Parque
					FROM			DispBBCalcEquipamentoContrato
					INNER JOIN		DispBBRegiao ON DispBBCalcEquipamentoContrato.Regiao = DispBBRegiao.Nome 
					INNER JOIN		DispBBPercRegiao ON DispBBRegiao.CodDispBBRegiao = DispBBPercRegiao.CodDispBBRegiao
									AND DispBBCalcEquipamentoContrato.Criticidade = DispBBPercRegiao.Criticidade
					INNER JOIN		DispBBCriticidade ON DispBBCalcEquipamentoContrato.Criticidade = DispBBCriticidade.CodDispBBCriticidade
					WHERE			DispBBCalcEquipamentoContrato.AnoMes = @AnoMes
					GROUP BY		DispBBCalcEquipamentoContrato.AnoMes,DispBBCalcEquipamentoContrato.Regiao, DispBBCalcEquipamentoContrato.Criticidade
				) AS Dados</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
