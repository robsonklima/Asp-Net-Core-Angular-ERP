    <DataSet Name="Mes">
      <Query>
        <DataSourceName>SAT</DataSourceName>
        <CommandText>WITH Meses AS
(
  SELECT 1 AS IdMes
  UNION ALL
  SELECT IdMes + 1
  FROM Meses
  WHERE IdMes &lt; 12
)
SELECT dbo.insertZero(IdMes, 2) IdMes FROM Meses
</CommandText>
